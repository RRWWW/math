\begin_layout Definition
\begin_inset Argument 1
status open

\begin_layout Plain Layout
group
\end_layout

\end_inset

 群
\end_layout

\begin_layout Definition
\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group}\\
\Updownarrow\\
G=\left(G,\cdot\right)=\left(G,\cdot_{{\scriptscriptstyle G}}\right)= & \left\{ g\middle|\begin{aligned}g_{{\scriptscriptstyle 1}}\cdot g_{{\scriptscriptstyle 2}}= & g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\in G & \forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in G & \quad\left(c\right)\cdot_{{\scriptscriptstyle G}}\text{closure}\\
g_{{\scriptscriptstyle 1}}\left(g_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 3}}\right)= & \left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)g_{{\scriptscriptstyle 3}}=g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 3}} & \forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}},g_{{\scriptscriptstyle 3}}\in G & \quad\left(a\right)\cdot_{{\scriptscriptstyle G}}\text{associativity}\\
e\cdot g=eg= & g=ge=g\cdot e & \exists e=e_{{\scriptscriptstyle G}}\in G,\forall g\in G & \quad\left(id\right)\text{identity element}\\
\overline{g}\cdot g=\bar{g}g= & e=g\bar{g}=g\cdot\overline{g} & \forall g\in G,\exists\overline{g}\in G & \quad\left(in\right)\text{inverse element}
\end{aligned}
\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
\begin_inset Argument 1
status open

\begin_layout Plain Layout
homomorphism
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Argument 1
status open

\begin_layout Plain Layout
kernel of homomorphism
\end_layout

\end_inset


\end_layout

\begin_layout Definition
\begin_inset Argument 1
status open

\begin_layout Plain Layout
exponential family
\end_layout

\end_inset

A family of PDF/PMF is called exponential family if
\end_layout

\begin_layout Definition
\begin_inset Formula 
\[
f\left(x|\boldsymbol{\theta}\right)=h\left(x\right)c\left(\boldsymbol{\theta}\right)\mathrm{e}^{{\scriptscriptstyle \sum\limits _{j=1}^{k}w_{{\scriptscriptstyle j}}\left(\boldsymbol{\theta}\right)t_{{\scriptscriptstyle j}}\left(x\right)}}=h\left(x\right)c\left(\boldsymbol{\theta}\right)\exp\left(\sum\limits _{j=1}^{k}w_{{\scriptscriptstyle j}}\left(\boldsymbol{\theta}\right)t_{{\scriptscriptstyle j}}\left(x\right)\right)
\]

\end_inset


\end_layout

\begin_layout Definition
with 
\begin_inset Formula $\boldsymbol{\theta}=\boldsymbol{\theta}\left(\theta_{{\scriptscriptstyle 1}},\cdots,\theta_{{\scriptscriptstyle k}}\right)=\left(\theta_{{\scriptscriptstyle 1}},\cdots,\theta_{{\scriptscriptstyle k}}\right)$
\end_inset

 for some 
\begin_inset Formula $h\left(x\right),c\left(\boldsymbol{\theta}\right),w_{{\scriptscriptstyle j}}\left(\boldsymbol{\theta}\right),t_{{\scriptscriptstyle j}}\left(x\right)$
\end_inset

, where
\begin_inset Formula 
\[
h\left(x\right)c\left(\boldsymbol{\theta}\right)\ge0\Rightarrow f\left(x|\theta\right)\ge0
\]

\end_inset


\end_layout

\begin_layout Definition
and parameters 
\begin_inset Formula $\boldsymbol{\theta}$
\end_inset

 and statistic or real number 
\begin_inset Formula $x$
\end_inset

 can be separated.
\begin_inset Formula 
\[
\mathcal{E}^{f}=\left\{ f\middle|f=f\left(x|\boldsymbol{\theta}\right)=h\left(x\right)c\left(\boldsymbol{\theta}\right)\mathrm{e}^{{\scriptscriptstyle \sum\limits _{j=1}^{k}w_{{\scriptscriptstyle j}}\left(\boldsymbol{\theta}\right)t_{{\scriptscriptstyle j}}\left(x\right)}}=h\left(x\right)c\left(\boldsymbol{\theta}\right)\exp\left(\sum\limits _{j=1}^{k}w_{{\scriptscriptstyle j}}\left(\boldsymbol{\theta}\right)t_{{\scriptscriptstyle j}}\left(x\right)\right)\right\} 
\]

\end_inset


\end_layout

