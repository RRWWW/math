\begin_layout Standard
\begin_inset Formula $D_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}$
\end_inset


\begin_inset Formula 
\[
\recolor{\begin{aligned}D_{{\scriptscriptstyle 3}}= & \left[e\right]\cup\left[\rho_{{\scriptscriptstyle 1}}\right]\cup\left[\pi_{{\scriptscriptstyle 0}}\right]=\left\{ \rho_{{\scriptscriptstyle 0}}\right\} \cup\left\{ \rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}}\right\} \cup\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\} \\
= & \left[e\right]\cup\left[\rho_{{\scriptscriptstyle 2}}\right]\cup\left[\pi_{{\scriptscriptstyle 1}}\right]=\left\{ \rho_{{\scriptscriptstyle 0}}\right\} \cup\left\{ \rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}}\right\} \cup\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\} \\
= & \left[\rho_{{\scriptscriptstyle 0}}\right]\cup\left[\rho_{{\scriptscriptstyle 2}}\right]\cup\left[\pi_{{\scriptscriptstyle 2}}\right]=\left\{ \rho_{{\scriptscriptstyle 0}}\right\} \cup\left\{ \rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}}\right\} \cup\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\} \\
=S_{{\scriptscriptstyle 3}}= & \left[\left[123\right]\right]\cup\left[\left[231\right]\right]\cup\left[\left[213\right]\right]=\left\{ \left[123\right]\right\} \cup\left\{ \left[231\right],\left[312\right]\right\} \cup\left\{ \left[213\right],\left[132\right],\left[321\right]\right\} \\
= & \left[\left[123\right]\right]\cup\left[\left[312\right]\right]\cup\left[\left[132\right]\right]=\left\{ \left[123\right]\right\} \cup\left\{ \left[231\right],\left[312\right]\right\} \cup\left\{ \left[213\right],\left[132\right],\left[321\right]\right\} \\
= & \left[\left[123\right]\right]\cup\left[\left[312\right]\right]\cup\left[\left[321\right]\right]=\left\{ \left[123\right]\right\} \cup\left\{ \left[231\right],\left[312\right]\right\} \cup\left\{ \left[213\right],\left[132\right],\left[321\right]\right\} \\
= & \left[\left(\right)\right]\cup\left[\left(123\right)\right]\cup\left[\left(12\right)\right]=\left\{ \left(\right)\right\} \cup\left\{ \left(123\right),\left(132\right)\right\} \cup\left\{ \left(12\right),\left(23\right),\left(31\right)\right\} \\
= & \left[\left(\right)\right]\cup\left[\left(132\right)\right]\cup\left[\left(23\right)\right]=\left\{ \left(\right)\right\} \cup\left\{ \left(123\right),\left(132\right)\right\} \cup\left\{ \left(12\right),\left(23\right),\left(31\right)\right\} \\
= & \left[\left(\right)\right]\cup\left[\left(132\right)\right]\cup\left[\left(31\right)\right]=\left\{ \left(\right)\right\} \cup\left\{ \left(123\right),\left(132\right)\right\} \cup\left\{ \left(12\right),\left(23\right),\left(31\right)\right\} 
\end{aligned}
}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 3}}\trianglelefteq S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}$
\end_inset


\begin_inset Formula 
\[
\recolor{\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left[e\right]\cup\left[\rho_{{\scriptscriptstyle 1}}\right] & =\left\{ \rho_{{\scriptscriptstyle 0}}\right\} \cup\left\{ \rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}}\right\} \\
= & \left[\left[123\right]\right]\cup\left[\left[231\right]\right] & =\left\{ \left[123\right]\right\} \cup\left\{ \left[231\right],\left[312\right]\right\} \\
= & \left[\left(\right)\right]\cup\left[\left(123\right)\right] & =\left\{ \left(\right)\right\} \cup\left\{ \left(123\right),\left(132\right)\right\} 
\end{aligned}
}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $S_{{\scriptscriptstyle n}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned} & S_{{\scriptscriptstyle n}}=\left(S_{{\scriptscriptstyle n}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\right)=\left(S_{{\scriptscriptstyle n}},\circ\right)\\
= & \left\{ \sigma\middle|\begin{aligned}n\in & \mathbb{N}\\
N= & \left\{ \overset{n}{\overbrace{1,\cdots,n}}\right\} \\
\sigma\in & N^{N}\\
\sigma\left(N\right)= & N
\end{aligned}
\right\} =\left\{ \sigma\middle|\begin{aligned}n\in & \mathbb{N}\\
N= & \left\{ 1,\cdots,n\right\} \\
\sigma: & N\rightarrow N\\
\forall\sigma_{{\scriptscriptstyle i}},\sigma_{{\scriptscriptstyle j}}\in S_{{\scriptscriptstyle n}} & \left[\sigma_{{\scriptscriptstyle i}}\sigma_{{\scriptscriptstyle j}}=\sigma_{{\scriptscriptstyle i}}\circ\sigma_{{\scriptscriptstyle j}}\right]\\
\forall m_{{\scriptscriptstyle 1}},m_{{\scriptscriptstyle 2}}\in N & \Biggl[m_{{\scriptscriptstyle 1}}\ne m_{{\scriptscriptstyle 2}}\Leftrightarrow\sigma\left(m_{{\scriptscriptstyle 1}}\right)\ne\sigma\left(m_{{\scriptscriptstyle 2}}\right)\Biggr]
\end{aligned}
\right\} \\
= & \left\{ \sigma\middle|\sigma=\begin{pmatrix}1 & 2 & \cdots & n\\
\sigma\left(1\right) & \sigma\left(2\right) & \cdots & \sigma\left(n\right)
\end{pmatrix}=\left(\overset{n}{\overbrace{\begin{array}{ccc}
1 & \cdots & n\\
\sigma\left(1\right) & \cdots & \sigma\left(n\right)
\end{array}}}\right)=\begin{pmatrix}1 & \cdots & n\\
\sigma\left(1\right) & \cdots & \sigma\left(n\right)
\end{pmatrix}\right\}  & \text{two-line notation}\\
= & \left\{ \sigma\middle|\sigma=\sigma\left(1\right)\sigma\left(2\right)\cdots\sigma\left(n\right)=\overset{n}{\overbrace{\sigma\left(1\right)\sigma\left(2\right)\cdots\sigma\left(n\right)}}=\sigma\left(1\right)\cdots\sigma\left(n\right)=\left[\sigma\left(1\right)\cdots\sigma\left(n\right)\right]\right\}  & \text{one-line notation}\\
= & \left\{ \sigma\middle|\sigma=c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}=\overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}=c_{{\scriptscriptstyle 1}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}=\overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{c_{{\scriptscriptstyle 1}}}{\underbrace{c_{{\scriptscriptstyle 11}}c_{{\scriptscriptstyle 12}}\cdots c_{{\scriptscriptstyle 1n_{{\scriptscriptstyle 1}}}}}}\cdots\underset{c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}{\underbrace{c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}1}}c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}n_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}}}}}}\right\}  & \text{cycle notation}\\
= & \left\{ \sigma\middle|\sigma=s_{{\scriptscriptstyle 1}}s_{{\scriptscriptstyle 2}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}=\overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{s_{{\scriptscriptstyle 1}}s_{{\scriptscriptstyle 2}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}}=s_{{\scriptscriptstyle 1}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}=\overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}}}}\right\}  & \text{swap notation}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\sigma= & c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}=\overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}=c_{{\scriptscriptstyle 1}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}=\overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{c_{{\scriptscriptstyle 1}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}} & c_{{\scriptscriptstyle i}}\cap c_{{\scriptscriptstyle j}}=\emptyset\\
= & \underset{c_{{\scriptscriptstyle 1}}}{\underbrace{c_{{\scriptscriptstyle 11}}c_{{\scriptscriptstyle 12}}\cdots c_{{\scriptscriptstyle 1n_{{\scriptscriptstyle 1}}}}}}\underset{c_{{\scriptscriptstyle 2}}}{\underbrace{c_{{\scriptscriptstyle 21}}c_{{\scriptscriptstyle 22}}\cdots c_{{\scriptscriptstyle 2n_{{\scriptscriptstyle 2}}}}}}\cdots\underset{c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}{\underbrace{c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}1}}c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}n_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}}}} & c_{{\scriptscriptstyle ij_{{\scriptscriptstyle 1}}}}\cap c_{{\scriptscriptstyle ij_{{\scriptscriptstyle 2}}}}=\emptyset\\
= & \underset{c_{{\scriptscriptstyle 1}}}{\underbrace{c_{{\scriptscriptstyle 11}}c_{{\scriptscriptstyle 12}}\cdots c_{{\scriptscriptstyle 1n_{{\scriptscriptstyle 1}}}}}}\cdots\underset{c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}{\underbrace{c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}1}}c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}n_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}}}}\\
= & \overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{c_{{\scriptscriptstyle 1}}}{\underbrace{c_{{\scriptscriptstyle 11}}c_{{\scriptscriptstyle 12}}\cdots c_{{\scriptscriptstyle 1n_{{\scriptscriptstyle 1}}}}}}\cdots\underset{c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}{\underbrace{c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}1}}c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}n_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}}}}}}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sum\limits_{i=1}^{n_{{\scriptscriptstyle \sigma}}}n_{{\scriptscriptstyle i}}=n\qquad\forall n\in\mathbb{N},\forall\sigma\in S_{{\scriptscriptstyle n}}
\]

\end_inset


\end_layout

\begin_layout Standard
cycle type
\begin_inset Formula 
\[
n_{{\scriptscriptstyle i}}
\]

\end_inset


\begin_inset Formula 
\[
\left[n_{{\scriptscriptstyle i}}\right]=\left[n_{{\scriptscriptstyle 1}}n_{{\scriptscriptstyle 2}}\cdots n_{n_{{\scriptscriptstyle \sigma}}}\right]=\left[1^{k_{{\scriptscriptstyle 1}}}2^{k_{{\scriptscriptstyle 2}}}\cdots n^{k_{{\scriptscriptstyle n}}}\right]=\left[\ell^{k_{{\scriptscriptstyle \ell}}}\right]
\]

\end_inset


\begin_inset Formula 
\[
S_{{\scriptscriptstyle 3}}\ni\left(3\right)\left(12\right)\rightarrow\left[n_{{\scriptscriptstyle i}}\right]=\left[n_{{\scriptscriptstyle 1}}n_{{\scriptscriptstyle 2}}\cdots n_{n_{{\scriptscriptstyle \sigma}}}\right]\overset{n_{{\scriptscriptstyle \sigma}}=2}{=}\left[n_{{\scriptscriptstyle 1}}n_{{\scriptscriptstyle 2}}\right]\overset{n_{{\scriptscriptstyle 1}}=1,n_{{\scriptscriptstyle 2}}=2}{=}\left[1\cdot2\right]=\left[1^{1}2^{1}\right]=\left[1^{k_{{\scriptscriptstyle 1}}}2^{k_{{\scriptscriptstyle 2}}}\cdots n^{k_{{\scriptscriptstyle n}}}\right]=\left[\ell^{k_{{\scriptscriptstyle \ell}}}\right]
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}S_{{\scriptscriptstyle n}}\ni e= & \left(1\right)\left(2\right)\left(3\right)\cdots\left(n\right)\rightarrow\left[n_{{\scriptscriptstyle i}}\right]=\left[n_{{\scriptscriptstyle 1}}n_{{\scriptscriptstyle 2}}\cdots n_{n_{{\scriptscriptstyle \sigma}}}\right]\\
\overset{n_{{\scriptscriptstyle \sigma}}=n}{=} & \left[n_{{\scriptscriptstyle 1}}n_{{\scriptscriptstyle 2}}\cdots n_{n}\right]\overset{n_{{\scriptscriptstyle 1}}=1,n_{{\scriptscriptstyle 2}}=1,\cdots,n_{{\scriptscriptstyle n}}=1}{=}\left[1\cdot1\cdot\cdots\cdot1\right]=\left[1^{n}\right]=\left[1^{k_{{\scriptscriptstyle 1}}}2^{k_{{\scriptscriptstyle 2}}}\cdots n^{k_{{\scriptscriptstyle n}}}\right]=\left[\ell^{k_{{\scriptscriptstyle \ell}}}\right]
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
