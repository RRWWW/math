\begin_layout Standard
\begin_inset Formula $D_{{\scriptscriptstyle n}}$
\end_inset


\begin_inset Formula 
\[
D_{{\scriptscriptstyle n}}=\begin{cases}
\left[\rho_{{\scriptscriptstyle 0,n}}\right]\cup\left[\rho_{{\scriptscriptstyle 1,n}}\right]\cup\left[\pi_{{\scriptscriptstyle 2m,n}}\right]\cup\left[\pi_{{\scriptscriptstyle 2m-1,n}}\right] & m\in\mathbb{Z}_{{\scriptscriptstyle \left[0,\frac{n}{2}\right]}},n\in2\mathbb{N}\\
\left[\rho_{{\scriptscriptstyle 0,n}}\right]\cup\left[\rho_{{\scriptscriptstyle 1,n}}\right]\cup\left[\pi_{{\scriptscriptstyle m,n}}\right] & m\in\mathbb{Z}_{{\scriptscriptstyle \left[0,n-1\right]}},n\in2\mathbb{N}-1
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $N\trianglelefteq G$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}hN\overline{h}= & h\overline{h}N=eN=N\\
N= & hN\overline{h}=\left\{ hn\overline{h}\middle|\begin{aligned}n\in N\\
h\in G
\end{aligned}
\right\} =\bigcup_{n\in N}\left[n\right]\\
N= & \bigcup_{n\in N}\left[n\right]\\
\left[n\right]\subseteq & N & \forall n\in N
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{array}{ccccccccc}
G & = & \left(g_{{\scriptscriptstyle 0}}N=eN=N\right) & \cup & \left(g_{{\scriptscriptstyle 1}}N\right) & \cup & \left(g_{{\scriptscriptstyle 2}}N\right) & \cup & \cdots\\
 &  & \parallel\\
 &  & \left[n_{{\scriptscriptstyle 0}}\right]=\left[e\right]\\
 &  & \cup\\
 &  & \left[n_{{\scriptscriptstyle 1}}\right]\\
 &  & \cup\\
 &  & \left[n_{{\scriptscriptstyle 2}}\right]\\
 &  & \cup\\
 &  & \vdots
\end{array}
\]

\end_inset


\end_layout

\begin_layout Section
homomorphism
\end_layout

\begin_layout Itemize
homomorphism 同態 = 均態 = 均形
\end_layout

\begin_layout Itemize
endomorphism 內同態 = 內態 = 內形 = 內均形
\end_layout

\begin_layout Itemize
isomorphism 同構 = 同形
\end_layout

\begin_layout Itemize
automorphism 自同構 = 自構 = 同形 = 自同形
\end_layout

\begin_layout Definition
homomorphism
\begin_inset Formula 
\[
\begin{aligned}G\text{ is } & \text{a group} & \Rightarrow G=\left(G,\cdot_{{\scriptscriptstyle G}}\right)\\
g,\widetilde{g}\in & G\\
\acute{G}\text{ is } & \text{a group} & \Rightarrow\acute{G}=\left(\acute{G},\cdot_{{\scriptscriptstyle \acute{G}}}\right)\\
\acute{g},\widetilde{\acute{g}}\in & \acute{G}\\
\varphi: & G\rightarrow\acute{G} & \Leftrightarrow G\overset{\varphi}{\rightarrow}\acute{G}\Leftrightarrow\varphi\in\acute{G}^{G}\\
 &  & \exists!\acute{g}=\varphi\left(g\right),\exists!\widetilde{\acute{g}}=\varphi\left(\widetilde{g}\right)\\
\varphi\left(g\cdot_{{\scriptscriptstyle G}}\widetilde{g}\right)= & \varphi\left(g\right)\cdot_{{\scriptscriptstyle \acute{G}}}\varphi\left(\widetilde{g}\right) & =\acute{g}\cdot_{{\scriptscriptstyle \acute{G}}}\widetilde{\acute{g}}\\
\Downarrow\\
G\simeq & \acute{G} & \text{homomorphism vs. isomorphism }G\cong\acute{G}\\
\Updownarrow\\
G,\acute{G} & \text{ have homomorphism} & \text{homomorphism map }\varphi
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
preserving the correspnding identity element
\begin_inset Formula 
\[
\begin{aligned}\varphi\left(g\right)= & \varphi\left(e_{{\scriptscriptstyle G}}\cdot_{{\scriptscriptstyle G}}g\right)=\varphi\left(e\cdot_{{\scriptscriptstyle G}}g\right)=\varphi\left(e\right)\cdot_{{\scriptscriptstyle \acute{G}}}\varphi\left(g\right)\\
\varphi\left(g\right)= & \varphi\left(e\right)\cdot_{{\scriptscriptstyle \acute{G}}}\varphi\left(g\right)\\
\Downarrow\\
\varphi\left(e\right)= & \acute{e}=e_{{\scriptscriptstyle \acute{G}}}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
preserving the correspnding inverse element
\begin_inset Formula 
\[
\begin{aligned}\acute{e}=e_{{\scriptscriptstyle \acute{G}}}=\varphi\left(e\right)= & \varphi\left(e_{{\scriptscriptstyle G}}\right)=\varphi\left(\overline{g}\cdot_{{\scriptscriptstyle G}}g\right)=\varphi\left(\overline{g}\right)\cdot_{{\scriptscriptstyle \acute{G}}}\varphi\left(g\right)\\
e_{{\scriptscriptstyle \acute{G}}}= & \varphi\left(\overline{g}\right)\cdot_{{\scriptscriptstyle \acute{G}}}\varphi\left(g\right)\\
\Downarrow\\
\varphi\left(g^{{\scriptscriptstyle -1}}\right)=\varphi\left(\overline{g}\right)= & \overline{\varphi\left(g\right)}=\left[\varphi\left(g\right)\right]^{-1}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Definition
endomorphism
\begin_inset Formula 
\[
\begin{aligned}G\text{ is } & \text{a group} & \Rightarrow G=\left(G,\cdot_{{\scriptscriptstyle G}}\right)\\
g,\widetilde{g}\in & G\\
\varphi: & G\rightarrow G & \Leftrightarrow G\overset{\varphi}{\rightarrow}G\Leftrightarrow\varphi\in G^{G}\\
 &  & \exists!\acute{g}=\varphi\left(g\right),\exists!\widetilde{\acute{g}}=\varphi\left(\widetilde{g}\right)\\
\varphi\left(g\cdot_{{\scriptscriptstyle G}}\widetilde{g}\right)= & \varphi\left(g\right)\cdot_{{\scriptscriptstyle \acute{G}}}\varphi\left(\widetilde{g}\right) & =\acute{g}\cdot_{{\scriptscriptstyle \acute{G}}}\widetilde{\acute{g}}\\
\Downarrow\\
G\simeq & G & \text{endomorphism vs. isomorphism }G\cong\acute{G}\\
\Updownarrow\\
G & \text{ has endomorphism} & \text{endomorphism map }\varphi
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Example
\begin_inset Formula $\mathcal{V}\simeq\acute{\mathcal{V}}\because\exists L:\mathcal{V}\rightarrow\acute{\mathcal{V}}$
\end_inset

 linear map over two vector spaces( = additive Abelian groups + scalar multiplication)
\begin_inset Formula 
\[
\begin{aligned}\mathcal{V}\text{ is } & \text{an additive Abelian group} & \Leftarrow\mathcal{V}=\left(\mathcal{V},+_{{\scriptscriptstyle \mathcal{V}}}\right)=\left(\mathcal{V},+_{{\scriptscriptstyle \mathcal{V}}},\cdot_{{\scriptscriptstyle \mathbb{F}\times\mathcal{V},\mathbb{F}}}\right)\\
\boldsymbol{v},\widetilde{\boldsymbol{v}}\in & \mathcal{V}\\
\acute{\mathcal{V}}\text{ is } & \text{an additive Abelian group} & \Leftarrow\acute{\mathcal{V}}=\left(\acute{\mathcal{V}},+_{{\scriptscriptstyle \acute{\mathcal{V}}}}\right)=\left(\acute{\mathcal{V}},+_{{\scriptscriptstyle \acute{\mathcal{V}}}},\cdot_{{\scriptscriptstyle \mathbb{F}\times\mathcal{\acute{\mathcal{V}}},\mathbb{F}}}\right)\\
\acute{\boldsymbol{v}},\widetilde{\acute{\boldsymbol{v}}}\in & \acute{\mathcal{V}}\\
L: & \mathcal{V}\rightarrow\acute{\mathcal{V}} & \Leftrightarrow\mathcal{V}\overset{L}{\rightarrow}\acute{\mathcal{V}}\Leftrightarrow\varphi\in\acute{\mathcal{V}}^{\mathcal{V}}\\
 &  & \exists!\acute{\boldsymbol{v}}=L\left(\boldsymbol{v}\right),\exists!\widetilde{\acute{\boldsymbol{v}}}=L\left(\widetilde{\boldsymbol{v}}\right)\\
 &  & \text{linear map }L:L\left(\lambda\boldsymbol{v}+_{{\scriptscriptstyle \mathcal{V}}}\widetilde{\lambda}\widetilde{\boldsymbol{v}}\right)=\lambda L\left(\boldsymbol{v}\right)+\widetilde{\lambda}_{{\scriptscriptstyle \acute{\mathcal{V}}}}L\left(\widetilde{\boldsymbol{v}}\right)\\
 &  & \Downarrow\\
 &  & L\left(\boldsymbol{v}+_{{\scriptscriptstyle \mathcal{V}}}\widetilde{\boldsymbol{v}}\right)=L\left(\boldsymbol{v}\right)+_{{\scriptscriptstyle \acute{\mathcal{V}}}}L\left(\widetilde{\boldsymbol{v}}\right)\\
L\left(\boldsymbol{v}+_{{\scriptscriptstyle \mathcal{V}}}\widetilde{\boldsymbol{v}}\right)= & L\left(\boldsymbol{v}\right)+_{{\scriptscriptstyle \acute{\mathcal{V}}}}L\left(\widetilde{\boldsymbol{v}}\right) & =\acute{\boldsymbol{v}}\cdot_{{\scriptscriptstyle \acute{\mathcal{V}}}}\widetilde{\acute{\boldsymbol{v}}}\\
\Downarrow\\
\mathcal{V}\simeq & \acute{\mathcal{V}} & \text{homomorphism vs. isomorphism }\mathcal{V}\cong\acute{\mathcal{V}}\\
\Updownarrow\\
\mathcal{V},\acute{\mathcal{V}} & \text{ have homomorphism} & \text{homomorphism map }L
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Example
preserving the correspnding identity element
\begin_inset Formula 
\[
L\left(\boldsymbol{0}_{{\scriptscriptstyle \mathcal{V}}}\right)=L\left(\boldsymbol{0}\right)=\acute{\boldsymbol{0}}=\boldsymbol{0}_{{\scriptscriptstyle \acute{\mathcal{V}}}}
\]

\end_inset


\end_layout

\begin_layout Example
preserving the correspnding inverse element
\begin_inset Formula 
\[
\begin{array}{c}
L\left(\boldsymbol{v}\right)=\acute{\boldsymbol{v}}\\
\Downarrow\\
L\left(\overline{\boldsymbol{v}}\right)=L\left(-\boldsymbol{v}\right)=-\acute{\boldsymbol{v}}=\overline{\acute{\boldsymbol{v}}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Definition
general linear group
\begin_inset Formula 
\[
\begin{aligned}\begin{aligned}GL_{{\scriptscriptstyle n}}= & \left(GL_{{\scriptscriptstyle n}},\cdot_{{\scriptscriptstyle \mathcal{M}}}\right)=\left\{ A\middle|\begin{aligned}A\in\mathcal{M}_{{\scriptscriptstyle n\times n}}\Leftrightarrow A=\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\\
\forall A\in GL_{{\scriptscriptstyle n}}\left[\exists A^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]
\end{aligned}
\right\} \\
= & \left\{ \left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\middle|\forall\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\in GL_{{\scriptscriptstyle n}}\left[\exists\left(\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\right)^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]\right\} \\
= & \left\{ \begin{array}{c}
\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}},\\
\left(\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\right)^{{\scriptscriptstyle -1}}
\end{array}\middle|\forall a_{{\scriptscriptstyle ij}}\right\} =\left\{ \begin{array}{c}
A,\\
A^{{\scriptscriptstyle -1}}
\end{array}\middle|\begin{aligned}A\in\mathcal{M}_{{\scriptscriptstyle n\times n}}\\
\forall A\in GL_{{\scriptscriptstyle n}}\left[\exists A^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]
\end{aligned}
\right\} 
\end{aligned}
 & \qquad\forall n\in\mathbb{N}\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\begin{aligned}GL\left(n,\mathbb{F}\right)=GL_{{\scriptscriptstyle n}}\left(\mathbb{F}\right)= & \left(GL_{{\scriptscriptstyle n}}\left(\mathbb{F}\right),\cdot_{{\scriptscriptstyle \mathcal{M}}}\right)=\left\{ A\middle|\begin{aligned}A\in\mathcal{M}_{{\scriptscriptstyle n\times n}}\left(\mathbb{F}\right)\Leftrightarrow\begin{cases}
a_{{\scriptscriptstyle ij}}\in\mathbb{F}\\
A=\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}
\end{cases}\\
\forall A\in GL_{{\scriptscriptstyle n}}\left[\exists A^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]
\end{aligned}
\right\} \\
= & \left\{ \left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\middle|\begin{aligned}a_{{\scriptscriptstyle ij}}\in\mathbb{F}\\
\forall\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\in GL_{{\scriptscriptstyle n}}\left[\exists\left(\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\right)^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]
\end{aligned}
\right\} \\
= & \left\{ \begin{array}{c}
\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}},\\
\left(\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\right)^{{\scriptscriptstyle -1}}
\end{array}\middle|a_{{\scriptscriptstyle ij}}\in\mathbb{F}\right\} =\left\{ \begin{array}{c}
A,\\
A^{{\scriptscriptstyle -1}}
\end{array}\middle|\begin{aligned}A\in\mathcal{M}_{{\scriptscriptstyle n\times n}}\left(\mathbb{F}\right)\\
\forall A\in GL_{{\scriptscriptstyle n}}\left(\mathbb{F}\right)\left[\exists A^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\left(\mathbb{F}\right)\right]
\end{aligned}
\right\} 
\end{aligned}
 & \qquad\forall n\in\mathbb{N}\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\begin{aligned}GL\left(n,\mathbb{R}\right)=GL_{{\scriptscriptstyle n}}\left(\mathbb{R}\right)= & \left(GL_{{\scriptscriptstyle n}}\left(\mathbb{R}\right),\cdot_{{\scriptscriptstyle \mathcal{M}}}\right)=\left\{ A\middle|\begin{aligned}A\in\mathcal{M}_{{\scriptscriptstyle n\times n}}\left(\mathbb{R}\right)\Leftrightarrow\begin{cases}
a_{{\scriptscriptstyle ij}}\in\mathbb{R}\\
A=\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}
\end{cases}\\
\forall A\in GL_{{\scriptscriptstyle n}}\left[\exists A^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]
\end{aligned}
\right\} \\
= & \left\{ \left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\middle|\begin{aligned}a_{{\scriptscriptstyle ij}}\in\mathbb{R}\\
\forall\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\in GL_{{\scriptscriptstyle n}}\left[\exists\left(\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\right)^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]
\end{aligned}
\right\} \\
= & \left\{ \begin{array}{c}
\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}},\\
\left(\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\right)^{{\scriptscriptstyle -1}}
\end{array}\middle|a_{{\scriptscriptstyle ij}}\in\mathbb{R}\right\} =\left\{ \begin{array}{c}
A,\\
A^{{\scriptscriptstyle -1}}
\end{array}\middle|\begin{aligned}A\in\mathcal{M}_{{\scriptscriptstyle n\times n}}\left(\mathbb{R}\right)\\
\forall A=\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\left[\exists A^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]
\end{aligned}
\right\} \\
= & \left\{ \left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\middle|\begin{aligned}a_{{\scriptscriptstyle ij}}\in\mathbb{R}\\
\det\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\ne0
\end{aligned}
\right\} =\left\{ \left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\middle|\begin{aligned}a_{{\scriptscriptstyle ij}}\in\mathbb{R}\\
\det\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\in\mathbb{R}_{{\scriptscriptstyle \ne0}}
\end{aligned}
\right\} 
\end{aligned}
 & \qquad\forall n\in\mathbb{N}\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\begin{aligned}GL\left(n,\mathbb{C}\right)=GL_{{\scriptscriptstyle n}}\left(\mathbb{C}\right)= & \left(GL_{{\scriptscriptstyle n}}\left(\mathbb{C}\right),\cdot_{{\scriptscriptstyle \mathcal{M}}}\right)=\left\{ A\middle|\begin{aligned}A\in\mathcal{M}_{{\scriptscriptstyle n\times n}}\left(\mathbb{C}\right)\Leftrightarrow\begin{cases}
a_{{\scriptscriptstyle ij}}\in\mathbb{C}\\
A=\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}
\end{cases}\\
\forall A\in GL_{{\scriptscriptstyle n}}\left[\exists A^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]
\end{aligned}
\right\} \\
= & \left\{ \left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\middle|\begin{aligned}a_{{\scriptscriptstyle ij}}\in\mathbb{C}\\
\forall\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\in GL_{{\scriptscriptstyle n}}\left[\exists\left(\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\right)^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\right]
\end{aligned}
\right\} \\
= & \left\{ \begin{array}{c}
\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}},\\
\left(\left[a_{{\scriptscriptstyle ij}}\right]_{{\scriptscriptstyle n\times n}}\right)^{{\scriptscriptstyle -1}}
\end{array}\middle|a_{{\scriptscriptstyle ij}}\in\mathbb{C}\right\} =\left\{ \begin{array}{c}
A,\\
A^{{\scriptscriptstyle -1}}
\end{array}\middle|\begin{aligned}A\in\mathcal{M}_{{\scriptscriptstyle n\times n}}\left(\mathbb{C}\right)\\
\forall A\in GL_{{\scriptscriptstyle n}}\left(\mathbb{C}\right)\left[\exists A^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle n}}\left(\mathbb{C}\right)\right]
\end{aligned}
\right\} 
\end{aligned}
 & \qquad\forall n\in\mathbb{N}\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Example
\begin_inset Formula $GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)\simeq\mathbb{R}_{{\scriptscriptstyle \ne0}}\because\exists\det:GL\left(2,\mathbb{R}\right)=GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)\rightarrow\mathbb{R}_{{\scriptscriptstyle \ne0}}\subset\mathbb{R}$
\end_inset

 
\begin_inset Formula 
\[
\begin{aligned}GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)\text{ is } & \text{a multiplicative group} & \Leftarrow GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)=\left(GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right),\cdot_{{\scriptscriptstyle \mathcal{M}}}\right)\\
 &  & =\left\{ \begin{array}{c}
A,\\
A^{{\scriptscriptstyle -1}}
\end{array}\middle|\begin{aligned}A\in\mathcal{M}_{{\scriptscriptstyle 2\times2}}\left(\mathbb{R}\right)\\
\forall A\in GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)\left[\exists A^{{\scriptscriptstyle -1}}\in GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)\right]
\end{aligned}
\right\} \\
M,\widetilde{M}\in & GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)\\
\mathbb{R}_{{\scriptscriptstyle \ne0}}\text{ is } & \text{a multiplicative group} & \Rightarrow\mathbb{R}_{{\scriptscriptstyle \ne0}}=\left(\mathbb{R}_{{\scriptscriptstyle \ne0}},\cdot_{\mathbb{R}}\right)\\
r,\widetilde{r}\in & \mathbb{R}\\
\det: & GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)\rightarrow\mathbb{R}_{{\scriptscriptstyle \ne0}}\subset\mathbb{R} & \Leftrightarrow GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)\overset{\det}{\rightarrow}\mathbb{R}\Leftrightarrow\det\in\mathbb{R}^{GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)}\\
 &  & \exists!r=\det\left(M\right)=\det M,\exists!\widetilde{r}=\det\left(\widetilde{M}\right)=\det\widetilde{M}\\
\det\left(M\cdot_{{\scriptscriptstyle \mathcal{M}}}\widetilde{M}\right)= & \det\left(M\widetilde{M}\right)=\det\left(M\right)\cdot_{\mathbb{R}}\det\left(\widetilde{M}\right)=\det M\det\widetilde{M} & =r\cdot_{\mathbb{R}}\widetilde{r}=r\widetilde{r}\\
\Downarrow\\
GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right)\simeq & \mathbb{R}_{{\scriptscriptstyle \ne0}} & \text{homomorphism not mentioned isomorphism here}\\
\Updownarrow\\
GL_{{\scriptscriptstyle 2}}\left(\mathbb{R}\right), & \mathbb{R}_{{\scriptscriptstyle \ne0}}\text{ have homomorphism} & \text{homomorphism map }\det
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Example
\begin_inset Formula $\mathbb{R}\simeq U\left(1\right)\because\exists\exp\mathrm{i}:\mathbb{R}\rightarrow U\left(1\right)$
\end_inset

 
\begin_inset Formula 
\[
\begin{aligned}\mathbb{R}\text{ is } & \text{a additive group} & \Leftarrow\mathbb{R}=\left(\mathbb{R},+_{\mathbb{R}}\right)\\
\theta,\widetilde{\theta}\in & \mathbb{R}\\
U\left(1\right)\text{ is } & \text{a multiplicative group} & \Leftarrow U\left(1\right)=\left(U\left(1\right),\cdot_{\mathbb{C}}\right)=\left\{ \left|z\right|=1\middle|\forall z\in\mathbb{C}\right\} \\
 &  & =\left\{ \mathrm{e}^{\mathrm{i}\theta}\middle|\forall\theta\in\mathbb{R}\right\} \\
u,\widetilde{u}\in & U\left(1\right)\\
\exp\mathrm{i}: & \mathbb{R}\rightarrow U\left(1\right) & \Leftrightarrow\mathbb{R}\overset{\exp\mathrm{i}}{\rightarrow}U\left(1\right)\Leftrightarrow\exp\mathrm{i}\in U\left(1\right)^{\mathbb{R}}\\
 &  & \exists!u=\exp\mathrm{i}\left(\theta\right)=\mathrm{e}^{\mathrm{i}\theta},\exists!\widetilde{u}=\exp\mathrm{i}\left(\widetilde{\theta}\right)=\mathrm{e}^{\mathrm{i}\widetilde{\theta}}\\
\exp\mathrm{i}\left(\theta+_{\mathbb{R}}\widetilde{\theta}\right)= & \mathrm{e}^{\mathrm{i}\left(\theta+\widetilde{\theta}\right)}=\mathrm{e}^{\mathrm{i}\theta}\mathrm{e}^{\mathrm{i}\widetilde{\theta}}=\exp\mathrm{i}\left(\theta\right)\cdot_{\mathbb{C}}\exp\mathrm{i}\left(\widetilde{\theta}\right) & =u\cdot_{\mathbb{C}}\widetilde{u}=u\widetilde{u}\\
\Downarrow\\
\mathbb{R}\simeq & U\left(1\right) & \text{homomorphism not mentioned isomorphism here}\\
\Updownarrow\\
\mathbb{R}, & U\left(1\right)\text{ have homomorphism} & \text{homomorphism map }\exp\mathrm{i}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\exp\mathrm{i}\left(0\right)=\exp\mathrm{i}\left(2\pi\right)=\exp\mathrm{i}\left(2\pi k\right)=1\quad\forall k\in\mathbb{Z}\Rightarrow\exp\mathrm{i}\text{ is not 1-1 or 1-to-1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Example
\begin_inset Formula $S_{{\scriptscriptstyle n}}\simeq\mathbb{Z}_{{\scriptscriptstyle 2}}\because\exists\mathrm{sign}=\mathrm{sgn}:S_{{\scriptscriptstyle n}}\rightarrow\mathbb{Z}_{{\scriptscriptstyle 2}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}\sigma= & s_{{\scriptscriptstyle 1}}s_{{\scriptscriptstyle 2}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}=\overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{s_{{\scriptscriptstyle 1}}s_{{\scriptscriptstyle 2}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}}=s_{{\scriptscriptstyle 1}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}=\overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{s_{{\scriptscriptstyle 1}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}} & s_{{\scriptscriptstyle i}}\cap s_{{\scriptscriptstyle i+1}}=\left\{ s_{{\scriptscriptstyle \left(i\right)2}}\right\} \\
= & \underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\underset{s_{{\scriptscriptstyle 2}}}{\underbrace{\left(s_{{\scriptscriptstyle 21}}s_{{\scriptscriptstyle 22}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}} & s_{{\scriptscriptstyle \left(i\right)2}}=s_{{\scriptscriptstyle \left(i+1\right)1}}\\
= & \underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}}\\
= & \overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}}}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\sigma\begin{cases}
\text{is an even permutation} & N_{{\scriptscriptstyle \sigma}}\in2\mathbb{N}-2\\
\text{is an odd permutation} & N_{{\scriptscriptstyle \sigma}}\in2\mathbb{N}-1
\end{cases}\Leftrightarrow & \sigma\begin{cases}
\text{even} & N_{{\scriptscriptstyle \sigma}}\in2\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
\text{odd} & N_{{\scriptscriptstyle \sigma}}\in2\mathbb{N}-1
\end{cases} & \forall\sigma\in S_{{\scriptscriptstyle n}}\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}S_{{\scriptscriptstyle n}}\text{ is } & \text{a group} & \Leftarrow S_{{\scriptscriptstyle n}}=\left(S_{{\scriptscriptstyle n}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\right)=\left(S_{{\scriptscriptstyle n}},\circ\right)\\
\sigma,\widetilde{\sigma}\in & S_{{\scriptscriptstyle n}}\\
\mathbb{Z}_{{\scriptscriptstyle 2}}\text{ is } & \text{a group} & \Leftarrow\mathbb{Z}_{{\scriptscriptstyle 2}}=\left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{{\scriptscriptstyle \mathbb{C}}}\right)=\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{2}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{2}\left(2-1\right)}}\right\} \\
 &  & =\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\pi}}\right\} \\
 &  & =\left\{ -1,+1\right\} \\
\acute{\sigma},\widetilde{\acute{\sigma}}\in & \mathbb{Z}_{{\scriptscriptstyle 2}}\\
\mathrm{sign}=\mathrm{sgn}: & S_{{\scriptscriptstyle n}}\rightarrow\mathbb{Z}_{{\scriptscriptstyle 2}} & \Leftrightarrow S_{{\scriptscriptstyle n}}\overset{\mathrm{sgn}}{\rightarrow}\mathbb{Z}_{{\scriptscriptstyle 2}}\Leftrightarrow\mathrm{sgn}\in\mathbb{Z}_{{\scriptscriptstyle 2}}^{S_{{\scriptscriptstyle n}}}\\
 &  & \mathrm{sgn}\left(\sigma\right)=\begin{cases}
+1 & \sigma\text{ even}\Leftrightarrow N_{{\scriptscriptstyle \sigma}}\in2\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
-1 & \sigma\text{ odd}\Leftrightarrow N_{{\scriptscriptstyle \sigma}}\in2\mathbb{N}-1
\end{cases}\\
 &  & \exists!\acute{\sigma}=\mathrm{sgn}\left(\sigma\right),\exists!\widetilde{\acute{\sigma}}=\mathrm{sgn}\left(\widetilde{\sigma}\right)\\
\mathrm{sgn}\left(\sigma\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\widetilde{\sigma}\right)= & \mathrm{sgn}\left(\sigma\widetilde{\sigma}\right)=\mathrm{sgn}\left(\sigma\right)\mathrm{sgn}\left(\widetilde{\sigma}\right)=\mathrm{sgn}\left(\sigma\right)\cdot_{{\scriptscriptstyle \mathbb{C}}}\mathrm{sgn}\left(\widetilde{\sigma}\right) & =\acute{\sigma}\cdot_{{\scriptscriptstyle \mathbb{C}}}\widetilde{\acute{\sigma}}\\
\Downarrow\\
S_{{\scriptscriptstyle n}}\simeq & \mathbb{Z}_{{\scriptscriptstyle 2}} & \text{homomorphism not mentioned isomorphism here}\\
\Updownarrow\\
S_{{\scriptscriptstyle n}},\mathbb{Z}_{{\scriptscriptstyle 2}} & \text{ have homomorphism} & \text{homomorphism map }\mathrm{sgn}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Subsection
isomorphism
\end_layout

\begin_layout Subsection
homomorphism kernel
\end_layout

\begin_layout Part
tensor
\end_layout

\begin_layout Section
tensor algebra
\end_layout

\begin_layout Subsection
vector space
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}\mathbb{F}^{n}\in & \left\{ \mathbb{R}^{n},\mathbb{C}^{n},\mathbb{R}^{\infty},\cdots\right\}  & \widetilde{\mathfrak{V}},\mathfrak{V}\subseteq & \mathcal{V}\subseteq\mathbb{F}^{n}\in\left\{ \mathbb{R}^{n},\mathbb{C}^{n},\cdots\right\} \\
\mathcal{V}\ni\boldsymbol{v}= & v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}=\sum\limits_{j}v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}} & \boldsymbol{v}_{{\scriptscriptstyle j}}\in\mathfrak{V}= & \left\{ \boldsymbol{v}_{{\scriptscriptstyle j}}\right\} =\left\{ \boldsymbol{v}_{{\scriptscriptstyle j}}\right\} _{{\scriptscriptstyle j}}\\
= & \begin{cases}
v^{{\scriptscriptstyle 1}}\boldsymbol{v}_{{\scriptscriptstyle 1}}+\cdots+v^{{\scriptscriptstyle n}}\boldsymbol{v}_{{\scriptscriptstyle n}} & =\sum\limits_{j=1}^{n}v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}\\
\cdots+v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}+\cdots & =\sum\limits_{j\in J}v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}
\end{cases} & = & \begin{cases}
\left\{ \boldsymbol{v}_{{\scriptscriptstyle j}}\right\} _{{\scriptscriptstyle j=1}}^{{\scriptscriptstyle n}}\\
\left\{ \boldsymbol{v}_{{\scriptscriptstyle j}}\right\} _{{\scriptscriptstyle j\in J}} & J\in\left\{ \mathbb{N},\mathbb{Z},\mathbb{R},\cdots\right\} 
\end{cases}\\
= & \begin{cases}
v^{{\scriptscriptstyle 1}}\begin{bmatrix}|\\
\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
|
\end{bmatrix}+\cdots+v^{{\scriptscriptstyle n}}\begin{bmatrix}|\\
\boldsymbol{v}_{{\scriptscriptstyle n}}\\
|
\end{bmatrix} & =\begin{bmatrix}| &  & |\\
\boldsymbol{v}_{{\scriptscriptstyle 1}} & \cdots & \boldsymbol{v}_{{\scriptscriptstyle n}}\\
| &  & |
\end{bmatrix}\begin{bmatrix}v^{{\scriptscriptstyle 1}}\\
\vdots\\
v^{{\scriptscriptstyle n}}
\end{bmatrix}\\
\cdots+v^{{\scriptscriptstyle j}}\begin{bmatrix}|\\
\boldsymbol{v}_{{\scriptscriptstyle j}}\\
|
\end{bmatrix}+\cdots & =\begin{bmatrix} & |\\
\cdots & \boldsymbol{v}_{{\scriptscriptstyle j}} & \cdots\\
 & |
\end{bmatrix}\begin{bmatrix}\vdots\\
v^{{\scriptscriptstyle j}}\\
\vdots
\end{bmatrix}
\end{cases}=\begin{bmatrix}\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle j}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix}\vdots\\
v^{{\scriptscriptstyle j}}\\
\vdots
\end{bmatrix} & =V\left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}} & \qquad\left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}}\in\begin{cases}
\mathbb{F}^{n}\\
\mathbb{F}^{\left|J\right|}
\end{cases}\\
= & \widetilde{v}^{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=\sum\limits_{j}\widetilde{v}^{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}} & \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\in\widetilde{\mathfrak{V}}= & \left\{ \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right\} =\left\{ \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right\} _{{\scriptscriptstyle j}}\\
= & \begin{cases}
\widetilde{v}^{{\scriptscriptstyle 1}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}+\cdots+\widetilde{v}^{{\scriptscriptstyle n}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}} & =\sum\limits_{j=1}^{n}\widetilde{v}^{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\\
\cdots+\widetilde{v}^{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}+\cdots & =\sum\limits_{j\in J}\widetilde{v}^{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}
\end{cases} & = & \begin{cases}
\left\{ \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right\} _{{\scriptscriptstyle j=1}}^{{\scriptscriptstyle n}}\\
\left\{ \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right\} _{{\scriptscriptstyle j\in J}} & J\in\left\{ \mathbb{N},\mathbb{Z},\mathbb{R},\cdots\right\} 
\end{cases}\\
= & \begin{cases}
\widetilde{v}^{{\scriptscriptstyle 1}}\begin{bmatrix}|\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
|
\end{bmatrix}+\cdots+\widetilde{v}^{{\scriptscriptstyle n}}\begin{bmatrix}|\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}\\
|
\end{bmatrix} & =\begin{bmatrix}| &  & |\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}} & \cdots & \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}\\
| &  & |
\end{bmatrix}\begin{bmatrix}\widetilde{v}^{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{v}^{{\scriptscriptstyle n}}
\end{bmatrix}\\
\cdots+\widetilde{v}^{{\scriptscriptstyle j}}\begin{bmatrix}|\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\\
|
\end{bmatrix}+\cdots & =\begin{bmatrix} & |\\
\cdots & \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}} & \cdots\\
 & |
\end{bmatrix}\begin{bmatrix}\vdots\\
\widetilde{v}^{{\scriptscriptstyle j}}\\
\vdots
\end{bmatrix}
\end{cases}=\begin{bmatrix}\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix}\vdots\\
\widetilde{v}^{{\scriptscriptstyle j}}\\
\vdots
\end{bmatrix} & =\widetilde{V}\left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}} & \qquad\left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}}\in\begin{cases}
\mathbb{F}^{n}\\
\mathbb{F}^{\left|J\right|}
\end{cases}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\boldsymbol{v}= &  & V\left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}}= & \widetilde{V}\left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}}\\
v^{{\scriptscriptstyle i}}= &  & V^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}= & \widetilde{V}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\\
v^{{\scriptscriptstyle k}}= &  & V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}= & \widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\\
 &  & \left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}}= & V^{{\scriptscriptstyle -1}}\widetilde{V}\left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}}=\overline{V}\widetilde{V}\left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}} & = & F\left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}}\\
 &  & v^{{\scriptscriptstyle i}}= & \left(V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)^{{\scriptscriptstyle -1}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}=\overline{V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}} & = & F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\\
 &  & = & \left(V^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}=\overline{V}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}} & = & F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\\
 &  &  & \left(V^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}=\overline{V}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}} & = & F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}= &  & \left(V^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}} & \widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\\
 &  & \left(V^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}} & =\left(V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)^{{\scriptscriptstyle -1}}\\
 &  & \left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}}= & \widetilde{V}^{{\scriptscriptstyle -1}}V\left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}} & = & B\left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}}\\
 &  & \widetilde{v}^{{\scriptscriptstyle j}}= & \left(\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\right)^{{\scriptscriptstyle -1}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}} & = & B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}\\
 &  & = & \left(\widetilde{V}^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}} & = & B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}\\
 &  &  & \left(\widetilde{V}^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}} & = & B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\\
B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}= &  & \left(\widetilde{V}^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}} & V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\\
 &  & \left(\widetilde{V}^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}} & =\left(\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\right)^{{\scriptscriptstyle -1}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}v^{{\scriptscriptstyle i}}\boldsymbol{v}_{{\scriptscriptstyle i}}= & \widetilde{v}^{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=\left(\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\right)^{{\scriptscriptstyle -1}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}} & \widetilde{v}^{{\scriptscriptstyle j}}=\left(\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\right)^{{\scriptscriptstyle -1}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}=B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}\\
v^{{\scriptscriptstyle i}}\boldsymbol{v}_{{\scriptscriptstyle i}}= & \left(\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\right)^{{\scriptscriptstyle -1}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}} & \not v^{{\scriptscriptstyle i}}\\
\boldsymbol{v}_{{\scriptscriptstyle i}}= & \left(\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\right)^{{\scriptscriptstyle -1}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\\
\widetilde{v}^{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}= & v^{{\scriptscriptstyle i}}\boldsymbol{v}_{{\scriptscriptstyle i}}=\left(V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)^{{\scriptscriptstyle -1}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle i}} & v^{{\scriptscriptstyle i}}=\left(V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)^{{\scriptscriptstyle -1}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\\
\widetilde{v}^{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}= & \left(V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)^{{\scriptscriptstyle -1}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle i}} & \not\widetilde{v}^{{\scriptscriptstyle j}}\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}= & \left(V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)^{{\scriptscriptstyle -1}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle i}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{cases}
\begin{cases}
v^{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}=\left(V^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}} & \left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}}=F\left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}}=V^{{\scriptscriptstyle -1}}\widetilde{V}\left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}}\\
\widetilde{v}^{{\scriptscriptstyle j}}=B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}=\left(\widetilde{V}^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}} & \left[\boldsymbol{v}\right]_{{\scriptscriptstyle \widetilde{V}}}=B\left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}}=\widetilde{V}^{{\scriptscriptstyle -1}}V\left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}}
\end{cases} & \text{contravariant}\\
\begin{cases}
\boldsymbol{v}_{{\scriptscriptstyle i}}=B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=\left(\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\right)^{{\scriptscriptstyle -1}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}} & \begin{bmatrix}\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}B=\begin{bmatrix}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\widetilde{V}^{{\scriptscriptstyle -1}}V\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle i}}=\left(V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)^{{\scriptscriptstyle -1}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle i}} & \begin{bmatrix}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}F=\begin{bmatrix}\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}V^{{\scriptscriptstyle -1}}\widetilde{V}
\end{cases} & \text{covariant}
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
\begin{cases}
\begin{cases}
v^{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}=\left(V^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}} & \begin{bmatrix}v^{{\scriptscriptstyle 1}}\\
\vdots\\
v^{{\scriptscriptstyle n}}
\end{bmatrix}=F\begin{bmatrix}\widetilde{v}^{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{v}^{{\scriptscriptstyle n}}
\end{bmatrix}=V^{{\scriptscriptstyle -1}}\widetilde{V}\begin{bmatrix}\widetilde{v}^{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{v}^{{\scriptscriptstyle n}}
\end{bmatrix}\\
\widetilde{v}^{{\scriptscriptstyle j}}=B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}=\left(\widetilde{V}^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}} & \begin{bmatrix}\widetilde{v}^{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{v}^{{\scriptscriptstyle n}}
\end{bmatrix}=B\begin{bmatrix}v^{{\scriptscriptstyle 1}}\\
\vdots\\
v^{{\scriptscriptstyle n}}
\end{bmatrix}=\widetilde{V}^{{\scriptscriptstyle -1}}V\begin{bmatrix}v^{{\scriptscriptstyle 1}}\\
\vdots\\
v^{{\scriptscriptstyle n}}
\end{bmatrix}
\end{cases} & \text{contravariant}\\
\begin{cases}
\boldsymbol{v}_{{\scriptscriptstyle i}}=\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}=\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\left(\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\right)^{{\scriptscriptstyle -1}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}} & \begin{bmatrix}\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}B=\begin{bmatrix}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\widetilde{V}^{{\scriptscriptstyle -1}}V\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=\boldsymbol{v}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}=\boldsymbol{v}_{{\scriptscriptstyle i}}\left(V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)^{{\scriptscriptstyle -1}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}} & \begin{bmatrix}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}F=\begin{bmatrix}\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}V^{{\scriptscriptstyle -1}}\widetilde{V}
\end{cases} & \text{covariant}
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
\begin{cases}
BF=\left(\widetilde{V}^{{\scriptscriptstyle -1}}V\right)\left(V^{{\scriptscriptstyle -1}}\widetilde{V}\right)=\widetilde{V}^{{\scriptscriptstyle -1}}\left(VV^{{\scriptscriptstyle -1}}\right)\widetilde{V}=\widetilde{V}^{{\scriptscriptstyle -1}}1\widetilde{V}=\widetilde{V}^{{\scriptscriptstyle -1}}\widetilde{V}=1 & \Rightarrow B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}=\delta^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\\
FB=\left(V^{{\scriptscriptstyle -1}}\widetilde{V}\right)\left(\widetilde{V}^{{\scriptscriptstyle -1}}V\right)=V^{{\scriptscriptstyle -1}}\left(\widetilde{V}\widetilde{V}^{{\scriptscriptstyle -1}}\right)V=V^{{\scriptscriptstyle -1}}1V=V^{{\scriptscriptstyle -1}}V=1 & \Rightarrow F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}=\delta^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
\begin{cases}
\begin{cases}
v^{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}=\left(V^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}} & \begin{bmatrix}v^{{\scriptscriptstyle 1}}\\
\vdots\\
v^{{\scriptscriptstyle n}}
\end{bmatrix}=\begin{bmatrix} & \vdots\\
\cdots & F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}} & \cdots\\
 & \vdots
\end{bmatrix}\begin{bmatrix}\widetilde{v}^{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{v}^{{\scriptscriptstyle n}}
\end{bmatrix}=V^{{\scriptscriptstyle -1}}\widetilde{V}\begin{bmatrix}\widetilde{v}^{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{v}^{{\scriptscriptstyle n}}
\end{bmatrix}\\
\widetilde{v}^{{\scriptscriptstyle j}}=B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}=\left(\widetilde{V}^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}} & \begin{bmatrix}\widetilde{v}^{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{v}^{{\scriptscriptstyle n}}
\end{bmatrix}=\begin{bmatrix} & \vdots\\
\cdots & B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}} & \cdots\\
 & \vdots
\end{bmatrix}\begin{bmatrix}v^{{\scriptscriptstyle 1}}\\
\vdots\\
v^{{\scriptscriptstyle n}}
\end{bmatrix}=\widetilde{V}^{{\scriptscriptstyle -1}}V\begin{bmatrix}v^{{\scriptscriptstyle 1}}\\
\vdots\\
v^{{\scriptscriptstyle n}}
\end{bmatrix}
\end{cases} & \text{contravariant}\\
\begin{cases}
\boldsymbol{v}_{{\scriptscriptstyle i}}=\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}=\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\left(\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\right)^{{\scriptscriptstyle -1}}V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}} & \begin{bmatrix}\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix} & \vdots\\
\cdots & B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}} & \cdots\\
 & \vdots
\end{bmatrix}=\begin{bmatrix}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\widetilde{V}^{{\scriptscriptstyle -1}}V\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=\boldsymbol{v}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}=\boldsymbol{v}_{{\scriptscriptstyle i}}\left(V^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)^{{\scriptscriptstyle -1}}\widetilde{V}^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}} & \begin{bmatrix}\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle 1}}\\
\vdots\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix} & \vdots\\
\cdots & F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}} & \cdots\\
 & \vdots
\end{bmatrix}=\begin{bmatrix}\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
\vdots\\
\boldsymbol{v}_{{\scriptscriptstyle n}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}V^{{\scriptscriptstyle -1}}\widetilde{V}
\end{cases} & \text{covariant}
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
We do not denote 
\begin_inset Formula $V\left[\boldsymbol{v}\right]_{{\scriptscriptstyle V}}=V\left[\boldsymbol{v}\right]_{{\scriptscriptstyle \mathfrak{V}}}$
\end_inset

,
 because 
\begin_inset Formula $\mathfrak{V}$
\end_inset

 can have elements or bases in different orders whereas 
\begin_inset Formula $V$
\end_inset

 cannot.
\end_layout

\begin_layout Subsection
dual space
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}\begin{cases}
\boldsymbol{v}\in\mathcal{V}\subseteq\mathbb{F}^{n}\in\left\{ \mathbb{R}^{n},\mathbb{C}^{n},\cdots\right\} \\
\exists!\omega\in\mathbb{F}\left[\boldsymbol{\omega}\left(\boldsymbol{v}\right)=\omega\right]
\end{cases}\Leftrightarrow & \mathcal{V}\overset{\boldsymbol{\omega}}{\rightarrow}\mathbb{F}\Leftrightarrow\boldsymbol{\omega}:\mathcal{V}\rightarrow\mathbb{F}\\
\Leftrightarrow & \mathbb{F}^{\mathcal{V}}=\left\{ \boldsymbol{\omega}\middle|\boldsymbol{\omega}:\mathcal{V}\rightarrow\mathbb{F}\right\} \\
 & \Downarrow\\
 & \left|\mathbb{F}^{\mathcal{V}}\right|=\left|\mathbb{F}\right|^{\left|\mathcal{V}\right|}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\boldsymbol{v}^{{\scriptscriptstyle 1}}\left(\boldsymbol{v}_{{\scriptscriptstyle 1}}\right) & =1 & \cdots &  & \boldsymbol{v}^{{\scriptscriptstyle 1}}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right) &  & \cdots &  & \boldsymbol{v}^{{\scriptscriptstyle 1}}\left(\boldsymbol{v}_{{\scriptscriptstyle n}}\right)\\
\vdots &  &  &  & \vdots &  &  &  & \vdots\\
\boldsymbol{v}^{{\scriptscriptstyle i}}\left(\boldsymbol{v}_{{\scriptscriptstyle 1}}\right) &  & \cdots &  & \boldsymbol{v}^{{\scriptscriptstyle i}}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right) & \overset{\text{def.}}{=}\begin{cases}
1 & j=i\\
0 & j\ne i
\end{cases}=\delta_{{\scriptscriptstyle j}}^{{\scriptscriptstyle i}} & \cdots &  & \boldsymbol{v}^{{\scriptscriptstyle i}}\left(\boldsymbol{v}_{{\scriptscriptstyle n}}\right)\\
\vdots &  &  &  & \vdots &  &  &  & \vdots\\
\boldsymbol{v}^{{\scriptscriptstyle n}}\left(\boldsymbol{v}_{{\scriptscriptstyle 1}}\right) &  & \cdots &  & \boldsymbol{v}^{{\scriptscriptstyle n}}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right) &  & \cdots &  & \boldsymbol{v}^{{\scriptscriptstyle n}}\left(\boldsymbol{v}_{{\scriptscriptstyle n}}\right) & =1
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\boldsymbol{v}^{{\scriptscriptstyle i}}\left(\boldsymbol{v}\right)=\boldsymbol{v}^{{\scriptscriptstyle i}}\left(v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}\right)=v^{{\scriptscriptstyle j}}\boldsymbol{v}^{{\scriptscriptstyle i}}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)\overset{\text{def.}}{=}v^{{\scriptscriptstyle j}}\delta_{{\scriptscriptstyle j}}^{{\scriptscriptstyle i}}=v^{{\scriptscriptstyle i}}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned} & \begin{cases}
\boldsymbol{\omega}\in\mathcal{V}^{*}=\left(\mathcal{V}^{*},\mathbb{F},+,\cdot\right)=\left(\mathcal{V}^{*},\mathbb{F},+_{{\scriptscriptstyle \mathcal{V}^{*},\mathbb{F}}},\cdot_{{\scriptscriptstyle \mathbb{F}\times\mathcal{V}^{*},\mathbb{F}}}\right)\\
\boldsymbol{v}\in\mathcal{V}=\left(\mathcal{V},\mathbb{F},+,\cdot\right)=\left(\mathcal{V},\mathbb{F},+_{{\scriptscriptstyle \mathcal{V},\mathbb{F}}},\cdot_{{\scriptscriptstyle \mathcal{\mathbb{F}\times V},\mathbb{F}}}\right)
\end{cases}\\
\boldsymbol{\omega}\left(\boldsymbol{v}\right)= & \boldsymbol{\omega}\left(v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}\right)=v^{{\scriptscriptstyle j}}\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)\\
= & \boldsymbol{\omega}\left(\sum\limits_{j}v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}\right)=\sum\limits_{j}\boldsymbol{\omega}\left(v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}\right)=\sum\limits_{j}v^{{\scriptscriptstyle j}}\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)\\
= & \begin{cases}
\boldsymbol{\omega}\left(v^{{\scriptscriptstyle 1}}\boldsymbol{v}_{{\scriptscriptstyle 1}}+\cdots+v^{{\scriptscriptstyle n}}\boldsymbol{v}_{{\scriptscriptstyle n}}\right) & =\boldsymbol{\omega}\left(\sum\limits_{j=1}^{n}v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}\right)\\
\boldsymbol{\omega}\left(\cdots+v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}+\cdots\right) & =\boldsymbol{\omega}\left(\sum\limits_{j\in J}v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}\right)
\end{cases} & +=+_{{\scriptscriptstyle \mathcal{V},\mathbb{F}}}\\
= & \begin{cases}
v^{{\scriptscriptstyle 1}}\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle 1}}\right)+\cdots+v^{{\scriptscriptstyle n}}\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle n}}\right) & =\sum\limits_{j=1}^{n}v^{{\scriptscriptstyle j}}\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)\\
\cdots+v^{{\scriptscriptstyle j}}\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)+\cdots & =\sum\limits_{j\in J}v^{{\scriptscriptstyle j}}\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)
\end{cases} & +=+_{{\scriptscriptstyle \mathcal{V}^{*},\mathbb{F}}}\\
= & v^{{\scriptscriptstyle j}}\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)=\boldsymbol{v}^{{\scriptscriptstyle j}}\left(\boldsymbol{v}\right)\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right) & v^{{\scriptscriptstyle j}}=\boldsymbol{v}^{{\scriptscriptstyle j}}\left(\boldsymbol{v}\right)\Leftarrow\boldsymbol{v}^{{\scriptscriptstyle i}}\left(\boldsymbol{v}\right)=v^{{\scriptscriptstyle i}}\Leftarrow\boldsymbol{v}^{{\scriptscriptstyle i}}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)\overset{\text{def.}}{=}\delta_{{\scriptscriptstyle j}}^{{\scriptscriptstyle i}}\\
= & \boldsymbol{v}^{{\scriptscriptstyle j}}\left(\boldsymbol{v}\right)\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}=\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle j}}\left(\boldsymbol{v}\right)=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}\left(\boldsymbol{v}\right) & \omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}\overset{\text{def.}}{=}\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)\\
\boldsymbol{\omega}\left(\boldsymbol{v}\right)= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}\left(\boldsymbol{v}\right)\\
\boldsymbol{\omega}= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathcal{V}^{*}\ni\boldsymbol{\omega}= & \omega_{{\scriptscriptstyle i}}\boldsymbol{\omega}^{{\scriptscriptstyle i}}=\sum\limits_{i}\omega_{{\scriptscriptstyle i}}\boldsymbol{\omega}^{{\scriptscriptstyle i}}=\begin{cases}
\omega_{{\scriptscriptstyle 1}}\boldsymbol{\omega}^{{\scriptscriptstyle 1}}+\cdots+\omega_{{\scriptscriptstyle n}}\boldsymbol{\omega}^{{\scriptscriptstyle n}} & =\sum\limits_{i=1}^{n}\omega_{{\scriptscriptstyle i}}\boldsymbol{\omega}^{{\scriptscriptstyle i}}\\
\cdots+\omega_{{\scriptscriptstyle i}}\boldsymbol{\omega}^{{\scriptscriptstyle i}}+\cdots & =\sum\limits_{i\in I}\omega_{{\scriptscriptstyle i}}\boldsymbol{\omega}^{{\scriptscriptstyle i}}
\end{cases}\\
= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}=\sum\limits_{i}\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}=\begin{cases}
\omega_{{\scriptscriptstyle 1}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle 1}}+\cdots+\omega_{{\scriptscriptstyle n}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle n}} & =\sum\limits_{i=1}^{n}\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}\\
\cdots+\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}+\cdots & =\sum\limits_{i\in I}\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}
\end{cases}\\
= & \begin{cases}
\omega_{{\scriptscriptstyle 1}}^{{\scriptscriptstyle \boldsymbol{v}}}\begin{bmatrix}|\\
\boldsymbol{v}^{{\scriptscriptstyle 1}}\\
|
\end{bmatrix}^{{\scriptscriptstyle \intercal}}+\cdots+\omega_{{\scriptscriptstyle n}}^{{\scriptscriptstyle \boldsymbol{v}}}\begin{bmatrix}|\\
\boldsymbol{v}^{{\scriptscriptstyle n}}\\
|
\end{bmatrix}^{{\scriptscriptstyle \intercal}} & =\begin{bmatrix}\omega_{{\scriptscriptstyle 1}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\vdots\\
\omega_{{\scriptscriptstyle n}}^{{\scriptscriptstyle \boldsymbol{v}}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix}\text{---} & \boldsymbol{v}^{{\scriptscriptstyle 1}} & \text{---}\\
 & \vdots\\
\text{---} & \boldsymbol{v}^{{\scriptscriptstyle n}} & \text{---}
\end{bmatrix}\\
\cdots+\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\begin{bmatrix}|\\
\boldsymbol{v}^{{\scriptscriptstyle i}}\\
|
\end{bmatrix}^{{\scriptscriptstyle \intercal}}+\cdots & =\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix} & \vdots\\
\text{---} & \boldsymbol{v}^{{\scriptscriptstyle i}} & \text{---}\\
 & \vdots
\end{bmatrix}
\end{cases}=\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix}\vdots\\
\boldsymbol{v}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix} & =\left[\boldsymbol{\omega}\right]^{{\scriptscriptstyle V}}V^{*}\\
= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}=\sum\limits_{i}\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}=\begin{cases}
\omega_{{\scriptscriptstyle 1}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle 1}}+\cdots+\omega_{{\scriptscriptstyle n}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle n}} & =\sum\limits_{i=1}^{n}\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}\\
\cdots+\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}+\cdots & =\sum\limits_{i\in I}\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}
\end{cases}\\
= & \begin{cases}
\omega_{{\scriptscriptstyle 1}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\begin{bmatrix}|\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle 1}}\\
|
\end{bmatrix}^{{\scriptscriptstyle \intercal}}+\cdots+\omega_{{\scriptscriptstyle n}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\begin{bmatrix}|\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle n}}\\
|
\end{bmatrix}^{{\scriptscriptstyle \intercal}} & =\begin{bmatrix}\omega_{{\scriptscriptstyle 1}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\\
\vdots\\
\omega_{{\scriptscriptstyle n}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix}\text{---} & \widetilde{\boldsymbol{v}}^{{\scriptscriptstyle 1}} & \text{---}\\
 & \vdots\\
\text{---} & \widetilde{\boldsymbol{v}}^{{\scriptscriptstyle n}} & \text{---}
\end{bmatrix}\\
\cdots+\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\begin{bmatrix}|\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}\\
|
\end{bmatrix}^{{\scriptscriptstyle \intercal}}+\cdots & =\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix} & \vdots\\
\text{---} & \widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}} & \text{---}\\
 & \vdots
\end{bmatrix}
\end{cases}=\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}\begin{bmatrix}\vdots\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix} & =\left[\boldsymbol{\omega}\right]^{{\scriptscriptstyle \widetilde{V}}}\widetilde{V}^{*}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\boldsymbol{\omega}= & \left[\boldsymbol{\omega}\right]^{{\scriptscriptstyle V}}V^{*}=\left[\boldsymbol{\omega}\right]^{{\scriptscriptstyle \widetilde{V}}}\widetilde{V}^{*}\\
= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}V^{*}{}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}=\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{V}^{*}{}^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{V}^{*}{}^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}V^{*}{}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}V^{*}{}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\left(\widetilde{V}^{*}{}^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}\right)^{{\scriptscriptstyle -1}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}V^{*}{}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\left(\widetilde{V}^{*}{}^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}Q^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\boldsymbol{\omega}\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right)=\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}Q^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}=\boldsymbol{\omega}\left(\boldsymbol{v}_{{\scriptscriptstyle i}}\right)Q^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}=\boldsymbol{\omega}\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle k}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\right)Q^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}=\boldsymbol{\omega}\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle k}}\right)B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}Q^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\boldsymbol{\omega}\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right)= & \boldsymbol{\omega}\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle k}}\right)B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}Q^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}Q^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}= & \delta^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\Rightarrow Q^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}Q^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\Rightarrow\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}F\\
\omega_{{\scriptscriptstyle k}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\delta^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}=\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}= & \omega_{{\scriptscriptstyle k}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\Rightarrow\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}B
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\boldsymbol{v}^{{\scriptscriptstyle i}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}= & \omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}=\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\boldsymbol{v}^{{\scriptscriptstyle i}}= & \omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle j}}\Rightarrow B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\boldsymbol{v}^{{\scriptscriptstyle i}}=\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle j}}\Rightarrow\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle j}}=B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\boldsymbol{v}^{{\scriptscriptstyle i}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}= & \omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}\Rightarrow F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}=\boldsymbol{v}^{{\scriptscriptstyle j}}\Rightarrow\boldsymbol{v}^{{\scriptscriptstyle j}}=F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{cases}
\begin{cases}
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}=\omega_{{\scriptscriptstyle k}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}} & \begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}B\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}} & \begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}F
\end{cases} & \text{covariant}\\
\begin{cases}
\boldsymbol{v}^{{\scriptscriptstyle j}}=F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}} & \begin{bmatrix}\vdots\\
\boldsymbol{v}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix}=F\begin{bmatrix}\vdots\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix}\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle j}}=B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\boldsymbol{v}^{{\scriptscriptstyle i}} & \begin{bmatrix}\vdots\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix}=B\begin{bmatrix}\vdots\\
\boldsymbol{v}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix}
\end{cases} & \text{contravariant}
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
\begin{cases}
\begin{cases}
\boldsymbol{v}^{{\scriptscriptstyle j}}=F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle i}}\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}} & \begin{bmatrix}\vdots\\
\boldsymbol{v}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix}=F\begin{bmatrix}\vdots\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix}\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle j}}=B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle i}}\boldsymbol{v}^{{\scriptscriptstyle i}} & \begin{bmatrix}\vdots\\
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix}=B\begin{bmatrix}\vdots\\
\boldsymbol{v}^{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix}
\end{cases} & \text{contravariant}\\
\begin{cases}
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}=\omega_{{\scriptscriptstyle k}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}} & \begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}B\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}} & \begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}=\begin{bmatrix}\vdots\\
\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\\
\vdots
\end{bmatrix}^{{\scriptscriptstyle \intercal}}F
\end{cases} & \text{covariant}
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned} & \text{covariant} &  & \text{contravariant}\\
\begin{array}{c}
\widetilde{\mathfrak{V}}\\
\mathfrak{V}
\end{array}\Biggr\}\ni & \begin{cases}
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=\boldsymbol{v}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\boldsymbol{v}_{{\scriptscriptstyle j}}=\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle i}}B^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}
\end{cases} & \mathbb{F}\ni & \begin{cases}
\widetilde{v}^{{\scriptscriptstyle i}}=B^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\\
v^{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}
\end{cases} & \text{vector space }\mathcal{V}\ni\boldsymbol{v}=\boldsymbol{v}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\\
\mathbb{F}\ni & \begin{cases}
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}=\omega_{{\scriptscriptstyle k}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}
\end{cases} & \begin{array}{c}
\widetilde{\mathfrak{V}}^{*}\\
\mathfrak{V}^{*}
\end{array}\Biggr\}\ni & \begin{cases}
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}=B^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\boldsymbol{v}^{{\scriptscriptstyle j}}\\
\boldsymbol{v}^{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}^{j}
\end{cases} & \text{dual space }\mathcal{V}^{*}\ni\boldsymbol{\omega}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}=\boldsymbol{v}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}v^{{\scriptscriptstyle k}}=\boldsymbol{v}_{{\scriptscriptstyle i}}\delta^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}v^{{\scriptscriptstyle k}}=\begin{cases}
\boldsymbol{v}_{{\scriptscriptstyle k}}v^{{\scriptscriptstyle k}} & \boldsymbol{v}_{{\scriptscriptstyle k}}=\boldsymbol{v}_{{\scriptscriptstyle i}}\delta^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}\\
\boldsymbol{v}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}} & \delta^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle k}}v^{{\scriptscriptstyle k}}=v^{{\scriptscriptstyle i}}
\end{cases}=v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}
\]

\end_inset


\end_layout

\begin_layout Subsection
linear map transformation
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}\begin{cases}
\boldsymbol{v}\in\mathcal{V}\subseteq\mathbb{F}^{n}\in\left\{ \mathbb{R}^{n},\mathbb{C}^{n},\cdots\right\} \\
\exists!\boldsymbol{w}\in\mathcal{W}\left[L\left(\boldsymbol{v}\right)=\boldsymbol{w}\right]
\end{cases}\Leftrightarrow & \mathcal{V}\overset{L}{\rightarrow}\mathcal{W}\Leftrightarrow L:\mathcal{V}\rightarrow\mathcal{W}\\
\Leftrightarrow & \mathbb{\mathcal{W}}^{\mathcal{V}}=\left\{ L\middle|L:\mathcal{V}\rightarrow\mathcal{W}\right\} \\
 & \Downarrow\\
 & \left|\mathbb{\mathcal{W}}^{\mathcal{V}}\right|=\left|\mathcal{W}\right|^{\left|\mathcal{V}\right|}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\boldsymbol{w}=L\left(\boldsymbol{v}\right)=L\left(v^{{\scriptscriptstyle j}}\boldsymbol{v}_{{\scriptscriptstyle j}}\right)=v^{{\scriptscriptstyle j}}L\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}L\left(\boldsymbol{v}_{{\scriptscriptstyle 1}}\right)= & \boldsymbol{v}_{{\scriptscriptstyle 1}}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle 1}}+\cdots+\boldsymbol{v}_{{\scriptscriptstyle n}}L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle 1}}=\begin{bmatrix}|\\
\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
|
\end{bmatrix}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle 1}}+\cdots+\begin{bmatrix}|\\
\boldsymbol{v}_{{\scriptscriptstyle n}}\\
|
\end{bmatrix}L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle 1}}=\begin{bmatrix}| &  & |\\
\boldsymbol{v}_{{\scriptscriptstyle 1}} & \cdots & \boldsymbol{v}_{{\scriptscriptstyle n}}\\
| &  & |
\end{bmatrix}\begin{bmatrix}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle 1}}\\
\vdots\\
L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle 1}}
\end{bmatrix}\\
\vdots\\
L\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)= & \boldsymbol{v}_{{\scriptscriptstyle 1}}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle j}}+\cdots+\boldsymbol{v}_{{\scriptscriptstyle n}}L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle j}}=\begin{bmatrix}|\\
\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
|
\end{bmatrix}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle j}}+\cdots+\begin{bmatrix}|\\
\boldsymbol{v}_{{\scriptscriptstyle n}}\\
|
\end{bmatrix}L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle j}}=\begin{bmatrix}| &  & |\\
\boldsymbol{v}_{{\scriptscriptstyle 1}} & \cdots & \boldsymbol{v}_{{\scriptscriptstyle n}}\\
| &  & |
\end{bmatrix}\begin{bmatrix}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle j}}\\
\vdots\\
L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle j}}
\end{bmatrix}\\
\vdots\\
L\left(\boldsymbol{v}_{{\scriptscriptstyle n}}\right)= & \boldsymbol{v}_{{\scriptscriptstyle 1}}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle n}}+\cdots+\boldsymbol{v}_{{\scriptscriptstyle n}}L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle n}}=\begin{bmatrix}|\\
\boldsymbol{v}_{{\scriptscriptstyle 1}}\\
|
\end{bmatrix}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle n}}+\cdots+\begin{bmatrix}|\\
\boldsymbol{v}_{{\scriptscriptstyle n}}\\
|
\end{bmatrix}L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle n}}=\begin{bmatrix}| &  & |\\
\boldsymbol{v}_{{\scriptscriptstyle 1}} & \cdots & \boldsymbol{v}_{{\scriptscriptstyle n}}\\
| &  & |
\end{bmatrix}\begin{bmatrix}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle n}}\\
\vdots\\
L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle n}}
\end{bmatrix}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\begin{bmatrix}| &  & |\\
L\left(\boldsymbol{v}_{{\scriptscriptstyle 1}}\right) & \cdots & L\left(\boldsymbol{v}_{{\scriptscriptstyle n}}\right)\\
| &  & |
\end{bmatrix}= & \begin{bmatrix}| &  & |\\
\boldsymbol{v}_{{\scriptscriptstyle 1}} & \cdots & \boldsymbol{v}_{{\scriptscriptstyle n}}\\
| &  & |
\end{bmatrix}\begin{bmatrix}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle 1}} &  & L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle n}}\\
\vdots & \cdots & \vdots\\
L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle 1}} &  & L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle n}}
\end{bmatrix}\\
\boldsymbol{w}=L\left(\boldsymbol{v}\right)=v^{{\scriptscriptstyle j}}L\left(\boldsymbol{v}_{{\scriptscriptstyle j}}\right)=\begin{bmatrix}| &  & |\\
L\left(\boldsymbol{v}_{{\scriptscriptstyle 1}}\right) & \cdots & L\left(\boldsymbol{v}_{{\scriptscriptstyle n}}\right)\\
| &  & |
\end{bmatrix}\begin{bmatrix}v^{{\scriptscriptstyle 1}}\\
\vdots\\
v^{{\scriptscriptstyle n}}
\end{bmatrix}= & \begin{bmatrix}| &  & |\\
\boldsymbol{v}_{{\scriptscriptstyle 1}} & \cdots & \boldsymbol{v}_{{\scriptscriptstyle n}}\\
| &  & |
\end{bmatrix}\begin{bmatrix}L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle 1}} &  & L^{{\scriptscriptstyle 1}}{}_{{\scriptscriptstyle n}}\\
\vdots & \cdots & \vdots\\
L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle 1}} &  & L^{{\scriptscriptstyle n}}{}_{{\scriptscriptstyle n}}
\end{bmatrix}\begin{bmatrix}v^{{\scriptscriptstyle 1}}\\
\vdots\\
v^{{\scriptscriptstyle n}}
\end{bmatrix}\\
=\begin{bmatrix} & |\\
\cdots & L\left(\boldsymbol{v}_{{\scriptscriptstyle n}}\right) & \cdots\\
 & |
\end{bmatrix}\begin{bmatrix}\vdots\\
v^{{\scriptscriptstyle j}}\\
\vdots
\end{bmatrix}= & \begin{bmatrix} & |\\
\cdots & \boldsymbol{v}_{{\scriptscriptstyle i}} & \cdots\\
 & |
\end{bmatrix}\begin{bmatrix} & \vdots\\
\cdots & L^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}} & \cdots\\
 & \vdots
\end{bmatrix}\begin{bmatrix}\vdots\\
v^{{\scriptscriptstyle j}}\\
\vdots
\end{bmatrix}\\
=\boldsymbol{v}_{{\scriptscriptstyle i}}w_{{\scriptscriptstyle \boldsymbol{v}}}^{{\scriptscriptstyle i}}= & \boldsymbol{v}_{{\scriptscriptstyle i}}L^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}} & w_{{\scriptscriptstyle \boldsymbol{v}}}^{{\scriptscriptstyle i}}=L^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\\
=\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle i}}w_{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}^{{\scriptscriptstyle i}}= & \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle i}}\widetilde{L}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}} & w_{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}^{{\scriptscriptstyle i}}=\widetilde{L}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle h}}B^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}L^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}\widetilde{v}^{{\scriptscriptstyle k}}=\boldsymbol{v}_{{\scriptscriptstyle i}}L^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}= & \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle i}}\widetilde{L}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}=\boldsymbol{v}_{{\scriptscriptstyle h}}F^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}\widetilde{L}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}v^{{\scriptscriptstyle k}}\\
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle h}}B^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}L^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}\widetilde{v}^{{\scriptscriptstyle k}}= & \widetilde{\boldsymbol{v}}_{{\scriptscriptstyle h}}\widetilde{L}^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle k}}\widetilde{v}^{{\scriptscriptstyle k}} & \widetilde{L}^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle k}}=B^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}L^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}\\
\boldsymbol{v}_{{\scriptscriptstyle h}}F^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}\widetilde{L}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}v^{{\scriptscriptstyle k}}= & \boldsymbol{v}_{{\scriptscriptstyle h}}L^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle k}}v^{{\scriptscriptstyle k}} & L^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle k}}=F^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}\widetilde{L}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\text{covariant } & \left(0,1\right)\text{-tensor} & \text{contravariant } & \left(1,0\right)\text{-tensor}\\
\begin{array}{c}
\widetilde{\mathfrak{V}}\\
\mathfrak{V}
\end{array}\Biggr\}\ni & \begin{cases}
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=\boldsymbol{v}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\boldsymbol{v}_{{\scriptscriptstyle j}}=\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle i}}B^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}
\end{cases} & \mathbb{F}\ni & \begin{cases}
\widetilde{v}^{{\scriptscriptstyle i}}=B^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\\
v^{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}
\end{cases} & \text{vector space }\mathcal{V}\ni\boldsymbol{v}=\boldsymbol{v}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\\
\mathbb{F}\ni & \begin{cases}
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}=\omega_{{\scriptscriptstyle k}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}
\end{cases} & \begin{array}{c}
\widetilde{\mathfrak{V}}^{*}\\
\mathfrak{V}^{*}
\end{array}\Biggr\}\ni & \begin{cases}
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}=B^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\boldsymbol{v}^{{\scriptscriptstyle j}}\\
\boldsymbol{v}^{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}^{j}
\end{cases} & \text{dual space }\mathcal{V}^{*}\ni\boldsymbol{\omega}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}\\
 &  &  & \left(1,1\right)\text{-tensor} & \mathcal{V}\overset{L}{\rightarrow}\mathcal{W}\\
 &  &  & \begin{cases}
\widetilde{L}^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle k}}=B^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}L^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}\\
L^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle k}}=F^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}\widetilde{L}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}
\end{cases} & \text{vector space }\mathcal{W}\ni\boldsymbol{v}=\boldsymbol{v}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Subsection
metric tensor
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}\left\Vert \boldsymbol{v}\right\Vert ^{2}=\boldsymbol{v}\cdot\boldsymbol{v}= & \begin{aligned}\left(\boldsymbol{v}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}\right)\cdot\left(\boldsymbol{v}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\right)\end{aligned}
=\left(\boldsymbol{v}_{{\scriptscriptstyle i}}\cdot\boldsymbol{v}_{{\scriptscriptstyle j}}\right)v^{{\scriptscriptstyle i}}v^{{\scriptscriptstyle j}}=g_{{\scriptscriptstyle ij}}v^{{\scriptscriptstyle i}}v^{{\scriptscriptstyle j}}=v^{{\scriptscriptstyle i}}\left(\boldsymbol{v}_{{\scriptscriptstyle i}}\cdot\boldsymbol{v}_{{\scriptscriptstyle j}}\right)v^{{\scriptscriptstyle j}}=v^{{\scriptscriptstyle i}}g_{{\scriptscriptstyle ij}}v^{{\scriptscriptstyle j}}\\
= & \begin{aligned}\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle i}}\widetilde{v}^{{\scriptscriptstyle i}}\right)\cdot\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\right)\end{aligned}
=\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle i}}\cdot\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right)\widetilde{v}^{{\scriptscriptstyle i}}\widetilde{v}^{{\scriptscriptstyle j}}=\widetilde{g}_{{\scriptscriptstyle ij}}\widetilde{v}^{{\scriptscriptstyle i}}\widetilde{v}^{{\scriptscriptstyle j}}=\widetilde{v}^{{\scriptscriptstyle i}}\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle i}}\cdot\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right)\widetilde{v}^{{\scriptscriptstyle j}}=\widetilde{v}^{{\scriptscriptstyle i}}\widetilde{g}_{{\scriptscriptstyle ij}}\widetilde{v}^{{\scriptscriptstyle j}}\\
= & g_{{\scriptscriptstyle ij}}v^{{\scriptscriptstyle i}}v^{{\scriptscriptstyle j}}=\widetilde{g}_{{\scriptscriptstyle ij}}\widetilde{v}^{{\scriptscriptstyle i}}\widetilde{v}^{{\scriptscriptstyle j}}=\widetilde{g}_{{\scriptscriptstyle hk}}B^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}v^{{\scriptscriptstyle i}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}=\widetilde{g}_{{\scriptscriptstyle hk}}B^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle i}}v^{{\scriptscriptstyle j}} & g_{{\scriptscriptstyle ij}}=\widetilde{g}_{{\scriptscriptstyle hk}}B^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\\
= & \widetilde{g}_{{\scriptscriptstyle ij}}\widetilde{v}^{{\scriptscriptstyle i}}\widetilde{v}^{{\scriptscriptstyle j}}=g_{{\scriptscriptstyle ij}}v^{{\scriptscriptstyle i}}v^{{\scriptscriptstyle j}}=g_{{\scriptscriptstyle hk}}F^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}\widetilde{v}^{{\scriptscriptstyle i}}F^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}=g_{{\scriptscriptstyle hk}}F^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle i}}\widetilde{v}^{{\scriptscriptstyle j}} & \widetilde{g}_{{\scriptscriptstyle ij}}=g_{{\scriptscriptstyle hk}}F^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\text{covariant } & \left(0,1\right)\text{-tensor} & \text{contravariant } & \left(1,0\right)\text{-tensor}\\
\begin{array}{c}
\widetilde{\mathfrak{V}}\\
\mathfrak{V}
\end{array}\Biggr\}\ni & \begin{cases}
\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}=\boldsymbol{v}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\boldsymbol{v}_{{\scriptscriptstyle j}}=\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle i}}B^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}
\end{cases} & \mathbb{F}\ni & \begin{cases}
\widetilde{v}^{{\scriptscriptstyle i}}=B^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\\
v^{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}
\end{cases} & \text{vector space }\mathcal{V}\ni\boldsymbol{v}=\boldsymbol{v}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\\
\mathbb{F}\ni & \begin{cases}
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\\
\omega_{{\scriptscriptstyle j}}^{{\scriptscriptstyle \boldsymbol{v}}}=\omega_{{\scriptscriptstyle k}}^{{\scriptscriptstyle \widetilde{\boldsymbol{v}}}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}
\end{cases} & \begin{array}{c}
\widetilde{\mathfrak{V}}^{*}\\
\mathfrak{V}^{*}
\end{array}\Biggr\}\ni & \begin{cases}
\widetilde{\boldsymbol{v}}^{{\scriptscriptstyle i}}=B^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\boldsymbol{v}^{{\scriptscriptstyle j}}\\
\boldsymbol{v}^{{\scriptscriptstyle i}}=F^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}\widetilde{\boldsymbol{v}}^{j}
\end{cases} & \text{dual space }\mathcal{V}^{*}\ni\boldsymbol{\omega}=\omega_{{\scriptscriptstyle i}}^{{\scriptscriptstyle \boldsymbol{v}}}\boldsymbol{v}^{{\scriptscriptstyle i}}\\
 &  &  & \left(1,1\right)\text{-tensor} & \mathcal{V}\overset{L}{\rightarrow}\mathcal{W}\\
 &  &  & \begin{cases}
\widetilde{L}^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle k}}=B^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}L^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}F^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}\\
L^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle k}}=F^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}\widetilde{L}^{{\scriptscriptstyle i}}{}_{{\scriptscriptstyle j}}B^{{\scriptscriptstyle j}}{}_{{\scriptscriptstyle k}}
\end{cases} & \text{vector space }\mathcal{W}\ni\boldsymbol{v}=\boldsymbol{v}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\\
 & \left(0,2\right)\text{-tensor} &  &  & \mathcal{V}^{2}\overset{g}{\rightarrow}\mathcal{\mathbb{R}}_{{\scriptscriptstyle \ge0}}\\
\mathcal{\mathbb{R}}_{{\scriptscriptstyle \ge0}}^{\mathcal{V}^{2}}\ni & \begin{cases}
\widetilde{g}_{{\scriptscriptstyle ij}}=g_{{\scriptscriptstyle hk}}F^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}F^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}\\
g_{{\scriptscriptstyle ij}}=\widetilde{g}_{{\scriptscriptstyle hk}}B^{{\scriptscriptstyle h}}{}_{{\scriptscriptstyle i}}B^{{\scriptscriptstyle k}}{}_{{\scriptscriptstyle j}}
\end{cases} &  &  & \text{metric space }\mathcal{V}\times\mathcal{V}\overset{g}{\rightarrow}\mathcal{\mathbb{F}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\boldsymbol{u}\cdot\boldsymbol{v}= & \begin{aligned}\left(\boldsymbol{u}_{{\scriptscriptstyle i}}u^{{\scriptscriptstyle i}}\right)\cdot\left(\boldsymbol{v}_{{\scriptscriptstyle j}}v^{{\scriptscriptstyle j}}\right)\end{aligned}
=\left(\boldsymbol{u}_{{\scriptscriptstyle i}}\cdot\boldsymbol{v}_{{\scriptscriptstyle j}}\right)u^{{\scriptscriptstyle i}}v^{{\scriptscriptstyle j}}=g_{{\scriptscriptstyle ij}}u^{{\scriptscriptstyle i}}v^{{\scriptscriptstyle j}}=u^{{\scriptscriptstyle i}}\left(\boldsymbol{u}_{{\scriptscriptstyle i}}\cdot\boldsymbol{v}_{{\scriptscriptstyle j}}\right)v^{{\scriptscriptstyle j}}=u^{{\scriptscriptstyle i}}g_{{\scriptscriptstyle ij}}v^{{\scriptscriptstyle j}}\\
= & \begin{aligned}\left(\widetilde{\boldsymbol{u}}_{{\scriptscriptstyle i}}\widetilde{u}^{{\scriptscriptstyle i}}\right)\cdot\left(\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\widetilde{v}^{{\scriptscriptstyle j}}\right)\end{aligned}
=\left(\widetilde{\boldsymbol{u}}_{{\scriptscriptstyle i}}\cdot\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right)\widetilde{u}^{{\scriptscriptstyle i}}\widetilde{v}^{{\scriptscriptstyle j}}=\widetilde{g}_{{\scriptscriptstyle ij}}\widetilde{u}^{{\scriptscriptstyle i}}\widetilde{v}^{{\scriptscriptstyle j}}=\widetilde{u}^{{\scriptscriptstyle i}}\left(\widetilde{\boldsymbol{u}}_{{\scriptscriptstyle i}}\cdot\widetilde{\boldsymbol{v}}_{{\scriptscriptstyle j}}\right)\widetilde{v}^{{\scriptscriptstyle j}}=\widetilde{u}^{{\scriptscriptstyle i}}\widetilde{g}_{{\scriptscriptstyle ij}}\widetilde{v}^{{\scriptscriptstyle j}}
\end{aligned}
\]

\end_inset


\end_layout

