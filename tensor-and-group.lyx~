\begin_layout Definition
\begin_inset Formula $S_{{\scriptscriptstyle 3}}$
\end_inset


\begin_inset Formula 
\[
\recolor{\begin{array}{ccccccccc}
S_{{\scriptscriptstyle 3}}=\{ & \sigma_{{\scriptscriptstyle 1}}, & \sigma_{{\scriptscriptstyle 2}}, & \sigma_{{\scriptscriptstyle 3}}, & \sigma_{{\scriptscriptstyle 4}}, & \sigma_{{\scriptscriptstyle 5}}, & \sigma_{{\scriptscriptstyle 6}} & \}\\
\quad=\{ & \begin{pmatrix}1 & 2 & 3\\
1 & 2 & 3
\end{pmatrix}, & \begin{pmatrix}1 & 2 & 3\\
2 & 3 & 1
\end{pmatrix}, & \begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix}, & \begin{pmatrix}1 & 2 & 3\\
2 & 1 & 3
\end{pmatrix}, & \begin{pmatrix}1 & 2 & 3\\
1 & 3 & 2
\end{pmatrix}, & \begin{pmatrix}1 & 2 & 3\\
3 & 2 & 1
\end{pmatrix} & \} & \text{two-line notation}\\
\quad=\{ & \left[123\right], & \left[231\right], & \left[312\right], & \left[213\right], & \left[132\right], & \left[321\right] & \} & \text{one-line notation}\\
\quad=\{ & 123, & 231, & 312, & 213, & 132, & 321 & \} & \text{one-line notation,}\\
 &  &  &  &  &  &  &  & \text{simplified}\\
\quad=\{ & \left(1\right)\left(2\right)\left(3\right), & \left(123\right), & \left(132\right), & \left(12\right)\left(3\right), & \left(1\right)\left(23\right), & \left(31\right)\left(2\right) & \} & \text{cycle notation}\\
\quad=\{ & \left(\right), & \left(123\right), & \left(132\right), & \left(12\right), & \left(23\right), & \left(31\right) & \} & \text{cycle notation,}\\
 &  &  &  &  &  &  &  & \text{simplified}
\end{array}}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned} & \sigma_{{\scriptscriptstyle 6}}\sigma_{{\scriptscriptstyle 3}} & =\sigma_{{\scriptscriptstyle 6}}\circ\sigma_{{\scriptscriptstyle 3}}\\
= & \begin{pmatrix}1 & 2 & 3\\
3 & 2 & 1
\end{pmatrix}\begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix} & =\begin{pmatrix}1 & 2 & 3\\
3 & 2 & 1
\end{pmatrix}\circ\begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix}\\
 & \qquad\parallel\\
= & \begin{pmatrix}3 & 1 & 2\\
1 & 3 & 2
\end{pmatrix}\begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix} & \begin{pmatrix}1 & 2 & 3\\
3 & 2 & 1
\end{pmatrix}=\begin{pmatrix}3 & 1 & 2\\
1 & 3 & 2
\end{pmatrix}\\
 & \begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix}\\
= & \begin{pmatrix}3 & 1 & 2\\
1 & 3 & 2
\end{pmatrix}=\begin{pmatrix}1 & 2 & 3\\
1 & 3 & 2
\end{pmatrix} & =\sigma_{{\scriptscriptstyle 5}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\recolor{\begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}} & \left[123\right] & \left[231\right] & \left[312\right] & \left[213\right] & \left[132\right] & \left[321\right]\\
\left[123\right] & \left[123\right] & \left[231\right] & \left[312\right] & \left[213\right] & \left[132\right] & \left[321\right]\\
\left[231\right] & \left[231\right]\\
\left[312\right] & \left[312\right]\\
\left[213\right] & \left[213\right]\\
\left[132\right] & \left[132\right]\\
\left[321\right] & \left[321\right]
\end{array}\leftrightarrow\begin{array}{ccccccc}
\circ & e & \left[231\right] & \left[312\right] & \left[213\right] & \left[132\right] & \left[321\right]\\
e & e & \left[231\right] & \left[312\right] & \left[213\right] & \left[132\right] & \left[321\right]\\
\left[231\right] & \left[231\right]\\
\left[312\right] & \left[312\right]\\
\left[213\right] & \left[213\right]\\
\left[132\right] & \left[132\right]\\
\left[321\right] & \left[321\right]
\end{array}}
\]

\end_inset


\end_layout

\begin_layout Definition
cycle notation= cyclic notation
\begin_inset Formula 
\[
\begin{aligned}\begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix}= & \begin{pmatrix}1 & 2 & 3\\
\downarrow & \nearrow & \nnwarrow\downarrow\\
3 & 1 & 2
\end{pmatrix}=\left(1\rightarrow3\rightarrow2\rightarrow1\right)=\left(1321\right)=\left(132\right)\\
\begin{pmatrix}1 & 2 & 3\\
2 & 3 & 1
\end{pmatrix}= & \begin{pmatrix}1 & 2 & 3\\
\downarrow\nnearrow & \downarrow\nnearrow & \downarrow\\
2 & 3 & 1
\end{pmatrix}=\left(1\rightarrow2\rightarrow3\rightarrow1\right)=\left(1231\right)=\left(123\right)
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\begin{pmatrix}1 & 2 & 3\\
1 & 2 & 3
\end{pmatrix}= & \left(1\rightarrow1\right)\left(2\rightarrow2\right)\left(3\rightarrow3\right)=\left(11\right)\left(22\right)\left(33\right) & =\left(1\right)\left(2\right)\left(3\right)\\
\begin{pmatrix}1 & 2 & 3\\
2 & 3 & 1
\end{pmatrix}= & \left(1\rightarrow2\rightarrow3\rightarrow1\right)=\left(1231\right) & =\left(123\right) & =\left(231\right)=\left(312\right)\\
\begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix}= & \left(1\rightarrow3\rightarrow2\rightarrow1\right)=\left(1321\right) & =\left(132\right) & =\left(321\right)=\left(213\right)\\
\begin{pmatrix}1 & 2 & 3\\
2 & 1 & 3
\end{pmatrix}= & \left(1\rightarrow2\rightarrow1\right)\left(3\rightarrow3\right)=\left(121\right)\left(33\right) & =\left(12\right)\left(3\right) & =\left(21\right)\left(3\right)=\left(3\right)\left(21\right)=\left(3\right)\left(12\right)\\
\begin{pmatrix}1 & 2 & 3\\
1 & 3 & 2
\end{pmatrix}= & \left(1\rightarrow1\right)\left(2\rightarrow3\rightarrow2\right)=\left(11\right)\left(232\right) & =\left(1\right)\left(23\right) & =\left(1\right)\left(32\right)=\left(32\right)\left(1\right)=\left(23\right)\left(1\right)\\
\begin{pmatrix}1 & 2 & 3\\
3 & 2 & 1
\end{pmatrix}= & \left(1\rightarrow3\rightarrow1\right)\left(2\rightarrow2\right)=\left(131\right)\left(22\right) & =\left(13\right)\left(2\right) & =\left(31\right)\left(2\right)=\left(2\right)\left(31\right)=\left(2\right)\left(31\right)
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left(123\right)\left(321\right)= & \begin{pmatrix}1 & 2 & 3\\
2 & 3 & 1
\end{pmatrix}\begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix}\\
 & \qquad\parallel\\
= & \begin{pmatrix}3 & 1 & 2\\
1 & 2 & 3
\end{pmatrix}\begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix} & \begin{pmatrix}1 & 2 & 3\\
2 & 3 & 1
\end{pmatrix}=\begin{pmatrix}3 & 1 & 2\\
1 & 2 & 3
\end{pmatrix}\\
= & \begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix}\\
 & \begin{pmatrix}3 & 1 & 2\\
1 & 2 & 3
\end{pmatrix}=\begin{pmatrix}1 & 2 & 3\\
1 & 2 & 3
\end{pmatrix}=e & =e_{{\scriptscriptstyle S_{{\scriptscriptstyle 3}}}}\\
\left(123\right)\left(321\right)= & e\\
\\\left(321\right)^{{\scriptscriptstyle -1}}=\overline{\left(321\right)}= & \left(123\right) & \left(123\right)^{{\scriptscriptstyle -1}}=\overline{\left(123\right)}=\left(321\right)\\
\\\left(321\right)\left(123\right)= & \begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix}\begin{pmatrix}1 & 2 & 3\\
2 & 3 & 1
\end{pmatrix}\\
 & \qquad\parallel\\
= & \begin{pmatrix}2 & 3 & 1\\
1 & 2 & 3
\end{pmatrix}\begin{pmatrix}1 & 2 & 3\\
2 & 3 & 1
\end{pmatrix} & \begin{pmatrix}1 & 2 & 3\\
3 & 1 & 2
\end{pmatrix}=\begin{pmatrix}2 & 3 & 1\\
1 & 2 & 3
\end{pmatrix}\\
= & \begin{pmatrix}1 & 2 & 3\\
2 & 3 & 1
\end{pmatrix}\\
 & \begin{pmatrix}2 & 3 & 1\\
1 & 2 & 3
\end{pmatrix}=\begin{pmatrix}1 & 2 & 3\\
1 & 2 & 3
\end{pmatrix}=e & =e_{{\scriptscriptstyle S_{{\scriptscriptstyle 3}}}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\sigma= & c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}=\overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}=c_{{\scriptscriptstyle 1}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}=\overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{c_{{\scriptscriptstyle 1}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}} & c_{{\scriptscriptstyle i}}\cap c_{{\scriptscriptstyle j}}=\emptyset\\
= & \underset{c_{{\scriptscriptstyle 1}}}{\underbrace{c_{{\scriptscriptstyle 11}}c_{{\scriptscriptstyle 12}}\cdots c_{{\scriptscriptstyle 1n_{{\scriptscriptstyle 1}}}}}}\underset{c_{{\scriptscriptstyle 2}}}{\underbrace{c_{{\scriptscriptstyle 21}}c_{{\scriptscriptstyle 22}}\cdots c_{{\scriptscriptstyle 2n_{{\scriptscriptstyle 2}}}}}}\cdots\underset{c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}{\underbrace{c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}1}}c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}n_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}}}} & c_{{\scriptscriptstyle ij_{{\scriptscriptstyle 1}}}}\cap c_{{\scriptscriptstyle ij_{{\scriptscriptstyle 2}}}}=\emptyset\\
= & \underset{c_{{\scriptscriptstyle 1}}}{\underbrace{c_{{\scriptscriptstyle 11}}c_{{\scriptscriptstyle 12}}\cdots c_{{\scriptscriptstyle 1n_{{\scriptscriptstyle 1}}}}}}\cdots\underset{c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}{\underbrace{c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}1}}c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}n_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}}}}\\
= & \overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{c_{{\scriptscriptstyle 1}}}{\underbrace{c_{{\scriptscriptstyle 11}}c_{{\scriptscriptstyle 12}}\cdots c_{{\scriptscriptstyle 1n_{{\scriptscriptstyle 1}}}}}}\cdots\underset{c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}{\underbrace{c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}1}}c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}n_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}}}}}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\sum\limits_{i=1}^{n_{{\scriptscriptstyle \sigma}}}n_{{\scriptscriptstyle i}}=n\qquad\forall n\in\mathbb{N},\forall\sigma\in S_{{\scriptscriptstyle n}}
\]

\end_inset


\begin_inset Formula $\begin{aligned}\left(ab\right)\left(bcd\right)= & \left(abcd\right) & \left(a-b\right)\left(b-c\right)\left(c-d\right)\left(d-a\right)\ne0\end{aligned}
$
\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
\begin{aligned}\left(ab\right)\left(bcd\right)= & \begin{pmatrix}a & b\\
b & a
\end{pmatrix}\begin{pmatrix}b & c & d\\
c & d & b
\end{pmatrix}\\
= & \begin{pmatrix}a & b\\
b & a
\end{pmatrix}\begin{pmatrix}a & b & c & d\\
a & c & d & b
\end{pmatrix} & \begin{pmatrix}b & c & d\\
c & d & b
\end{pmatrix}=\begin{pmatrix}a & b & c & d\\
a & c & d & b
\end{pmatrix}\\
= & \begin{pmatrix}a & b\\
b & a
\end{pmatrix}\begin{pmatrix}a & d & b & c\\
a & b & c & d
\end{pmatrix} & \begin{pmatrix}a & b & c & d\\
a & c & d & b
\end{pmatrix}=\begin{pmatrix}a & d & b & c\\
a & b & c & d
\end{pmatrix}\\
 & \begin{pmatrix}a & d & b & c\\
a & b & c & d
\end{pmatrix}\\
= & \begin{pmatrix}a & b\\
b & a
\end{pmatrix}\qquad=\begin{pmatrix}a & d & b & c\\
b & a & c & d
\end{pmatrix}=\begin{pmatrix}a & b & c & d\\
b & c & d & a
\end{pmatrix} & =\left(abcd\right)
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
swap
\begin_inset Formula 
\[
\begin{aligned}\left(ab\right)\left(bc\right)\left(ca\right)= & \begin{pmatrix}a & b\\
b & a
\end{pmatrix}\begin{pmatrix}b & c\\
c & b
\end{pmatrix}\begin{pmatrix}c & a\\
a & c
\end{pmatrix}\\
= & \begin{pmatrix}c & a & b\\
c & b & a
\end{pmatrix}\begin{pmatrix}b & a & c\\
c & a & b
\end{pmatrix}\begin{pmatrix}b & c & a\\
b & a & c
\end{pmatrix}\\
 & \begin{pmatrix}b & c & a\\
b & a & c
\end{pmatrix}\\
 & \begin{pmatrix}b & a & c\\
c & a & b
\end{pmatrix}\\
= & \begin{pmatrix}c & a & b\\
c & b & a
\end{pmatrix}=\begin{pmatrix}b & c & a\\
c & b & a
\end{pmatrix}=\begin{pmatrix}a & b & c\\
a & c & b
\end{pmatrix}=\left(bc\right)
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left(ab\right)\left(bc\right)= & \begin{pmatrix}a & b\\
b & a
\end{pmatrix}\begin{pmatrix}b & c\\
c & b
\end{pmatrix}\\
= & \begin{pmatrix}c & a & b\\
c & b & a
\end{pmatrix}\begin{pmatrix}b & a & c\\
c & a & b
\end{pmatrix}\\
 & \begin{pmatrix}b & a & c\\
c & a & b
\end{pmatrix}\\
= & \begin{pmatrix}c & a & b\\
c & b & a
\end{pmatrix}=\begin{pmatrix}b & a & c\\
c & b & a
\end{pmatrix}=\begin{pmatrix}a & b & c\\
b & c & a
\end{pmatrix}=\left(abc\right)
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\sigma= & s_{{\scriptscriptstyle 1}}s_{{\scriptscriptstyle 2}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}=\overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{s_{{\scriptscriptstyle 1}}s_{{\scriptscriptstyle 2}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}}=s_{{\scriptscriptstyle 1}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}=\overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{s_{{\scriptscriptstyle 1}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}} & s_{{\scriptscriptstyle i}}\cap s_{{\scriptscriptstyle i+1}}=\left\{ s_{{\scriptscriptstyle \left(i\right)2}}\right\} \\
= & \underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\underset{s_{{\scriptscriptstyle 2}}}{\underbrace{\left(s_{{\scriptscriptstyle 21}}s_{{\scriptscriptstyle 22}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}} & s_{{\scriptscriptstyle \left(i\right)2}}=s_{{\scriptscriptstyle \left(i+1\right)1}}\\
= & \underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}}\\
= & \overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}}}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\sigma\begin{cases}
\text{is an even permutation} & N_{{\scriptscriptstyle \sigma}}\in2\mathbb{N}-2\\
\text{is an odd permutation} & N_{{\scriptscriptstyle \sigma}}\in2\mathbb{N}-1
\end{cases}\Leftrightarrow & \sigma\begin{cases}
\text{even} & N_{{\scriptscriptstyle \sigma}}\in2\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
\text{odd} & N_{{\scriptscriptstyle \sigma}}\in2\mathbb{N}-1
\end{cases} & \forall\sigma\in S_{{\scriptscriptstyle n}}\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Definition
alternating group
\begin_inset Formula 
\[
A_{{\scriptscriptstyle n}}=\left\{ \sigma\middle|\begin{aligned}\sigma\in & S_{{\scriptscriptstyle n}}\\
N_{{\scriptscriptstyle \sigma}}\in & 2\mathbb{Z}_{{\scriptscriptstyle \ge0}}
\end{aligned}
\right\} 
\]

\end_inset


\end_layout

\begin_layout Subsection
dihedral group
\end_layout

\begin_layout Standard
\begin_inset Formula $D_{{\scriptscriptstyle 3}}$
\end_inset


\begin_inset Formula 
\[
\begin{array}{ccccccccccccc}
1 & : & \left\langle \cos0,\sin0\right\rangle  & = & \left\langle \cos\dfrac{2\pi}{3}0,\sin\dfrac{2\pi}{3}0\right\rangle  & = & \left\langle 1,0\right\rangle  & = & \begin{bmatrix}1\\
0
\end{bmatrix} & : & \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}0}} & = & 1+\mathrm{i}0\\
2 & : & \left\langle \cos\dfrac{2\pi}{3},\sin\dfrac{2\pi}{3}\right\rangle  & = & \left\langle \cos\dfrac{2\pi}{3}1,\sin\dfrac{2\pi}{3}1\right\rangle  & = & \left\langle \dfrac{-1}{2},\dfrac{\sqrt{3}}{2}\right\rangle  & = & \begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix} & : & \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}1}} & = & \dfrac{-1}{2}+\mathrm{i}\dfrac{\sqrt{3}}{2}\\
3 & : & \left\langle \cos\dfrac{4\pi}{3},\sin\dfrac{4\pi}{3}\right\rangle  & = & \left\langle \cos\dfrac{2\pi}{3}2,\sin\dfrac{2\pi}{3}2\right\rangle  & = & \left\langle \dfrac{-1}{2},\dfrac{-\sqrt{3}}{2}\right\rangle  & = & \begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix} & : & \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}2}} & = & \dfrac{-1}{2}+\mathrm{i}\dfrac{-\sqrt{3}}{2}
\end{array}
\]

\end_inset


\begin_inset Formula 
\[
\recolor{\begin{aligned}\left(\right)= & \left(1\right)\left(2\right)\left(3\right) & : & \begin{cases}
\begin{bmatrix}1\\
0
\end{bmatrix}=\begin{bmatrix}1 & 0\\
0 & 1
\end{bmatrix}\begin{bmatrix}1\\
0
\end{bmatrix} & \left(1\right)\\
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}1 & 0\\
0 & 1
\end{bmatrix}\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix} & \left(2\right)\\
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}1 & 0\\
0 & 1
\end{bmatrix}\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix} & \left(3\right)
\end{cases} & \begin{bmatrix}1 & 0\\
0 & 1
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}0 & -\sin\dfrac{2\pi}{3}0\\
\sin\dfrac{2\pi}{3}0 & \cos\dfrac{2\pi}{3}0
\end{bmatrix}\\
\left(123\right)= & \left(12\right)\left(23\right) & : & \begin{cases}
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix}\begin{bmatrix}1\\
0
\end{bmatrix} & \left(12\right)\\
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}\dfrac{-1}{2} & \dfrac{-\sqrt{3}}{2}\\
\dfrac{\sqrt{3}}{2} & \dfrac{-1}{2}
\end{bmatrix}\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix} & \left(23\right)
\end{cases} & \begin{bmatrix}\dfrac{-1}{2} & \dfrac{-\sqrt{3}}{2}\\
\dfrac{\sqrt{3}}{2} & \dfrac{-1}{2}
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}1 & -\sin\dfrac{2\pi}{3}1\\
\sin\dfrac{2\pi}{3}1 & \cos\dfrac{2\pi}{3}1
\end{bmatrix}\\
\left(132\right)= & \left(13\right)\left(32\right) & : & \begin{cases}
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix}\begin{bmatrix}1\\
0
\end{bmatrix} & \left(13\right)\\
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}\dfrac{-1}{2} & \dfrac{\sqrt{3}}{2}\\
\dfrac{-\sqrt{3}}{2} & \dfrac{-1}{2}
\end{bmatrix}\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix} & \left(32\right)
\end{cases} & \begin{bmatrix}\dfrac{-1}{2} & \dfrac{\sqrt{3}}{2}\\
\dfrac{-\sqrt{3}}{2} & \dfrac{-1}{2}
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}2 & -\sin\dfrac{2\pi}{3}2\\
\sin\dfrac{2\pi}{3}2 & \cos\dfrac{2\pi}{3}2
\end{bmatrix}\\
\left(12\right)= & \left(12\right)\left(3\right) & : & \begin{cases}
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}\dfrac{-1}{2} & 0\\
\dfrac{\sqrt{3}}{2} & 0
\end{bmatrix}\begin{bmatrix}1\\
0
\end{bmatrix} & \left(12\right)\\
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}\dfrac{-1}{2} & \dfrac{\sqrt{3}}{2}\\
\dfrac{\sqrt{3}}{2} & \dfrac{1}{2}
\end{bmatrix}\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix} & \left(3\right)
\end{cases} & \begin{bmatrix}\dfrac{-1}{2} & \dfrac{\sqrt{3}}{2}\\
\dfrac{\sqrt{3}}{2} & \dfrac{1}{2}
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}1 & \sin\dfrac{2\pi}{3}1\\
\sin\dfrac{2\pi}{3}1 & -\cos\dfrac{2\pi}{3}1
\end{bmatrix}\\
\left(23\right)= & \left(23\right)\left(1\right) & : & \begin{cases}
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}1 & 0\\
0 & -1
\end{bmatrix}\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix} & \left(23\right)\\
\begin{bmatrix}1\\
0
\end{bmatrix}=\begin{bmatrix}1 & 0\\
0 & -1
\end{bmatrix}\begin{bmatrix}1\\
0
\end{bmatrix} & \left(1\right)
\end{cases} & \begin{bmatrix}1 & 0\\
0 & -1
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}0 & \sin\dfrac{2\pi}{3}0\\
\sin\dfrac{2\pi}{3}0 & -\cos\dfrac{2\pi}{3}0
\end{bmatrix}\\
\left(31\right)= & \left(31\right)\left(2\right) & : & \begin{cases}
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{-\sqrt{3}}{2}
\end{bmatrix}\begin{bmatrix}1\\
0
\end{bmatrix} & \left(31\right)\\
\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix}=\begin{bmatrix}\dfrac{-1}{2} & \dfrac{-\sqrt{3}}{2}\\
\dfrac{-\sqrt{3}}{2} & \dfrac{1}{2}
\end{bmatrix}\begin{bmatrix}\dfrac{-1}{2}\\
\dfrac{\sqrt{3}}{2}
\end{bmatrix} & \left(2\right)
\end{cases} & \begin{bmatrix}\dfrac{-1}{2} & \dfrac{-\sqrt{3}}{2}\\
\dfrac{-\sqrt{3}}{2} & \dfrac{1}{2}
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}2 & \sin\dfrac{2\pi}{3}2\\
\sin\dfrac{2\pi}{3}2 & -\cos\dfrac{2\pi}{3}2
\end{bmatrix}
\end{aligned}
}
\]

\end_inset


\begin_inset Formula 
\[
\recolor{\begin{array}{cccccc}
D_{{\scriptscriptstyle 3}}=\{ & \left(\right), & \left(123\right), & \left(132\right),\\
 & \left(12\right), & \left(23\right), & \left(31\right), & \} & =S_{{\scriptscriptstyle 3}}\\
\quad=\{ & \left[123\right], & \left[231\right], & \left[312\right],\\
 & \left[213\right], & \left[132\right], & \left[321\right], & \} & =S_{{\scriptscriptstyle 3}}\\
\quad=\{ & \begin{bmatrix}1 & 0\\
0 & 1
\end{bmatrix}, & \begin{bmatrix}\dfrac{-1}{2} & \dfrac{-\sqrt{3}}{2}\\
\dfrac{\sqrt{3}}{2} & \dfrac{-1}{2}
\end{bmatrix}, & \begin{bmatrix}\dfrac{-1}{2} & \dfrac{\sqrt{3}}{2}\\
\dfrac{-\sqrt{3}}{2} & \dfrac{-1}{2}
\end{bmatrix},\\
 & \begin{bmatrix}\dfrac{-1}{2} & \dfrac{\sqrt{3}}{2}\\
\dfrac{\sqrt{3}}{2} & \dfrac{1}{2}
\end{bmatrix}, & \begin{bmatrix}1 & 0\\
0 & -1
\end{bmatrix}, & \begin{bmatrix}\dfrac{-1}{2} & \dfrac{-\sqrt{3}}{2}\\
\dfrac{-\sqrt{3}}{2} & \dfrac{1}{2}
\end{bmatrix} & \} & \text{matrix representation}\\
\quad=\{ & \begin{bmatrix}\cos0 & -\sin0\\
\sin0 & \cos0
\end{bmatrix}, & \begin{bmatrix}\cos\dfrac{2\pi}{3} & -\sin\dfrac{2\pi}{3}\\
\sin\dfrac{2\pi}{3} & \cos\dfrac{2\pi}{3}
\end{bmatrix}, & \begin{bmatrix}\cos\dfrac{4\pi}{3} & -\sin\dfrac{4\pi}{3}\\
\sin\dfrac{4\pi}{3} & \cos\dfrac{4\pi}{3}
\end{bmatrix}, &  & \rho_{{\scriptscriptstyle k}}=\begin{bmatrix}\cos\dfrac{2\pi}{3}k & -\sin\dfrac{2\pi}{3}k\\
\sin\dfrac{2\pi}{3}k & \cos\dfrac{2\pi}{3}k
\end{bmatrix}\\
 & \begin{bmatrix}\cos\dfrac{2\pi}{3} & \sin\dfrac{2\pi}{3}\\
\sin\dfrac{2\pi}{3} & -\cos\dfrac{2\pi}{3}
\end{bmatrix}, & \begin{bmatrix}\cos0 & \sin0\\
\sin0 & -\cos0
\end{bmatrix}, & \begin{bmatrix}\cos\dfrac{4\pi}{3} & \sin\dfrac{4\pi}{3}\\
\sin\dfrac{4\pi}{3} & -\cos\dfrac{4\pi}{3}
\end{bmatrix} & \} & \pi_{{\scriptscriptstyle k}}=\begin{bmatrix}\cos\dfrac{2\pi}{3}k & \sin\dfrac{2\pi}{3}k\\
\sin\dfrac{2\pi}{3}k & -\cos\dfrac{2\pi}{3}k
\end{bmatrix}\\
\quad=\{ & \rho_{{\scriptscriptstyle 0}}, & \rho_{{\scriptscriptstyle 1}}, & \rho_{{\scriptscriptstyle 2}}, &  & \text{rotations}\\
 & \pi_{{\scriptscriptstyle 0}}, & \pi_{{\scriptscriptstyle 1}}, & \pi_{{\scriptscriptstyle 2}} & \} & \text{reflections}\\
\quad=\{ & \rho_{{\scriptscriptstyle k}} & | & \rho_{{\scriptscriptstyle k+3}}=\rho_{{\scriptscriptstyle k}}\\
 & \pi_{{\scriptscriptstyle k}} & | & \pi_{{\scriptscriptstyle k+3}}=\pi_{{\scriptscriptstyle k}} & \}
\end{array}}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\cos\alpha\cos\beta= & \frac{+\cos\left(\alpha+\beta\right)+\cos\left(\alpha-\beta\right)}{2}\\
\sin\alpha\sin\beta= & \frac{-\cos\left(\alpha+\beta\right)+\cos\left(\alpha-\beta\right)}{2}\\
\sin\alpha\cos\beta= & \frac{+\sin\left(\alpha+\beta\right)+\sin\left(\alpha-\beta\right)}{2}\\
\cos\alpha\sin\beta= & \frac{+\sin\left(\alpha+\beta\right)-\sin\left(\alpha-\beta\right)}{2}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\rho_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}= & \begin{bmatrix}\cos\dfrac{2\pi}{3}i & -\sin\dfrac{2\pi}{3}i\\
\sin\dfrac{2\pi}{3}i & \cos\dfrac{2\pi}{3}i
\end{bmatrix}\begin{bmatrix}\cos\dfrac{2\pi}{3}j & -\sin\dfrac{2\pi}{3}j\\
\sin\dfrac{2\pi}{3}j & \cos\dfrac{2\pi}{3}j
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}\left(i+j\right) & -\sin\dfrac{2\pi}{3}\left(i+j\right)\\
\sin\dfrac{2\pi}{3}\left(i+j\right) & \cos\dfrac{2\pi}{3}\left(i+j\right)
\end{bmatrix}=\rho_{{\scriptscriptstyle i+j}} & \rho_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}=\rho_{{\scriptscriptstyle i+j}}\\
\rho_{{\scriptscriptstyle i}}\pi_{{\scriptscriptstyle j}}= & \begin{bmatrix}\cos\dfrac{2\pi}{3}i & -\sin\dfrac{2\pi}{3}i\\
\sin\dfrac{2\pi}{3}i & \cos\dfrac{2\pi}{3}i
\end{bmatrix}\begin{bmatrix}\cos\dfrac{2\pi}{3}j & \sin\dfrac{2\pi}{3}j\\
\sin\dfrac{2\pi}{3}j & -\cos\dfrac{2\pi}{3}j
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}\left(i+j\right) & \sin\dfrac{2\pi}{3}\left(i+j\right)\\
\sin\dfrac{2\pi}{3}\left(i+j\right) & -\cos\dfrac{2\pi}{3}\left(i+j\right)
\end{bmatrix}=\pi_{{\scriptscriptstyle i+j}} & \rho_{{\scriptscriptstyle i}}\pi_{{\scriptscriptstyle j}}=\pi_{{\scriptscriptstyle i+j}}\\
\pi_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}= & \begin{bmatrix}\cos\dfrac{2\pi}{3}i & \sin\dfrac{2\pi}{3}i\\
\sin\dfrac{2\pi}{3}i & -\cos\dfrac{2\pi}{3}i
\end{bmatrix}\begin{bmatrix}\cos\dfrac{2\pi}{3}j & -\sin\dfrac{2\pi}{3}j\\
\sin\dfrac{2\pi}{3}j & \cos\dfrac{2\pi}{3}j
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}\left(i-j\right) & \sin\dfrac{2\pi}{3}\left(i-j\right)\\
\sin\dfrac{2\pi}{3}\left(i-j\right) & -\cos\dfrac{2\pi}{3}\left(i-j\right)
\end{bmatrix}=\pi_{{\scriptscriptstyle i-j}} & \pi_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}=\pi_{{\scriptscriptstyle i-j}}\\
\pi_{{\scriptscriptstyle i}}\pi_{{\scriptscriptstyle j}}= & \begin{bmatrix}\cos\dfrac{2\pi}{3}i & \sin\dfrac{2\pi}{3}i\\
\sin\dfrac{2\pi}{3}i & -\cos\dfrac{2\pi}{3}i
\end{bmatrix}\begin{bmatrix}\cos\dfrac{2\pi}{3}j & \sin\dfrac{2\pi}{3}j\\
\sin\dfrac{2\pi}{3}j & -\cos\dfrac{2\pi}{3}j
\end{bmatrix}=\begin{bmatrix}\cos\dfrac{2\pi}{3}\left(i-j\right) & -\sin\dfrac{2\pi}{3}\left(i-j\right)\\
\sin\dfrac{2\pi}{3}\left(i-j\right) & \cos\dfrac{2\pi}{3}\left(i-j\right)
\end{bmatrix}=\rho_{{\scriptscriptstyle i-j}} & \pi_{{\scriptscriptstyle i}}\pi_{{\scriptscriptstyle j}}=\rho_{{\scriptscriptstyle i-j}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\rho_{{\scriptscriptstyle k+3}}= & \rho_{{\scriptscriptstyle k}}\\
\pi_{{\scriptscriptstyle k+3}}= & \pi_{{\scriptscriptstyle k}}\\
\rho_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}= & \rho_{{\scriptscriptstyle i+j}}\\
\rho_{{\scriptscriptstyle i}}\pi_{{\scriptscriptstyle j}}= & \pi_{{\scriptscriptstyle i+j}}\\
\pi_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}= & \pi_{{\scriptscriptstyle i-j}}\\
\pi_{{\scriptscriptstyle i}}\pi_{{\scriptscriptstyle j}}= & \rho_{{\scriptscriptstyle i-j}}
\end{aligned}
\]

