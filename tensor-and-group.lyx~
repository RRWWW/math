\begin_layout Part
group theory
\end_layout

\begin_layout Section
basic definition
\end_layout

\begin_layout Definition
group
\end_layout

\begin_layout Definition
\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group}\\
\Updownarrow\\
G=\left(G,\cdot\right)=\left(G,\cdot_{{\scriptscriptstyle G}}\right)= & \left\{ g\middle|\begin{aligned}g_{{\scriptscriptstyle 1}}\cdot g_{{\scriptscriptstyle 2}}= & g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\in G & \forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in G & \quad\left(c\right)\cdot_{{\scriptscriptstyle G}}\text{closure}\\
g_{{\scriptscriptstyle 1}}\left(g_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 3}}\right)= & \left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)g_{{\scriptscriptstyle 3}}=g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 3}} & \forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}},g_{{\scriptscriptstyle 3}}\in G & \quad\left(a\right)\cdot_{{\scriptscriptstyle G}}\text{associativity}\\
e\cdot g=eg= & g=ge=g\cdot e & \exists e=e_{{\scriptscriptstyle G}}\in G,\forall g\in G & \quad\left(id\right)\text{identity element}\\
\overline{g}\cdot g=\bar{g}g= & e=g\bar{g}=g\cdot\overline{g} & \forall g\in G,\exists\overline{g}\in G & \quad\left(in\right)\text{inverse element}
\end{aligned}
\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
\begin{aligned}\forall g\in & G\\
g\ne e\in & G
\end{aligned}
\Rightarrow\forall\widetilde{g}\in G\left[g\widetilde{g}\ne\widetilde{g}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
\begin{aligned}\forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in & G\\
g_{{\scriptscriptstyle 1}}\ne g_{{\scriptscriptstyle 2}}
\end{aligned}
\Rightarrow\forall g\in G\left[g_{{\scriptscriptstyle 1}}g\ne g_{{\scriptscriptstyle 2}}g\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
rearrangement theorem
\begin_inset Formula 
\[
\forall g\in G\left[\left\{ g\widetilde{g}\middle|\widetilde{g}\in G\right\} =G\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Proof
proof idea 
\begin_inset Formula $f=g\left(\overline{g}f\right)=gg^{{\scriptscriptstyle -1}}f=ef=f$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\forall g\in G,\exists\overline{g}\in G\Biggl[\overline{g}g=e=g\overline{g} & \Biggr]\\
\Downarrow\\
\forall f\in G\Biggl[f=ef\overset{e=g\overline{g}}{=} & \left(g\overline{g}\right)f\overset{\left(a\right)}{=}g\left(\overline{g}f\right)\Biggr]\Rightarrow & \forall f\in G\left[f=g\left(\overline{g}f\right)\right]\overset{\left(c\right)\overline{g}f\in G}{\Rightarrow} & f\in\left\{ g\widetilde{g}\middle|\widetilde{g}\in G\right\} \\
 &  & \Downarrow\\
 &  & \forall f\in G & \left[f\in\left\{ g\widetilde{g}\middle|\widetilde{g}\in G\right\} \right]\\
 &  & \Downarrow\\
 &  & G\subseteq & \left\{ g\widetilde{g}\middle|\widetilde{g}\in G\right\} \\
 &  & \left\{ g\widetilde{g}\middle|\forall\widetilde{g}\in G\right\} \subseteq & G\because\left(c\right)\cdot_{{\scriptscriptstyle G}}\text{closure}\\
 &  & \Downarrow\\
 &  & G= & \left\{ g\widetilde{g}\middle|\widetilde{g}\in G\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Corollary
\begin_inset Formula 
\[
\Biggl\{\begin{aligned}\forall g\in G\left[g\in\left\{ g\widetilde{g}\middle|\widetilde{g}\in G\right\} \right] &  & \left(l\right)\text{lossless}=\text{complete}\\
\begin{aligned}\forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in G & =\left\{ g\widetilde{g}\middle|\widetilde{g}\in G\right\} \\
g_{{\scriptscriptstyle 1}}\ne g_{{\scriptscriptstyle 2}}
\end{aligned}
\Rightarrow & g_{{\scriptscriptstyle 1}}\widetilde{g}\ne g_{{\scriptscriptstyle 2}}\widetilde{g} & \left(r\right)\text{repeatless}=\text{rearrange}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
exponentiation or power
\begin_inset Formula 
\[
\begin{aligned}g^{n}= & \overset{n}{\overbrace{g\cdot_{{\scriptscriptstyle G}}g\cdot_{{\scriptscriptstyle G}}\cdots\cdot_{{\scriptscriptstyle G}}g}}=\overset{n}{\overbrace{g\cdot g\cdot\cdots\cdot g}}=\overset{n}{\overbrace{g\cdot\cdots\cdot g}}=\overset{n}{\overbrace{g\cdots g}} & n\in\mathbb{N}\\
g^{-n}=\overline{g}^{n}= & \overset{n}{\overbrace{\overline{g}\cdot_{{\scriptscriptstyle G}}\overline{g}\cdot_{{\scriptscriptstyle G}}\cdots\cdot_{{\scriptscriptstyle G}}\overline{g}}}=\overset{n}{\overbrace{\overline{g}\cdot\overline{g}\cdot\cdots\cdot\overline{g}}}=\overset{n}{\overbrace{\overline{g}\cdot\cdots\cdot\overline{g}}}=\overset{n}{\overbrace{\overline{g}\cdots\overline{g}}} & n\in\mathbb{N}\\
 &  & \overline{g}g=e=g\overline{g}\\
g^{0}= & g^{n}\overline{g}^{n}=\overline{g}^{n}g^{n}=\left(\overline{g}^{n-1}\overline{g}\right)\left(gg^{n-1}\right)=\overline{g}^{n-1}\left(\overline{g}g\right)g^{n-1}=\overline{g}^{n-1}\left(e\right)g^{n-1}=\cdots=e\\
g^{0}= & e\\
g^{k}= & \begin{cases}
g^{k} & k>0\\
e & k=0\\
\overline{g}^{\left|k\right|}=g^{-\left|k\right|} & k<0
\end{cases} & k\in\mathbb{Z}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
infinite group vs.
 finite group
\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group} &  &  & G & \text{ is a group}\\
\exists H\subseteq G\Biggl[\left|G\right| & =\left|H\right|\Biggr] &  &  & \not\exists H\subseteq G\Biggl[\left|G\right| & =\left|H\right|\Biggr]\\
\Updownarrow &  & \leftarrow & \rightarrow & \Updownarrow\\
G & \text{ is an infinite group} &  &  & G & \text{ is a finite group}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
discrete group vs.
 Lie group
\end_layout

\begin_layout Definition
\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group} &  &  & G & \text{ is a group}\\
\exists H\subseteq\mathbb{Z}\Biggl[\left|G\right| & =\left|H\right|\Biggr] &  &  & \not\exists H\subseteq\mathbb{Z}\Biggl[\left|G\right| & =\left|H\right|\Biggr]\\
\Updownarrow &  & \leftarrow & \rightarrow & \Updownarrow\\
G & \text{ is a discrete group} &  &  & G & \text{ is a Lie group}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
commutative group = Abelian group
\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group} &  &  & A & \text{ is a group}\\
\forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in G & \left[g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}=g_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 1}}\right] &  &  & \forall a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}}\in A & \left[a_{{\scriptscriptstyle 1}}a_{{\scriptscriptstyle 2}}=a_{{\scriptscriptstyle 2}}a_{{\scriptscriptstyle 1}}\right]\\
\Updownarrow &  &  &  & \Updownarrow\\
G & \text{ is a commutative group} &  &  & A & \text{ is a commutative group}\\
\Updownarrow &  &  &  & \Updownarrow\\
G & \text{ is an Abelian group} &  &  & A & \text{ is an Abelian group}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
trivial group
\begin_inset Formula 
\[
\begin{aligned}\left\{ e\right\} = & \left\{ e_{{\scriptscriptstyle G}}\right\} =\left\{ g^{0}\right\}  & g\in G=\left(G,\cdot\right)=\left(G,\cdot_{{\scriptscriptstyle G}}\right)\text{ is a group}\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Section
finite group
\end_layout

\begin_layout Subsection
cyclic group
\end_layout

\begin_layout Definition
cyclic group
\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle n}}=\left(\mathbb{Z}_{{\scriptscriptstyle n}},+\right)=\left(\mathbb{Z}_{{\scriptscriptstyle n}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)\overset{\text{def.}}{=} & \left\{ 0,1,2,\cdots,n-1\right\}  &  &  & n\in\mathbb{N}\\
= & \left\{ 0,1,\cdots,n-1\right\}  & = & \left\{ \overset{n}{\overbrace{0,1,\cdots,n-1}}\right\} \\
= & \left\{ 0,\cdots,n-1\right\}  & = & \left\{ \overset{n}{\overbrace{0,\cdots,n-1}}\right\} 
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in\mathbb{Z}_{{\scriptscriptstyle n}}\left[\begin{aligned}g_{{\scriptscriptstyle 1}}+g_{{\scriptscriptstyle 2}}=g_{{\scriptscriptstyle 1}}+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}g_{{\scriptscriptstyle 2}}\overset{\text{def.}}{=} & \left(g_{{\scriptscriptstyle 1}}+_{{\scriptscriptstyle \mathbb{Z}}}g_{{\scriptscriptstyle 2}}\right)\mod n & \left(g_{{\scriptscriptstyle 1}}+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}g_{{\scriptscriptstyle 2}}\right)-\left(g_{{\scriptscriptstyle 1}}+_{{\scriptscriptstyle \mathbb{Z}}}g_{{\scriptscriptstyle 2}}\right)=n\cdot_{{\scriptscriptstyle \mathbb{Z}}}k=nk\quad k\in\mathbb{Z}\\
= & g_{{\scriptscriptstyle 1}}\%g_{{\scriptscriptstyle 2}} & \text{some programming language}\\
\equiv & \left(g_{{\scriptscriptstyle 1}}+_{{\scriptscriptstyle \mathbb{Z}}}g_{{\scriptscriptstyle 2}}\right)\mod n & \left(g_{{\scriptscriptstyle 1}}+_{{\scriptscriptstyle \mathbb{Z}}}g_{{\scriptscriptstyle 2}}\right)\mod n<n\\
= & r & \mod{\text{ is integer modular arithmetic or modulus}}\\
 &  & g_{{\scriptscriptstyle 1}}+_{{\scriptscriptstyle \mathbb{Z}}}g_{{\scriptscriptstyle 2}}=nk+r\quad\mathbb{Z}\ni r<n
\end{aligned}
\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 2}}$
\end_inset


\begin_inset Formula 
\begin{align*}
\mathbb{Z}_{{\scriptscriptstyle 2}}=\left(\mathbb{Z}_{{\scriptscriptstyle 2}},+\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 2}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)= & \left\{ 0,1\right\}  & \mathbb{Z}_{{\scriptscriptstyle n}}=\left(\mathbb{Z}_{{\scriptscriptstyle n}},+\right)=\left(\mathbb{Z}_{{\scriptscriptstyle n}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right),2=n\in\mathbb{N}\\
0+0= & \left(0\mod 2\right)=0\\
0+1= & \left(1\mod 2\right)=1\\
1+0= & \left(1\mod 2\right)=1\\
1+1= & \left(2\mod 2\right)=0
\end{align*}

\end_inset


\begin_inset Formula 
\[
\begin{array}{ccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 2}}}} & 0 & 1\\
0 & 0 & 1\\
1 & 1 & 0
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 3}}$
\end_inset


\begin_inset Formula 
\begin{align*}
\mathbb{Z}_{{\scriptscriptstyle 3}}=\left(\mathbb{Z}_{{\scriptscriptstyle 3}},+\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 3}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)= & \left\{ 0,1,2\right\}  & \mathbb{Z}_{{\scriptscriptstyle n}}=\left(\mathbb{Z}_{{\scriptscriptstyle n}},+\right)=\left(\mathbb{Z}_{{\scriptscriptstyle n}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right),3=n\in\mathbb{N}
\end{align*}

\end_inset


\begin_inset Formula 
\[
\begin{array}{cccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}} & 0 & 1 & 2\\
0 & \left(0\mod 3\right) & \left(1\mod 3\right) & \left(2\mod 3\right)\\
1 & \left(1\mod 3\right) & \left(2\mod 3\right) & \left(3\mod 3\right)\\
2 & \left(2\mod 3\right) & \left(3\mod 3\right) & \left(4\mod 3\right)
\end{array}\rightarrow\begin{array}{cccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 3}}}} & 0 & 1 & 2\\
0 & 0 & 1 & 2\\
1 & 1 & 2 & 0\\
2 & 2 & 0 & 1
\end{array}
\]

\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
\forall n\in\mathbb{N}\left[\begin{aligned}+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\text{ commutative} & \Rightarrow & \mathbb{Z}_{{\scriptscriptstyle n}}\text{ is an Abelian group}\\
 &  & \Updownarrow\\
 &  & \mathbb{Z}_{{\scriptscriptstyle n}}\text{ is a commutative group}
\end{aligned}
\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
complex multiplication
\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}k_{{\scriptscriptstyle 1}}}}\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}k_{{\scriptscriptstyle 2}}}}=\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}k_{{\scriptscriptstyle 1}}}}\cdot_{{\scriptscriptstyle \mathbb{C}}}\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}k_{{\scriptscriptstyle 2}}}}=\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(k_{{\scriptscriptstyle 1}}+k_{{\scriptscriptstyle 2}}\right)\mod n}}\quad\begin{aligned}\forall k_{{\scriptscriptstyle 1}},k_{{\scriptscriptstyle 2}}\in\mathbb{Z}\\
\left|k_{{\scriptscriptstyle 1}}\right|,\left|k_{{\scriptscriptstyle 2}}\right|<n\in\mathbb{N}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle n}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle n}}= & \left(\mathbb{Z}_{{\scriptscriptstyle n}},+\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle n}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right) & = & \left\{ 0,1,2,\cdots,n-1\right\}  & n\in\mathbb{N}\\
\mathbb{Z}_{{\scriptscriptstyle n}}= & \left(\mathbb{Z}_{{\scriptscriptstyle n}},\cdot\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle n}},\cdot_{{\scriptscriptstyle \mathbb{C}}}\right) & = & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}1}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}2}},\cdots,\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(n-1\right)}}\right\}  & \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(nk\right)}}=1\quad\forall k\in\mathbb{Z}\\
\mathbb{Z}_{{\scriptscriptstyle n}}= & \left(\mathbb{Z}_{{\scriptscriptstyle n}},\cdot\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle n}},\cdot_{{\scriptscriptstyle G}}\right) & = & \left\{ g^{0},g^{1},g^{2},\cdots,g^{n-1}\right\} \\
 &  &  &  & = & \left\{ e,g,g^{2},\cdots,g^{n-1}\right\}  & g^{n}=e\\
 &  &  &  & = & \left\{ g^{k}\middle|\begin{aligned}g^{n}= & g^{0}=e=e_{{\scriptscriptstyle G}}\in G\\
k\in & \mathbb{Z}
\end{aligned}
\right\}  & g\text{ is a generating element}\\
 &  &  &  & = & \left\langle g\right\rangle  & \text{or a generator of the group}\\
 &  &  & \mathbb{Z}_{{\scriptscriptstyle n}} & \overset{\text{e.g.}}{=} & \left\langle \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}}}\right\rangle 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 2}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},+\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right) & = & \left\{ 0,2-1\right\}  & = & \left\{ 0,1\right\} \\
\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{{\scriptscriptstyle \mathbb{C}}}\right) & = & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{2}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{2}\left(2-1\right)}}\right\} =\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\pi}}\right\}  & = & \left\{ 1,-1\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 3}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 3}},+\right) & = & \left(\mathbb{Z}_{3},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right) & = & \left\{ 0,1,3-1\right\}  & = & \left\{ 0,1,2\right\} \\
\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 3}},\cdot\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle 3}},\cdot_{{\scriptscriptstyle \mathbb{C}}}\right) & = & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}1}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}\left(3-1\right)}}\right\} =\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{4\pi}{3}}}\right\}  & = & \left\{ 1,\dfrac{-1}{2}+\mathrm{i}\dfrac{\sqrt{3}}{2},\dfrac{-1}{2}-\mathrm{i}\dfrac{\sqrt{3}}{2}\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 4}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 4}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 4}},+\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle 4}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right) & = & \left\{ 0,1,2,4-1\right\}  & = & \left\{ 0,1,2,3\right\} \\
\mathbb{Z}_{{\scriptscriptstyle 4}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot_{{\scriptscriptstyle \mathbb{C}}}\right) & = & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{4}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{4}1}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{4}2}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{4}\left(4-1\right)}}\right\} =\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{\pi}{2}}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\pi}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{3\pi}{2}}}\right\}  & = & \left\{ 1,\mathrm{i},-1,-\mathrm{i}\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Subsection
permutation group or symmetric group
\end_layout

\begin_layout Definition
permutation
\begin_inset Formula 
\[
\begin{aligned}N= & \left\{ 1,2,\cdots,n\right\} =\left\{ 1,\cdots,n\right\} =\left\{ \overset{n}{\overbrace{1,\cdots,n}}\right\}  & \text{finite set by }n\in\mathbb{N}\\
\sigma\in N^{N}\Leftrightarrow & \sigma:N\rightarrow N\Leftrightarrow N\overset{\sigma}{\rightarrow}N & \text{autofunction over }N & =\mathbb{N}_{{\scriptscriptstyle \le n\in\mathbb{N}}}=\mathbb{N}_{{\scriptscriptstyle \le n}}\\
\sigma\left(N\right)= & N & \sigma\left(N\right)\text{ range equals codomain }N & \Leftrightarrow\sigma\text{ is a permutation}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
permutation group = symmetric group
\begin_inset Formula 
\[
S_{{\scriptscriptstyle n}}=\left(S_{{\scriptscriptstyle n}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\right)=\left(S_{{\scriptscriptstyle n}},\circ\right)=\left\{ \sigma\middle|\begin{aligned}n\in & \mathbb{N}\\
N= & \left\{ \overset{n}{\overbrace{1,\cdots,n}}\right\} \\
\sigma\in & N^{N}\\
\sigma\left(N\right)= & N
\end{aligned}
\right\} =\left\{ \sigma\middle|\begin{aligned}n\in & \mathbb{N}\\
N= & \left\{ 1,\cdots,n\right\} \\
\sigma: & N\rightarrow N\\
\forall\sigma_{{\scriptscriptstyle i}},\sigma_{{\scriptscriptstyle j}}\in S_{{\scriptscriptstyle n}} & \left[\sigma_{{\scriptscriptstyle i}}\sigma_{{\scriptscriptstyle j}}=\sigma_{{\scriptscriptstyle i}}\circ\sigma_{{\scriptscriptstyle j}}\right]\\
\forall m_{{\scriptscriptstyle 1}},m_{{\scriptscriptstyle 2}}\in N & \Biggl[m_{{\scriptscriptstyle 1}}\ne m_{{\scriptscriptstyle 2}}\Leftrightarrow\sigma\left(m_{{\scriptscriptstyle 1}}\right)\ne\sigma\left(m_{{\scriptscriptstyle 2}}\right)\Biggr]
\end{aligned}
\right\} 
\]

\end_inset


\end_layout

\begin_layout Definition
\begin_inset Formula $S_{{\scriptscriptstyle 1}}$
\end_inset


\end_layout

\begin_layout Definition
\begin_inset Formula 
\[
\sigma\left(1\right)=1
\]

\end_inset


\begin_inset Formula 
\[
S_{{\scriptscriptstyle 1}}=\left\{ \sigma\right\} =\left\{ e\right\} =\left\{ e_{{\scriptscriptstyle S_{{\scriptscriptstyle 1}}}}\right\} =\left\{ \mathrm{id}\right\} 
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\sigma\sigma\left(1\right)= & \sigma\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma\left(1\right)=\sigma\circ\sigma\left(1\right)=\sigma\left(\sigma\left(1\right)\right)=\sigma\left(1\right)=1=\sigma\left(1\right)\\
\sigma\sigma\left(1\right)= & \sigma\left(1\right)\\
\sigma\sigma= & \sigma=\sigma\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma=\sigma\circ\sigma
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{array}{cccccccccccc}
S_{{\scriptscriptstyle 1}} & =\{ &  & \sigma &  &  & \}=\{ &  & \sigma_{{\scriptscriptstyle 1}} &  &  & \}\\
 &  & 1 & \rightarrow & 1 & =\sigma\left(1\right) &  & 1 & \rightarrow & 1 & =\sigma_{{\scriptscriptstyle 1}}\left(1\right)\\
 & =\{ & \begin{array}{c}
1\\
\downarrow\\
1
\end{array} &  &  &  & \}=\{ & \begin{pmatrix}1\\
1
\end{pmatrix} &  &  &  & \}
\end{array}
\]

\end_inset


\begin_inset Formula 
\[
\begin{array}{cc}
\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}} & \sigma\\
\sigma & \sigma
\end{array}\leftrightarrow\begin{array}{cc}
\circ & \sigma_{{\scriptscriptstyle 1}}\\
\sigma_{{\scriptscriptstyle 1}} & \sigma_{{\scriptscriptstyle 1}}
\end{array}\leftrightarrow\begin{array}{cc}
\circ & \sigma_{{\scriptscriptstyle 1}}\\
\sigma_{{\scriptscriptstyle 1}} & e
\end{array}\leftrightarrow\begin{array}{cc}
\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}} & \sigma_{{\scriptscriptstyle 1}}\\
\sigma_{{\scriptscriptstyle 1}} & e_{{\scriptscriptstyle S_{{\scriptscriptstyle 1}}}}
\end{array}\leftrightarrow\begin{array}{cc}
\circ & e\\
e & e
\end{array}
\]

\end_inset


\end_layout

\begin_layout Definition
\begin_inset Formula $S_{{\scriptscriptstyle 2}}$
\end_inset


\begin_inset Formula 
\[
\begin{array}{cccccccccccccccc}
S_{{\scriptscriptstyle 2}} & =\{ &  & \sigma_{{\scriptscriptstyle 1}} &  &  & , &  & \sigma_{{\scriptscriptstyle 2}} &  &  & \}=\{ & e & , & \sigma_{{\scriptscriptstyle 2}} & \}\\
 &  & 1 & \rightarrow & 1 & =\sigma_{{\scriptscriptstyle 1}}\left(1\right) &  & 1 & \rightarrow & 2 & =\sigma_{{\scriptscriptstyle 2}}\left(1\right) &  & \parallel\\
 &  & 2 & \rightarrow & 2 & =\sigma_{{\scriptscriptstyle 1}}\left(2\right) &  & 2 & \rightarrow & 1 & =\sigma_{{\scriptscriptstyle 2}}\left(2\right) &  & e_{{\scriptscriptstyle S_{{\scriptscriptstyle 2}}}}\\
 & =\{ & \sigma_{{\scriptscriptstyle 1}} & \begin{array}{c}
1\\
\downarrow\\
1
\end{array} & \begin{array}{c}
2\\
\downarrow\\
2
\end{array} &  & , & \sigma_{{\scriptscriptstyle 2}} & \begin{array}{c}
1\\
\downarrow\\
2
\end{array} & \begin{array}{c}
2\\
\downarrow\\
1
\end{array} &  & \}=\{ & \begin{pmatrix}1 & 2\\
1 & 2
\end{pmatrix} & , & \begin{pmatrix}1 & 2\\
2 & 1
\end{pmatrix} & \}
\end{array}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\sigma_{{\scriptscriptstyle 1}}\sigma_{{\scriptscriptstyle 1}}\left(1\right)= & \sigma_{{\scriptscriptstyle 1}}\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma_{{\scriptscriptstyle 1}}\left(1\right)=\sigma_{{\scriptscriptstyle 1}}\circ\sigma_{{\scriptscriptstyle 1}}\left(1\right)=\sigma_{{\scriptscriptstyle 1}}\left(\sigma_{{\scriptscriptstyle 1}}\left(1\right)\right)=\sigma_{{\scriptscriptstyle 1}}\left(1\right)=1=\sigma_{{\scriptscriptstyle 1}}\left(1\right)\\
\sigma_{{\scriptscriptstyle 1}}\sigma_{{\scriptscriptstyle 1}}\left(2\right)= & \sigma_{{\scriptscriptstyle 1}}\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma_{{\scriptscriptstyle 1}}\left(2\right)=\sigma_{{\scriptscriptstyle 1}}\circ\sigma_{{\scriptscriptstyle 1}}\left(2\right)=\sigma_{{\scriptscriptstyle 1}}\left(\sigma_{{\scriptscriptstyle 1}}\left(2\right)\right)=\sigma_{{\scriptscriptstyle 1}}\left(2\right)=2=\sigma_{{\scriptscriptstyle 1}}\left(2\right)\\
\sigma_{{\scriptscriptstyle 1}}\sigma_{{\scriptscriptstyle 1}}= & \sigma_{{\scriptscriptstyle 1}}\\
\sigma_{{\scriptscriptstyle 2}}\sigma_{{\scriptscriptstyle 1}}\left(1\right)= & \sigma_{{\scriptscriptstyle 2}}\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma_{{\scriptscriptstyle 1}}\left(1\right)=\sigma_{{\scriptscriptstyle 2}}\circ\sigma_{{\scriptscriptstyle 1}}\left(1\right)=\sigma_{{\scriptscriptstyle 2}}\left(\sigma_{{\scriptscriptstyle 1}}\left(1\right)\right)=\sigma_{{\scriptscriptstyle 2}}\left(1\right)=2=\sigma_{{\scriptscriptstyle 2}}\left(1\right)\\
\sigma_{{\scriptscriptstyle 2}}\sigma_{{\scriptscriptstyle 1}}\left(2\right)= & \sigma_{{\scriptscriptstyle 2}}\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma_{{\scriptscriptstyle 1}}\left(2\right)=\sigma_{{\scriptscriptstyle 2}}\circ\sigma_{{\scriptscriptstyle 1}}\left(2\right)=\sigma_{{\scriptscriptstyle 2}}\left(\sigma_{{\scriptscriptstyle 1}}\left(2\right)\right)=\sigma_{{\scriptscriptstyle 2}}\left(2\right)=1=\sigma_{{\scriptscriptstyle 2}}\left(2\right)\\
\sigma_{{\scriptscriptstyle 2}}\sigma_{{\scriptscriptstyle 1}}= & \sigma_{{\scriptscriptstyle 2}}\\
\sigma_{{\scriptscriptstyle 1}}\sigma_{{\scriptscriptstyle 2}}\left(1\right)= & \sigma_{{\scriptscriptstyle 1}}\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma_{{\scriptscriptstyle 2}}\left(1\right)=\sigma_{{\scriptscriptstyle 1}}\circ\sigma_{{\scriptscriptstyle 2}}\left(1\right)=\sigma_{{\scriptscriptstyle 1}}\left(\sigma_{{\scriptscriptstyle 2}}\left(1\right)\right)=\sigma_{{\scriptscriptstyle 1}}\left(2\right)=2=\sigma_{{\scriptscriptstyle 2}}\left(1\right)\\
\sigma_{{\scriptscriptstyle 1}}\sigma_{{\scriptscriptstyle 2}}\left(2\right)= & \sigma_{{\scriptscriptstyle 1}}\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma_{{\scriptscriptstyle 2}}\left(2\right)=\sigma_{{\scriptscriptstyle 1}}\circ\sigma_{{\scriptscriptstyle 2}}\left(2\right)=\sigma_{{\scriptscriptstyle 1}}\left(\sigma_{{\scriptscriptstyle 2}}\left(2\right)\right)=\sigma_{{\scriptscriptstyle 1}}\left(1\right)=1=\sigma_{{\scriptscriptstyle 2}}\left(2\right)\\
\sigma_{{\scriptscriptstyle 1}}\sigma_{{\scriptscriptstyle 2}}= & \sigma_{{\scriptscriptstyle 2}}\\
\sigma_{{\scriptscriptstyle 2}}\sigma_{{\scriptscriptstyle 2}}\left(1\right)= & \sigma_{{\scriptscriptstyle 2}}\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma_{{\scriptscriptstyle 2}}\left(1\right)=\sigma_{{\scriptscriptstyle 2}}\circ\sigma_{{\scriptscriptstyle 2}}\left(1\right)=\sigma_{{\scriptscriptstyle 2}}\left(\sigma_{{\scriptscriptstyle 2}}\left(1\right)\right)=\sigma_{{\scriptscriptstyle 2}}\left(2\right)=1=\sigma_{{\scriptscriptstyle 1}}\left(1\right)\\
\sigma_{{\scriptscriptstyle 2}}\sigma_{{\scriptscriptstyle 2}}\left(2\right)= & \sigma_{{\scriptscriptstyle 2}}\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\sigma_{{\scriptscriptstyle 2}}\left(2\right)=\sigma_{{\scriptscriptstyle 2}}\circ\sigma_{{\scriptscriptstyle 2}}\left(2\right)=\sigma_{{\scriptscriptstyle 2}}\left(\sigma_{{\scriptscriptstyle 2}}\left(2\right)\right)=\sigma_{{\scriptscriptstyle 2}}\left(1\right)=2=\sigma_{{\scriptscriptstyle 1}}\left(2\right)\\
\sigma_{{\scriptscriptstyle 2}}\sigma_{{\scriptscriptstyle 2}}= & \sigma_{{\scriptscriptstyle 1}}\\
\sigma_{{\scriptscriptstyle 1}}\sigma_{{\scriptscriptstyle i}}=\sigma_{{\scriptscriptstyle i}}\sigma_{{\scriptscriptstyle 1}}=\sigma_{{\scriptscriptstyle i}}\Rightarrow & \sigma_{{\scriptscriptstyle 1}}=e=e_{{\scriptscriptstyle S_{{\scriptscriptstyle 2}}}}=\overline{\sigma}_{{\scriptscriptstyle 1}}=\sigma_{{\scriptscriptstyle 1}}^{{\scriptscriptstyle -1}}\\
\sigma_{{\scriptscriptstyle 2}}\sigma_{{\scriptscriptstyle 2}}=\sigma_{{\scriptscriptstyle 1}}\Rightarrow & \sigma_{{\scriptscriptstyle 2}}=\overline{\sigma}_{{\scriptscriptstyle 2}}=\sigma_{{\scriptscriptstyle 2}}^{{\scriptscriptstyle -1}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{array}{ccc}
\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}} & \sigma_{{\scriptscriptstyle 1}} & \sigma_{{\scriptscriptstyle 2}}\\
\sigma_{{\scriptscriptstyle 1}} & \sigma_{{\scriptscriptstyle 1}} & \sigma_{{\scriptscriptstyle 2}}\\
\sigma_{{\scriptscriptstyle 2}} & \sigma_{{\scriptscriptstyle 2}} & \sigma_{{\scriptscriptstyle 1}}
\end{array}\leftrightarrow\begin{array}{ccc}
\circ & \sigma_{{\scriptscriptstyle 1}} & \sigma_{{\scriptscriptstyle 2}}\\
\sigma_{{\scriptscriptstyle 1}} & e & \sigma_{{\scriptscriptstyle 2}}\\
\sigma_{{\scriptscriptstyle 2}} & \sigma_{{\scriptscriptstyle 2}} & e
\end{array}\leftrightarrow\begin{array}{ccc}
\circ & e & \sigma_{{\scriptscriptstyle 2}}\\
e & e & \sigma_{{\scriptscriptstyle 2}}\\
\sigma_{{\scriptscriptstyle 2}} & \sigma_{{\scriptscriptstyle 2}} & e
\end{array}
\]

\end_inset


\end_layout

\begin_layout Definition
\begin_inset Formula $S_{{\scriptscriptstyle 2}}=\mathbb{Z}_{{\scriptscriptstyle 2}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},+\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right) & = & \left\{ 0,2-1\right\}  & = & \left\{ 0,1\right\} \\
\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{{\scriptscriptstyle \mathbb{C}}}\right) & = & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{2}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{2}\left(2-1\right)}}\right\} =\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\pi}}\right\}  & = & \left\{ 1,-1\right\} \\
S_{{\scriptscriptstyle 2}}=\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left(S_{{\scriptscriptstyle 2}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\right) & = & \left(S_{{\scriptscriptstyle 2}},\circ\right) & = & \left\{ \sigma_{{\scriptscriptstyle 1}},\sigma_{{\scriptscriptstyle 2}}\right\} =\left\{ e_{{\scriptscriptstyle S_{{\scriptscriptstyle 2}}}},\sigma_{{\scriptscriptstyle 2}}\right\}  & = & \left\{ e,\sigma_{{\scriptscriptstyle 2}}\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Definition
\begin_inset Formula $\left|S_{{\scriptscriptstyle n}}\right|=n!$
\end_inset

 
\begin_inset Formula 
\[
\left|S_{{\scriptscriptstyle n}}\right|=\mathrm{P}_{{\scriptscriptstyle n}}^{{\scriptscriptstyle n}}=n!=n\cdot\left(n-1\right)\cdot\cdots\cdot2\cdot1=\overset{n}{\overbrace{n\left(n-1\right)\cdots1}}=n\left(n-1\right)\cdots1
\]

\end_inset


\begin_inset Formula 
\[
I\begin{aligned} & S_{{\scriptscriptstyle n}}=\left(S_{{\scriptscriptstyle n}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\right)=\left(S_{{\scriptscriptstyle n}},\circ\right)\\
= & \left\{ \sigma\middle|\begin{aligned}n\in & \mathbb{N}\\
N= & \left\{ \overset{n}{\overbrace{1,\cdots,n}}\right\} \\
\sigma\in & N^{N}\\
\sigma\left(N\right)= & N
\end{aligned}
\right\} =\left\{ \sigma\middle|\begin{aligned}n\in & \mathbb{N}\\
N= & \left\{ 1,\cdots,n\right\} \\
\sigma: & N\rightarrow N\\
\forall\sigma_{{\scriptscriptstyle i}},\sigma_{{\scriptscriptstyle j}}\in S_{{\scriptscriptstyle n}} & \left[\sigma_{{\scriptscriptstyle i}}\sigma_{{\scriptscriptstyle j}}=\sigma_{{\scriptscriptstyle i}}\circ\sigma_{{\scriptscriptstyle j}}\right]\\
\forall m_{{\scriptscriptstyle 1}},m_{{\scriptscriptstyle 2}}\in N & \Biggl[m_{{\scriptscriptstyle 1}}\ne m_{{\scriptscriptstyle 2}}\Leftrightarrow\sigma\left(m_{{\scriptscriptstyle 1}}\right)\ne\sigma\left(m_{{\scriptscriptstyle 2}}\right)\Biggr]
\end{aligned}
\right\} \\
= & \left\{ \sigma\middle|\sigma=\begin{pmatrix}1 & 2 & \cdots & n\\
\sigma\left(1\right) & \sigma\left(2\right) & \cdots & \sigma\left(n\right)
\end{pmatrix}=\left(\overset{n}{\overbrace{\begin{array}{ccc}
1 & \cdots & n\\
\sigma\left(1\right) & \cdots & \sigma\left(n\right)
\end{array}}}\right)=\begin{pmatrix}1 & \cdots & n\\
\sigma\left(1\right) & \cdots & \sigma\left(n\right)
\end{pmatrix}\right\}  & \text{two-line notation}\\
= & \left\{ \sigma\middle|\sigma=\sigma\left(1\right)\sigma\left(2\right)\cdots\sigma\left(n\right)=\overset{n}{\overbrace{\sigma\left(1\right)\sigma\left(2\right)\cdots\sigma\left(n\right)}}=\sigma\left(1\right)\cdots\sigma\left(n\right)=\left[\sigma\left(1\right)\cdots\sigma\left(n\right)\right]\right\}  & \text{one-line notation}\\
= & \left\{ \sigma\middle|\sigma=c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}=\overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}=c_{{\scriptscriptstyle 1}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}=\overset{n_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{c_{{\scriptscriptstyle 1}}}{\underbrace{c_{{\scriptscriptstyle 11}}c_{{\scriptscriptstyle 12}}\cdots c_{{\scriptscriptstyle 1n_{{\scriptscriptstyle 1}}}}}}\cdots\underset{c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}{\underbrace{c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}1}}c_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}2}}\cdots c_{{\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}n_{{\scriptscriptstyle {\scriptscriptstyle n_{{\scriptscriptstyle \sigma}}}}}}}}}}}\right\}  & \text{cycle notation}\\
= & \left\{ \sigma\middle|\sigma=s_{{\scriptscriptstyle 1}}s_{{\scriptscriptstyle 2}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}=\overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{s_{{\scriptscriptstyle 1}}s_{{\scriptscriptstyle 2}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}}=s_{{\scriptscriptstyle 1}}\cdots s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}=\overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}}}}\right\}  & \text{swap notation}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Definition
two-line notation
\begin_inset Formula 
\[
\begin{pmatrix}1 & 2 & \cdots & n\\
\sigma\left(1\right) & \sigma\left(2\right) & \cdots & \sigma\left(n\right)
\end{pmatrix}=\begin{pmatrix}2 & 1 & \cdots & n\\
\sigma\left(2\right) & \sigma\left(1\right) & \cdots & \sigma\left(n\right)
\end{pmatrix},\cdots
\]

\end_inset


\end_layout

