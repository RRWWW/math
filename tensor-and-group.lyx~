\begin_layout Section
subgroup
\end_layout

\begin_layout Definition
subgroup
\begin_inset Formula 
\[
\begin{aligned}G=\left(G,\cdot\right) & =\left(G,\cdot_{{\scriptscriptstyle G}}\right)\text{ is a group}\\
G\supseteq H & \ne\emptyset\\
h_{{\scriptscriptstyle 1}}\cdot h_{{\scriptscriptstyle 2}}=h_{{\scriptscriptstyle 1}}h_{{\scriptscriptstyle 2}} & \in H & \forall h_{{\scriptscriptstyle 1}},h_{{\scriptscriptstyle 2}}\in H\quad\left(c\right)\cdot\text{closure}\\
h^{{\scriptscriptstyle -1}}=\overline{h} & \in H & \forall h\in H\quad\left(in\right)\text{inverse}\\
\Updownarrow\\
H & \le G\\
\Updownarrow\\
H & \text{ is a subgroup of }G & \overset{\text{need to be proved}}{\Rightarrow}H=\left(H,\cdot\right)=\left(H,\cdot_{{\scriptscriptstyle G}}\right)\text{ is a group}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
trivial subgroups
\begin_inset Formula 
\[
\begin{aligned}\left\{ e\right\} =\left\{ e_{{\scriptscriptstyle G}}\right\} \le & G\\
G=G\le & G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{array}{ccc}
C_{{\scriptscriptstyle 3}}= & \mathbb{Z}_{{\scriptscriptstyle 3}}\le D_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}\\
 & \mathbb{Z}_{{\scriptscriptstyle 2}}\le S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}} & \qquad\mathbb{Z}_{{\scriptscriptstyle 2}}\le S_{{\scriptscriptstyle n}}\quad\forall n\in\mathbb{N}_{{\scriptscriptstyle \ge2}}
\end{array}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\recolor{\begin{array}{ccc}
\begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
\rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 0+0}} & \rho_{{\scriptscriptstyle 0+1}} & \rho_{{\scriptscriptstyle 0+2}} & \pi_{{\scriptscriptstyle 0+0}} & \pi_{{\scriptscriptstyle 0+1}} & \pi_{{\scriptscriptstyle 0+2}}\\
\rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 1+0}} & \rho_{{\scriptscriptstyle 1+1}} & \rho_{{\scriptscriptstyle 1+2}} & \pi_{{\scriptscriptstyle 1+0}} & \pi_{{\scriptscriptstyle 1+1}} & \pi_{{\scriptscriptstyle 1+2}}\\
\rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 2+0}} & \rho_{{\scriptscriptstyle 2+1}} & \rho_{{\scriptscriptstyle 2+2}} & \pi_{{\scriptscriptstyle 2+0}} & \pi_{{\scriptscriptstyle 2+1}} & \pi_{{\scriptscriptstyle 2+2}}\\
\pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 0-0}} & \pi_{{\scriptscriptstyle 0-1}} & \pi_{{\scriptscriptstyle 0-2}} & \rho_{{\scriptscriptstyle 0-0}} & \rho_{{\scriptscriptstyle 0-1}} & \rho_{{\scriptscriptstyle 0-2}}\\
\pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 1-0}} & \pi_{{\scriptscriptstyle 1-1}} & \pi_{{\scriptscriptstyle 1-2}} & \rho_{{\scriptscriptstyle 1-0}} & \rho_{{\scriptscriptstyle 1-1}} & \rho_{{\scriptscriptstyle 1-2}}\\
\pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 2-0}} & \pi_{{\scriptscriptstyle 2-1}} & \pi_{{\scriptscriptstyle 2-2}} & \rho_{{\scriptscriptstyle 2-0}} & \rho_{{\scriptscriptstyle 2-1}} & \rho_{{\scriptscriptstyle 2-2}}
\end{array} & \rightarrow & \begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
\rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
\rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 3}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 3}}\\
\rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 3}} & \rho_{{\scriptscriptstyle 4}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 3}} & \pi_{{\scriptscriptstyle 4}}\\
\pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle -1}} & \pi_{{\scriptscriptstyle -2}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle -1}} & \rho_{{\scriptscriptstyle -2}}\\
\pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle -1}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle -1}}\\
\pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}}
\end{array}\\
 &  & \rho_{{\scriptscriptstyle k+3}}=\rho_{{\scriptscriptstyle k}}\downarrow\pi_{{\scriptscriptstyle k+3}}=\pi_{{\scriptscriptstyle k}}\\
\begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle S_{3}}} & \left[123\right] & \left[231\right] & \left[312\right] & \left[213\right] & \left[132\right] & \left[321\right]\\
\left[123\right] & \left[123\right] & \left[231\right] & \left[312\right] & \left[213\right] & \left[132\right] & \left[321\right]\\
\left[231\right] & \left[231\right] & \left[312\right] & \left[123\right] & \left[132\right] & \left[321\right] & \left[213\right]\\
\left[312\right] & \left[312\right] & \left[123\right] & \left[231\right] & \left[321\right] & \left[213\right] & \left[132\right]\\
\left[213\right] & \left[213\right] & \left[321\right] & \left[132\right] & \left[123\right] & \left[312\right] & \left[231\right]\\
\left[132\right] & \left[132\right] & \left[213\right] & \left[321\right] & \left[231\right] & \left[123\right] & \left[312\right]\\
\left[321\right] & \left[321\right] & \left[132\right] & \left[213\right] & \left[312\right] & \left[231\right] & \left[123\right]
\end{array} & \overset{S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}}{\leftarrow} & \begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
\rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
\rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}}\\
\rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}}\\
\pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}}
\end{array}\\
 &  & \downarrow\\
\begin{array}{ccccccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 3}}}} & 0_{{\scriptscriptstyle \rho}} & 1_{{\scriptscriptstyle \rho}} & 2_{{\scriptscriptstyle \rho}} & 0_{{\scriptscriptstyle \pi}} & 1_{{\scriptscriptstyle \pi}} & 2_{{\scriptscriptstyle \pi}}\\
0_{{\scriptscriptstyle \rho}} & 0_{{\scriptscriptstyle \rho}} & 1_{{\scriptscriptstyle \rho}} & 2_{{\scriptscriptstyle \rho}} & 0_{{\scriptscriptstyle \pi}} & 1_{{\scriptscriptstyle \pi}} & 2_{{\scriptscriptstyle \pi}}\\
1_{{\scriptscriptstyle \rho}} & 1_{{\scriptscriptstyle \rho}} & 2_{{\scriptscriptstyle \rho}} & 0_{{\scriptscriptstyle \rho}} & 1_{{\scriptscriptstyle \pi}} & 2_{{\scriptscriptstyle \pi}} & 0_{{\scriptscriptstyle \pi}}\\
2_{{\scriptscriptstyle \rho}} & 2_{{\scriptscriptstyle \rho}} & 0_{{\scriptscriptstyle \rho}} & 1_{{\scriptscriptstyle \rho}} & 2_{{\scriptscriptstyle \pi}} & 0_{{\scriptscriptstyle \pi}} & 1_{{\scriptscriptstyle \pi}}\\
0_{{\scriptscriptstyle \pi}} & 0_{{\scriptscriptstyle \pi}} & 2_{{\scriptscriptstyle \pi}} & 1_{{\scriptscriptstyle \pi}} & 0_{{\scriptscriptstyle \rho}} & 2_{{\scriptscriptstyle \rho}} & 1_{{\scriptscriptstyle \rho}}\\
1_{{\scriptscriptstyle \pi}} & 1_{{\scriptscriptstyle \pi}} & 0_{{\scriptscriptstyle \pi}} & 2_{{\scriptscriptstyle \pi}} & 1_{{\scriptscriptstyle \rho}} & 0_{{\scriptscriptstyle \rho}} & 2_{{\scriptscriptstyle \rho}}\\
2_{{\scriptscriptstyle \pi}} & 2_{{\scriptscriptstyle \pi}} & 1_{{\scriptscriptstyle \pi}} & 0_{{\scriptscriptstyle \pi}} & 2_{{\scriptscriptstyle \rho}} & 1_{{\scriptscriptstyle \rho}} & 0_{{\scriptscriptstyle \rho}}
\end{array} & \leftarrow & \begin{array}{ccccccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 3}}}} & 0 & 1 & 2 & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
0 & 0 & 1 & 2 & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
1 & 1 & 2 & 0 & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}}\\
2 & 2 & 0 & 1 & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}}\\
\pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}}
\end{array}\\
\Downarrow &  & \Downarrow\\
\begin{aligned} & \mathbb{Z}_{{\scriptscriptstyle 3}}=\left\{ 0,1,2\right\} \\
= & \left\{ 0_{{\scriptscriptstyle \rho}},1_{{\scriptscriptstyle \rho}},2_{{\scriptscriptstyle \rho}}\right\} =\left\{ \left[123\right],\left[231\right],\left[312\right]\right\} =\left\{ \left(\right),\left(123\right),\left(132\right)\right\} \\
= & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}1}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}2}},\cdots,\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(n-1\right)}}\right\} \overset{n=3}{=}\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}1}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}2}}\right\} \\
= & \left\{ e,g,g^{2},\cdots,g^{n-1}\right\} =\left\{ g^{0},g^{1},g^{2}\right\} =\left\{ e,g,g^{2}\right\} ,g^{n}=e
\end{aligned}
 &  & \mathbb{Z}_{{\scriptscriptstyle 3}}\le D_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}\\
\Uparrow\\
\begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle S_{3}}} & \left(\right) & \left(123\right) & \left(132\right) & \left(12\right) & \left(23\right) & \left(31\right)\\
\left(\right) & \left(\right) & \left(123\right) & \left(132\right) & \left(12\right) & \left(23\right) & \left(31\right)\\
\left(123\right) & \left(123\right) & \left(132\right) & \left(\right) & \left(23\right) & \left(31\right) & \left(12\right)\\
\left(132\right) & \left(132\right) & \left(\right) & \left(123\right) & \left(31\right) & \left(12\right) & \left(23\right)\\
\left(12\right) & \left(12\right) & \left(31\right) & \left(23\right) & \left(\right) & \left(132\right) & \left(123\right)\\
\left(23\right) & \left(23\right) & \left(12\right) & \left(31\right) & \left(123\right) & \left(\right) & \left(132\right)\\
\left(31\right) & \left(31\right) & \left(23\right) & \left(12\right) & \left(132\right) & \left(123\right) & \left(\right)
\end{array} & \rightarrow & \begin{array}{ccccccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 2}}}} & 0 & \left(123\right) & \left(132\right) & 1 & \left(23\right) & \left(31\right)\\
0 & 0 & \left(123\right) & \left(132\right) & 1 & \left(23\right) & \left(31\right)\\
\left(123\right) & \left(123\right) & \left(132\right) & \left(\right) & \left(23\right) & \left(31\right) & \left(12\right)\\
\left(132\right) & \left(132\right) & \left(\right) & \left(123\right) & \left(31\right) & \left(12\right) & \left(23\right)\\
1 & 1 & \left(31\right) & \left(23\right) & 0 & \left(132\right) & \left(123\right)\\
\left(23\right) & \left(23\right) & \left(12\right) & \left(31\right) & \left(123\right) & \left(\right) & \left(132\right)\\
\left(31\right) & \left(31\right) & \left(23\right) & \left(12\right) & \left(132\right) & \left(123\right) & \left(\right)
\end{array}\\
\Downarrow &  & \text{or}\\
\mathbb{Z}_{{\scriptscriptstyle 2}}\le S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}} & \Leftarrow & \begin{array}{ccccccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 2}}}} & 0 & \left(123\right) & \left(132\right) & \left(12\right) & 1 & \left(31\right)\\
0 & 0 & \left(123\right) & \left(132\right) & \left(12\right) & 1 & \left(31\right)\\
\left(123\right) & \left(123\right) & \left(132\right) & \left(\right) & \left(23\right) & \left(31\right) & \left(12\right)\\
\left(132\right) & \left(132\right) & \left(\right) & \left(123\right) & \left(31\right) & \left(12\right) & \left(23\right)\\
\left(12\right) & \left(12\right) & \left(31\right) & \left(23\right) & \left(\right) & \left(132\right) & \left(123\right)\\
1 & 1 & \left(12\right) & \left(31\right) & \left(123\right) & 0 & \left(132\right)\\
\left(31\right) & \left(31\right) & \left(23\right) & \left(12\right) & \left(132\right) & \left(123\right) & \left(\right)
\end{array}\\
\Downarrow &  & \text{or}\\
\mathbb{Z}_{{\scriptscriptstyle 2}}\le S_{{\scriptscriptstyle n}}\quad\forall n\in\mathbb{N}_{{\scriptscriptstyle \ge2}} &  & \begin{array}{ccccccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 2}}}} & 0 & \left(123\right) & \left(132\right) & \left(12\right) & \left(23\right) & 1\\
0 & 0 & \left(123\right) & \left(132\right) & \left(12\right) & \left(23\right) & 1\\
\left(123\right) & \left(123\right) & \left(132\right) & \left(\right) & \left(23\right) & \left(31\right) & \left(12\right)\\
\left(132\right) & \left(132\right) & \left(\right) & \left(123\right) & \left(31\right) & \left(12\right) & \left(23\right)\\
\left(12\right) & \left(12\right) & \left(31\right) & \left(23\right) & \left(\right) & \left(132\right) & \left(123\right)\\
\left(23\right) & \left(23\right) & \left(12\right) & \left(31\right) & \left(123\right) & \left(\right) & \left(132\right)\\
1 & 1 & \left(23\right) & \left(12\right) & \left(132\right) & \left(123\right) & 0
\end{array}
\end{array}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $S_{{\scriptscriptstyle 2}}=\mathbb{Z}_{{\scriptscriptstyle 2}}\le S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}$
\end_inset


\begin_inset Formula 
\[
\recolor{\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},+\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right) & = & \left\{ 0,2-1\right\}  & = & \left\{ 0,1\right\} \\
=\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot\right) & = & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{{\scriptscriptstyle \mathbb{C}}}\right) & = & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{2}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{2}\left(2-1\right)}}\right\} =\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\pi}}\right\}  & = & \left\{ 1,-1\right\} \\
=S_{{\scriptscriptstyle 2}}=\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left(S_{{\scriptscriptstyle 2}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle n}}}}\right) & = & \left(S_{{\scriptscriptstyle 2}},\circ\right) & = & \left\{ \sigma_{{\scriptscriptstyle 1}},\sigma_{{\scriptscriptstyle 2}}\right\} =\left\{ e_{{\scriptscriptstyle S_{{\scriptscriptstyle 2}}}},\sigma_{{\scriptscriptstyle 2}}\right\}  & = & \left\{ e,\sigma_{{\scriptscriptstyle 2}}\right\} \\
= & C_{{\scriptscriptstyle 2,1}} & = & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle 3}}}}\right) & = & \left\{ \left[123\right],\left[213\right]\right\} =\left\{ \left(\right),\left(12\right)\right\} \\
= & C_{{\scriptscriptstyle 2,2}} & = & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle 3}}}}\right) & = & \left\{ \left[123\right],\left[132\right]\right\} =\left\{ \left(\right),\left(23\right)\right\} \\
= & C_{{\scriptscriptstyle 2,3}} & = & \left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle 3}}}}\right) & = & \left\{ \left[123\right],\left[321\right]\right\} =\left\{ \left(\right),\left(31\right)\right\} \\
 & E_{{\scriptscriptstyle 3}} & = & \left(\mathbb{Z}_{{\scriptscriptstyle 1}},\cdot_{{\scriptscriptstyle S_{{\scriptscriptstyle 3}}}}\right) & = & \left\{ e\right\} =\left\{ \left[123\right]\right\} =\left\{ \left(\right)\right\}  & e= & e_{{\scriptscriptstyle S_{{\scriptscriptstyle 3}}}}
\end{aligned}
}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 2}}\le\mathbb{Z}_{{\scriptscriptstyle 4}}$
\end_inset


\begin_inset Formula 
\[
\mathbb{Z}_{{\scriptscriptstyle 4}}=\left\{ e,a,a^{2},a^{3}\middle|a^{4}=e\right\} 
\]

\end_inset


\begin_inset Formula 
\[
\begin{array}{ccccc}
\begin{array}{ccccc}
\cdot_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 4}}}} & e & a & a^{2} & a^{3}\\
e & e & a & a^{2} & a^{3}\\
a & a & a^{2} & a^{3} & a^{4}\\
a^{2} & a^{2} & a^{3} & a^{4} & a^{5}\\
a^{3} & a^{3} & a^{4} & a^{5} & a^{6}
\end{array} & \rightarrow & \begin{array}{ccccc}
\cdot_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 4}}}} & e & a & a^{2} & a^{3}\\
e & e & a & a^{2} & a^{3}\\
a & a & a^{2} & a^{3} & e\\
a^{2} & a^{2} & a^{3} & e & a\\
a^{3} & a^{3} & e & a & a^{2}
\end{array} & \rightarrow & \begin{array}{ccccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 2}}}} & 0 & a & 1 & a^{3}\\
0 & 0 & a & 1 & a^{3}\\
a & a & a^{2} & a^{3} & e\\
1 & 1 & a^{3} & 0 & a\\
a^{3} & a^{3} & e & a & a^{2}
\end{array}\\
 &  &  &  & \Downarrow\\
 &  & \left\{ e,a^{2}\right\}  & = & \left\{ 0,1\right\} =\mathbb{Z}_{{\scriptscriptstyle 2}}\le\mathbb{Z}_{{\scriptscriptstyle 4}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 2}}\le\mathbb{Z}_{{\scriptscriptstyle 2n}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 2n}}= & \left\{ e,a,\cdots,a^{n},\cdots,a^{2n-1}\middle|a^{2n}=e\right\}  & \forall n\in\mathbb{N}\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{array}{ccc}
\begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 2n}}}} & e & a & \cdots & a^{n} & \cdots & a^{2n-1}\\
e & e & a & \cdots & a^{n} & \cdots & a^{2n-1}\\
a & a & a^{2} & \cdots & a^{n+1} & \cdots & a^{2n}\\
\vdots & \vdots & \vdots & \ddots & \vdots & \ddots & \vdots\\
a^{n} & a^{n} & a^{n+1} & \cdots & a^{2n} & \cdots & a^{3n-1}\\
\vdots & \vdots & \vdots & \ddots & \vdots & \ddots & \vdots\\
a^{2n-1} & a^{2n-1} & a^{2n} & \cdots & a^{3n-1} & \cdots & a^{4n-2}
\end{array} & \rightarrow & \begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 2n}}}} & e & a & \cdots & a^{n} & \cdots & a^{2n-1}\\
e & e & a & \cdots & a^{n} & \cdots & a^{2n-1}\\
a & a & a^{2} & \cdots & a^{n+1} & \cdots & a^{2n}\\
\vdots & \vdots & \vdots & \ddots & \vdots & \ddots & \vdots\\
a^{n} & a^{n} & a^{n+1} & \cdots & a^{2n} & \cdots & a^{2n+\left(n-1\right)}\\
\vdots & \vdots & \vdots & \ddots & \vdots & \ddots & \vdots\\
a^{2n-1} & a^{2n-1} & a^{2n} & \cdots & a^{2n+\left(n-1\right)} & \cdots & a^{2\left(n-1\right)}
\end{array}\\
 &  & \downarrow\\
\begin{array}{ccccccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 2}}}} & 0 & a & \cdots & 1 & \cdots & a^{2n-1}\\
0 & 0 & a & \cdots & 1 & \cdots & a^{2n-1}\\
a & a & a^{2} & \cdots & a^{n+1} & \cdots & e\\
\vdots & \vdots & \vdots & \ddots & \vdots & \ddots & \vdots\\
1 & 1 & a^{n+1} & \cdots & 0 & \cdots & a^{n-1}\\
\vdots & \vdots & \vdots & \ddots & \vdots & \ddots & \vdots\\
a^{2n-1} & a^{2n-1} & e & \cdots & a^{n-1} & \cdots & e
\end{array} & \leftarrow & \begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 2n}}}} & e & a & \cdots & a^{n} & \cdots & a^{2n-1}\\
e & e & a & \cdots & a^{n} & \cdots & a^{2n-1}\\
a & a & a^{2} & \cdots & a^{n+1} & \cdots & e\\
\vdots & \vdots & \vdots & \ddots & \vdots & \ddots & \vdots\\
a^{n} & a^{n} & a^{n+1} & \cdots & e & \cdots & a^{n-1}\\
\vdots & \vdots & \vdots & \ddots & \vdots & \ddots & \vdots\\
a^{2n-1} & a^{2n-1} & e & \cdots & a^{n-1} & \cdots & e
\end{array}\\
\Downarrow\\
\mathbb{Z}_{{\scriptscriptstyle 2n}}\ge\mathbb{Z}_{{\scriptscriptstyle 2}}=\left\{ 0,1\right\}  & = & \left\{ e,a^{n}\right\} 
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 2}}\nleq\mathbb{Z}_{{\scriptscriptstyle 2n+1}}$
\end_inset


\begin_inset Formula 
\[
\mathbb{Z}_{{\scriptscriptstyle 1}}=\left\{ e\right\} =\left\{ 0\right\} 
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 2n}+1}= & \left\{ e,a,\cdots,a^{k},\cdots,a^{2n}\middle|\begin{aligned}\begin{aligned}a^{2n+1}= & e & \left(id\right)\\
k\in & \mathbb{N}_{{\scriptscriptstyle \le2n}} & \left(xp\right)
\end{aligned}
\end{aligned}
\right\}  & \forall n\in\mathbb{N}\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left(xp\right)\Rightarrow & \left\{ e,a^{k}\middle|k\in\mathbb{N}_{{\scriptscriptstyle \le2n}}\right\}  & \text{even }2\mathbb{N}\ni2k\ne2n+1\in2\mathbb{N}+1\text{ odd}\\
\in & \left\{ \left\{ e,a^{k}\middle|1\le k\le2n\right\} \right\}  & \Rightarrow2\le2k\le2\cdot2n<2\left(2n+1\right)\\
 &  & \Downarrow\\
 &  & 2k\ne2\left(2n+1\right)\Rightarrow2n+1\nmid2k\\
a^{2k}= & \left(a^{k}\right)^{2}\ne\left(a^{2n+1}\right)^{m}\overset{\left(id\right)}{=}\left(e\right)^{m}=e & m\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
\left(a^{k}\right)^{2}\ne & e & \Rightarrow\mathbb{Z}_{{\scriptscriptstyle 2}}\nleq\mathbb{Z}_{{\scriptscriptstyle 2n}+1}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $2\mathbb{Z}\le\mathbb{Z}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}= & \left\{ \cdots,-3,-2,-1,0,+1,+2,+3,\cdots\right\} =\left\{ \cdots,-3,-2,-1,0,1,2,3,\cdots\right\} =\left\{ k\middle|k\in\mathbb{Z}\right\} \\
2\mathbb{Z}= & \left\{ \cdots,-6,-4,-2,0,+2,+4,+6,\cdots\right\} =\left\{ \cdots,-6,-4,-2,0,2,4,6,\cdots\right\} =\left\{ 2k\middle|k\in\mathbb{Z}\right\} \\
2\mathbb{Z}\subset & \mathbb{Z}\\
2k_{{\scriptscriptstyle 1}}+2k_{{\scriptscriptstyle 2}}= & 2\left(k_{{\scriptscriptstyle 1}}+k_{{\scriptscriptstyle 2}}\right)\in2\mathbb{Z}\\
-2k=\overline{2k}= & 2\left(-k\right)\in2\mathbb{Z}\\
\Downarrow\\
2\mathbb{Z}\le & \mathbb{Z}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $n\mathbb{Z}\le\mathbb{Z}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}= & \left\{ \cdots,-3,-2,-1,0,+1,+2,+3,\cdots\right\} =\left\{ \cdots,-3,-2,-1,0,1,2,3,\cdots\right\} =\left\{ k\middle|k\in\mathbb{Z}\right\} \\
n\mathbb{Z}= & \left\{ \cdots,-3n,-2n,-n,0,n,2n,3n,\cdots\right\} =\left\{ nk\middle|\begin{aligned}k\in & \mathbb{Z}\\
n\in & \mathbb{N}
\end{aligned}
\right\} \\
n\mathbb{Z}\subseteq & \mathbb{Z}\\
nk_{{\scriptscriptstyle 1}}+nk_{{\scriptscriptstyle 2}}= & n\left(k_{{\scriptscriptstyle 1}}+k_{{\scriptscriptstyle 2}}\right)\in n\mathbb{Z}\\
-nk=\overline{nk}= & n\left(-k\right)\in n\mathbb{Z}\\
\Downarrow\\
n\mathbb{Z}\le & \mathbb{Z}\\
\left|n\mathbb{Z}\right|\nless & \infty
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left\{ 0\right\} \le\mathbb{Z}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left\{ 0\right\} \subset & \mathbb{Z}\\
0+0= & 0\in\left\{ 0\right\} \\
-0=\overline{0}= & 0\in\left\{ 0\right\} \\
\Downarrow\\
\left\{ 0\right\} \le & \mathbb{Z}\\
\left|\left\{ 0\right\} \right|= & 1<\infty
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle n}}\le U\left(1\right)$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle n}}=\left(\mathbb{Z}_{{\scriptscriptstyle n}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle n}},\cdot_{{\scriptscriptstyle \mathbb{C}}}\right)= & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}1}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}2}},\cdots,\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(n-1\right)}}\right\}  & \forall n\in\mathbb{N}\\
= & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}k}}\middle|k\in\mathbb{N}_{{\scriptscriptstyle <n}}\right\} \\
= & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}k}}\middle|k\in\mathbb{N}_{{\scriptscriptstyle \le n-1}}\right\} \\
= & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}k}}\middle|k\in\left\{ 0,1,\cdots,n-1\right\} \right\} \\
\subset & U\left(1\right)=\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\theta}}\middle|\theta\in\left[0,2\pi\right]\right\} \\
\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}k_{{\scriptscriptstyle 1}}}}\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}k_{{\scriptscriptstyle 2}}}}= & \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(k_{{\scriptscriptstyle 1}}+k_{{\scriptscriptstyle 2}}\right)}}=\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(k_{{\scriptscriptstyle 1}}+k_{{\scriptscriptstyle 2}}\right)\mod n}}\in\mathbb{Z}_{{\scriptscriptstyle n}}\\
\mathrm{e}^{{\scriptscriptstyle -\mathrm{i}\frac{2\pi}{n}k}}=\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(-k\right)}}= & \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(-k\right)}}\cdot1=\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(-k\right)}}\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}n}}=\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(n-k\right)}}\in\mathbb{Z}_{{\scriptscriptstyle n}}\\
\Downarrow\\
\mathbb{Z}_{{\scriptscriptstyle n}}\le & U\left(1\right)
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $D_{{\scriptscriptstyle n}}\le S_{{\scriptscriptstyle n}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}D_{{\scriptscriptstyle n}}= & \left\{ \begin{aligned}\rho_{{\scriptscriptstyle k,n}}\\
\pi_{{\scriptscriptstyle k,n}}
\end{aligned}
\middle|\begin{aligned}\rho_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & -\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & +\cos\dfrac{2\pi}{n}k
\end{bmatrix}\\
\pi_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & +\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & -\cos\dfrac{2\pi}{n}k
\end{bmatrix}
\end{aligned}
\right\}  & \begin{aligned}\forall n\in\mathbb{N}\\
k\in\left\{ 0,\cdots,n-1\right\} =\mathbb{Z}_{{\scriptscriptstyle \left[0,n\right)}}
\end{aligned}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left|D_{{\scriptscriptstyle 1}}\right|=2\cdot1=2> & 1=1!=\left|S_{{\scriptscriptstyle 1}}\right| & n=1\\
\left|D_{{\scriptscriptstyle 2}}\right|=2\cdot2=4> & 2=2!=\left|S_{{\scriptscriptstyle 2}}\right| & n=2\\
\left|D_{{\scriptscriptstyle 3}}\right|=2\cdot3=6= & 6=3!=\left|S_{{\scriptscriptstyle 3}}\right| & n=3\\
\left|D_{{\scriptscriptstyle n}}\right|=2n< & n!=\left|S_{{\scriptscriptstyle n}}\right| & \forall n\in\mathbb{N}_{{\scriptscriptstyle >3}}\\
\Downarrow &  & D_{{\scriptscriptstyle n}}\text{ do not change neighbors}\\
D_{{\scriptscriptstyle n}}\subseteq & S_{{\scriptscriptstyle n}} & \forall n\in\mathbb{N}_{{\scriptscriptstyle >3}}\\
\Downarrow &  & D_{{\scriptscriptstyle n}}\text{ are groups}\\
D_{{\scriptscriptstyle n}}\le & S_{{\scriptscriptstyle n}} & \forall n\in\mathbb{N}_{{\scriptscriptstyle >3}}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A_{{\scriptscriptstyle n}}\le S_{{\scriptscriptstyle n}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}A_{{\scriptscriptstyle n}}= & \left\{ \sigma\middle|\begin{aligned}\sigma\in & S_{{\scriptscriptstyle n}}\\
N_{{\scriptscriptstyle \sigma}}\in & 2\mathbb{Z}_{{\scriptscriptstyle \ge0}}
\end{aligned}
\right\} \subseteq S_{{\scriptscriptstyle n}}\\
\sigma\widetilde{\sigma}= & \overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}}}}\overset{\widetilde{N}_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{\widetilde{s}_{{\scriptscriptstyle 1}}}{\underbrace{\left(\widetilde{s}_{{\scriptscriptstyle 11}}\widetilde{s}_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{\widetilde{s}_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(\widetilde{s}_{{\scriptscriptstyle {\scriptscriptstyle \widetilde{N}_{{\scriptscriptstyle \sigma}}}1}}\widetilde{s}_{{\scriptscriptstyle {\scriptscriptstyle \widetilde{N}_{{\scriptscriptstyle \sigma}}}2}}\right)}}}} & \begin{aligned}\forall\sigma\in A_{{\scriptscriptstyle n}}\Rightarrow N_{{\scriptscriptstyle \sigma}}\in & 2\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
\forall\widetilde{\sigma}\in A_{{\scriptscriptstyle n}}\Rightarrow\widetilde{N}_{{\scriptscriptstyle \sigma}}\in & 2\mathbb{Z}_{{\scriptscriptstyle \ge0}}
\end{aligned}
\\
= & \overset{N_{{\scriptscriptstyle \sigma}}+\widetilde{N}_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}\underset{\widetilde{s}_{{\scriptscriptstyle 1}}}{\underbrace{\left(\widetilde{s}_{{\scriptscriptstyle 11}}\widetilde{s}_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{\widetilde{s}_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(\widetilde{s}_{{\scriptscriptstyle {\scriptscriptstyle \widetilde{N}_{{\scriptscriptstyle \sigma}}}1}}\widetilde{s}_{{\scriptscriptstyle {\scriptscriptstyle \widetilde{N}_{{\scriptscriptstyle \sigma}}}2}}\right)}}}}} & N_{{\scriptscriptstyle \sigma}}+\widetilde{N}_{{\scriptscriptstyle \sigma}}\in2\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
\in & A_{{\scriptscriptstyle n}}\\
\sigma^{{\scriptscriptstyle -1}}=\overline{\sigma}= & \overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{\overline{\underset{s_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 11}}s_{{\scriptscriptstyle 12}}\right)}}\cdots\underset{s_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}\right)}}}}}=\overset{N_{{\scriptscriptstyle \sigma}}}{\overbrace{\underset{\overline{s}_{{\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}}}{\underbrace{\left(s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}2}}s_{{\scriptscriptstyle {\scriptscriptstyle N_{{\scriptscriptstyle \sigma}}}1}}\right)}}\cdots\underset{\overline{s}_{{\scriptscriptstyle 1}}}{\underbrace{\left(s_{{\scriptscriptstyle 12}}s_{{\scriptscriptstyle 11}}\right)}}}} & \forall\sigma\in A_{{\scriptscriptstyle n}}\Rightarrow N_{{\scriptscriptstyle \sigma}}\in2\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
\in & A_{{\scriptscriptstyle n}}\\
\Downarrow\\
A_{{\scriptscriptstyle n}}\le & S_{{\scriptscriptstyle n}}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Theorem
finite cyclic subgroup
\begin_inset Formula 
\[
\begin{aligned}\left(G,\cdot_{{\scriptscriptstyle G}}\right)=\left(G,\cdot\right)= & G\text{ is a group}\\
\left|G\right|< & \infty & G\text{ is a finite group}\\
g\in & G\\
\left\langle g\right\rangle = & \left\{ g^{n}\middle|n\in\mathbb{N}\right\}  & \forall g\in G\\
\ensuremath{\Downarrow}\\
\left\langle g\right\rangle \le & G & \left\langle g\right\rangle \text{ is a finite cyclic subgroup of }G\text{, generated by }g\in G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula $g=e=e_{{\scriptscriptstyle G}}$
\end_inset

,
\begin_inset Formula 
\[
\begin{aligned}\left\langle e\right\rangle = & \left\{ e\right\} =\left\{ e_{{\scriptscriptstyle G}}\right\} \le G & \text{is one of trivial subgroups}\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula $g\ne e=e_{{\scriptscriptstyle G}}$
\end_inset

,
\begin_inset Formula 
\[
\begin{aligned}\left\langle g\right\rangle  & =\left\{ g^{n}\middle|n\in\mathbb{N}\right\} =\left\{ g^{1},g^{2},\cdots\right\} =\left\{ g,g^{2},\cdots,g^{n},\cdots\right\}  & n\in\mathbb{N}\\
 & \subseteq G & g^{n}\in G\\
\left|\left\langle g\right\rangle \right| & \le\left|G\right|<\infty & \Rightarrow\left|\left\langle g\right\rangle \right|<\infty\\
\left\langle g\right\rangle :\mathbb{N} & \rightarrow G & \Leftrightarrow\left\langle g\right\rangle \in G^{\mathbb{N}}\\
 & \Downarrow\text{pigeonhole principle}=\text{Dirichlet drawer principle} & \Leftarrow\left|\mathbb{N}\right|\nless\infty\wedge\begin{aligned}\left|G\right| & <\infty\\
\left|\left\langle g\right\rangle \right| & <\infty
\end{aligned}
\\
\exists n_{{\scriptscriptstyle 1}}\ne n_{{\scriptscriptstyle 2}}\Biggl[g^{n_{{\scriptscriptstyle 1}}} & =g^{n_{{\scriptscriptstyle 2}}}\Biggr] & n_{{\scriptscriptstyle 1}},n_{{\scriptscriptstyle 2}}\in\mathbb{N}\\
 & \Downarrow\text{without loss of generality, let }n_{{\scriptscriptstyle 1}}<n_{{\scriptscriptstyle 2}}\\
\exists n_{{\scriptscriptstyle 1}}<n_{{\scriptscriptstyle 2}}\Biggl[g^{n_{{\scriptscriptstyle 2}}} & =g^{n_{{\scriptscriptstyle 1}}}\Biggr]\\
g^{n_{{\scriptscriptstyle 2}}} & \overline{g^{n_{{\scriptscriptstyle 1}}}}=g^{n_{{\scriptscriptstyle 1}}}\overline{g^{n_{{\scriptscriptstyle 1}}}}=g^{n_{{\scriptscriptstyle 1}}}g^{-n_{{\scriptscriptstyle 1}}}=g^{n_{{\scriptscriptstyle 1}}-n_{{\scriptscriptstyle 1}}}=g^{0}=e=e_{{\scriptscriptstyle G}} & \forall G\text{ is a group,\forall}g^{n_{{\scriptscriptstyle 2}}}\in G,\exists\overline{g^{n_{{\scriptscriptstyle 2}}}}\in G\\
=g^{n_{{\scriptscriptstyle 2}}} & g^{-n_{{\scriptscriptstyle 1}}}=g^{n_{{\scriptscriptstyle 2}}-n_{{\scriptscriptstyle 1}}}=g^{\left|n_{{\scriptscriptstyle 2}}-n_{{\scriptscriptstyle 1}}\right|} & n_{{\scriptscriptstyle 1}}<n_{{\scriptscriptstyle 2}}\Rightarrow0<n_{{\scriptscriptstyle 2}}-n_{{\scriptscriptstyle 1}}=\left|n_{{\scriptscriptstyle 2}}-n_{{\scriptscriptstyle 1}}\right|\\
g^{\left|n_{{\scriptscriptstyle 2}}-n_{{\scriptscriptstyle 1}}\right|} & =e\\
 & \downarrow\\
\mathrm{ord}\left\langle g\right\rangle  & \overset{\text{def.}}{=}\min\left\{ \left|n_{{\scriptscriptstyle 2}}-n_{{\scriptscriptstyle 1}}\right|\right\} \in\mathbb{N} & \min\left\{ \left|n_{{\scriptscriptstyle 2}}-n_{{\scriptscriptstyle 1}}\right|\right\} \text{ is the order of }\left\langle g\right\rangle \\
\left\langle g\right\rangle  & =\left\{ g^{1},g^{2},\cdots,g^{\mathrm{ord}\left\langle g\right\rangle },\cdots\right\} =\left\{ g,g^{2},\cdots,e,\cdots\right\}  & g^{\mathrm{ord}\left\langle g\right\rangle }=e
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left\langle g\right\rangle = & \left\{ g^{1},g^{2},\cdots,g^{\mathrm{ord}\left\langle g\right\rangle -1},g^{\mathrm{ord}\left\langle g\right\rangle },g^{\mathrm{ord}\left\langle g\right\rangle +1},\cdots\right\} =\left\{ g,g^{2},\cdots,\overline{g},e,g,\cdots\right\} \\
= & \left\{ g^{1},g^{2},\cdots,g^{\mathrm{ord}\left\langle g\right\rangle -1},g^{\mathrm{ord}\left\langle g\right\rangle }\right\} =\left\{ g,g^{2},\cdots,\overline{g},e\right\} \\
\exists\overline{g^{k}}=g^{\mathrm{ord}\left\langle g\right\rangle -k} & \left[g^{k}g^{\mathrm{ord}\left\langle g\right\rangle -k}=g^{k+\mathrm{ord}\left\langle g\right\rangle -k}=g^{\mathrm{ord}\left\langle g\right\rangle }=e\right],\forall k\in\left\{ 1,\cdots,\mathrm{ord}\left\langle g\right\rangle \right\} =\mathbb{N}_{\le\mathrm{ord}\left\langle g\right\rangle }
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left\langle g\right\rangle \subseteq & G\\
g^{n_{{\scriptscriptstyle 1}}}g^{n_{{\scriptscriptstyle 2}}}= & g^{n_{{\scriptscriptstyle 1}}+n_{{\scriptscriptstyle 2}}}\in\left\langle g\right\rangle  & \forall n_{{\scriptscriptstyle 1}},n_{{\scriptscriptstyle 2}}\in\mathbb{N}\left[n_{{\scriptscriptstyle 1}}+n_{{\scriptscriptstyle 2}}\in\mathbb{N}\right]\\
\exists\overline{g^{k}}=g^{\mathrm{ord}\left\langle g\right\rangle -k}\Biggl[\overline{g^{k}}g^{k}= & g^{\mathrm{ord}\left\langle g\right\rangle -k}g^{k}=g^{\mathrm{ord}\left\langle g\right\rangle }=e\Biggr] & \forall k\in\left\{ 1,\cdots,\mathrm{ord}\left\langle g\right\rangle \right\} =\mathbb{N}_{\le\mathrm{ord}\left\langle g\right\rangle }\\
\Downarrow\\
\left\langle g\right\rangle \le & G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Definition
order of a group element
\begin_inset Formula 
\[
\forall g\in G\text{ is a group},\forall m\in\mathbb{N}\left[g^{m}=e=e_{{\scriptscriptstyle G}}\Rightarrow\exists!\min\left\{ m\right\} \in\mathbb{N}\left[\mathrm{ord}g=\min\left\{ m\right\} \right]\right]
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left\langle g\right\rangle = & \left\{ g^{1},g^{2},\cdots,g^{\mathrm{ord}\left\langle g\right\rangle -1},g^{\mathrm{ord}\left\langle g\right\rangle },g^{\mathrm{ord}\left\langle g\right\rangle +1},\cdots\right\} =\left\{ g,g^{2},\cdots,\overline{g},e,g,\cdots\right\} \\
= & \left\{ g^{1},g^{2},\cdots,g^{\mathrm{ord}\left\langle g\right\rangle -1},g^{\mathrm{ord}\left\langle g\right\rangle }\right\} =\left\{ g,g^{2},\cdots,\overline{g},e\right\} \\
= & \left\{ g,g^{2},\cdots,e\right\} =\left\{ g,g^{2},\cdots,g^{\min\left\{ m\right\} }\right\} =\left\{ g,g^{2},\cdots,g^{\mathrm{ord}g}\right\} \\
\left|\left\langle g\right\rangle \right|= & \left|\left\{ g,g^{2},\cdots,g^{\mathrm{ord}g}\right\} \right|=\left|\left\{ g,g^{2},\cdots,g^{\mathrm{ord}\left\langle g\right\rangle }\right\} \right|\\
= & \mathrm{ord}g=\mathrm{ord}\left\langle g\right\rangle  & \left\langle g\right\rangle \ne\emptyset=\left\{ \right\} \\
\mathrm{ord}\left\langle g\right\rangle = & \mathrm{ord}g=\left|\left\langle g\right\rangle \right|\in\mathbb{N} & \left|\left\langle g\right\rangle \right|\in\mathbb{N}\ne\mathbb{Z}_{{\scriptscriptstyle \ge0}}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 4}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 4}}= & \left\{ 0,1,2,3\right\}  & =\mathbb{Z}_{{\scriptscriptstyle \left[0,4\right)}}=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)\\
= & \left\{ e,g,g^{2},g^{3}\right\}  & =\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot_{G}\right)\\
= & \left\{ g,g^{2},g^{3},e\right\} \\
= & \left\{ g^{1},g^{2},g^{3},g^{4}\right\} =\left\langle g\right\rangle  & g^{4}=e\\
= & \left\{ a^{1},a^{2},a^{3},a^{4}\right\} \\
= & \left\{ a^{1},a^{2},a^{3},e\right\}  & a^{4}=e\\
= & \left\{ e,a,a^{2},a^{3}\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathrm{ord}$
\end_inset


\begin_inset Formula 
\[
\begin{array}{cccccc}
g=e & g^{1}=e^{1}=e & \mathrm{ord}g=\mathrm{ord}e=1\\
\prec\\
g\ne e & g^{1}=g\ne e & g^{2}\ne e & g^{3}\ne e & g^{4}=e & \mathrm{ord}g=4\\
g^{2}\ne e & \left(g^{2}\right)^{1}=g^{2}\ne e & \left(g^{2}\right)^{2}=g^{4}=e & \mathrm{ord}g^{2}=2\\
g^{3}\ne e & \left(g^{3}\right)^{1}=g^{3}\ne e & \begin{aligned}\left(g^{3}\right)^{2}= & g^{6}\\
= & g^{4}g^{2}\\
= & eg^{2}\\
= & g^{2}\ne e
\end{aligned}
 & \begin{aligned}\left(g^{3}\right)^{3}= & g^{9}\\
= & \left(g^{4}\right)^{2}g\\
= & e^{2}g\\
= & eg\\
= & g\ne e
\end{aligned}
 & \begin{aligned}\left(g^{3}\right)^{4}= & \left(g^{4}\right)^{3}\\
= & e^{3}=e
\end{aligned}
 & \mathrm{ord}g^{3}=4\\
\prec\\
g^{4}=e & \left(g^{4}\right)^{1}=e^{1}=e & \mathrm{ord}g^{4}=\mathrm{ord}e=1
\end{array}
\]

\end_inset


\begin_inset Formula 
\[
\mathrm{ord}\left\langle g\right\rangle =\left|\left\langle g\right\rangle \right|=\mathrm{ord}g=\begin{cases}
1 & g=e\\
\begin{cases}
2 & g^{2}=e\\
4 & g^{2}\ne e
\end{cases} & g\ne e
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Theorem
subgroup intersection
\begin_inset Formula 
\[
\begin{aligned}H_{{\scriptscriptstyle 1}}\le & G\\
H_{{\scriptscriptstyle 2}}\le & G
\end{aligned}
\Leftrightarrow H_{{\scriptscriptstyle 1}},H_{{\scriptscriptstyle 2}}\le G\Rightarrow H_{{\scriptscriptstyle 1}}\cap H_{{\scriptscriptstyle 2}}\le G\Leftrightarrow\left(H_{{\scriptscriptstyle 1}}\cap H_{{\scriptscriptstyle 2}}\right)\le G
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
central subgroup
\begin_inset Formula 
\[
C\left(G\right)=\left\{ c\middle|\begin{aligned} &  & c\in G\\
cg= & gc & \forall g\in G
\end{aligned}
\right\} 
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $C\left(G\right)\le G$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\forall c_{{\scriptscriptstyle 1}},c_{{\scriptscriptstyle 2}}\in C\left(G\right)=\left(C\left(G\right),\cdot\right)=\left(C\left(G\right),\cdot_{{\scriptscriptstyle G}}\right)\left[c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\in C\left(G\right)\right]$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}g\overset{c_{{\scriptscriptstyle 2}}\in C\left(G\right)}{=} & c_{{\scriptscriptstyle 1}}gc_{{\scriptscriptstyle 2}}\overset{c_{{\scriptscriptstyle 1}}\in C\left(G\right)}{=}gc_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}} & \forall g\in G,\forall c_{{\scriptscriptstyle 1}},c_{{\scriptscriptstyle 2}}\in C\left(G\right)\\
c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}g= & gc_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\\
\left(c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\right)g= & g\left(c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\right) & \Rightarrow c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\in C\left(G\right)\\
\Downarrow\\
c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\in & C\left(G\right) & \forall c_{{\scriptscriptstyle 1}},c_{{\scriptscriptstyle 2}}\in C\left(G\right)
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\overline{c}c= & e=e_{{\scriptscriptstyle G}} & \forall g\in G,\forall c\in C\left(G\right),\exists\overline{c}=c^{{\scriptscriptstyle -1}}\in G\\
\overline{c}g\overline{g}c= & \left(\overline{c}g\right)\left(\overline{g}c\right)=\overline{c}\left(g\overline{g}\right)c=\overline{c}ec=\overline{c}c=e & \Rightarrow\left(g^{{\scriptscriptstyle -1}}c\right)^{{\scriptscriptstyle -1}}=\overline{\overline{g}c}=\overline{\left(\overline{g}c\right)}=\overline{c}g=c^{{\scriptscriptstyle -1}}g\quad\left(1\right)\\
g\overline{c}c\overline{g}= & \left(g\overline{c}\right)\left(c\overline{g}\right)=g\left(\overline{c}c\right)\overline{g}=ge\overline{g}=g\overline{g}=e & \Rightarrow\left(cg^{{\scriptscriptstyle -1}}\right)^{{\scriptscriptstyle -1}}=\overline{c\overline{g}}=\overline{\left(c\overline{g}\right)}=g\overline{c}=gc^{{\scriptscriptstyle -1}}\quad\left(2\right)\\
\overline{c}g\overset{\left(1\right)}{=} & \overline{\overline{g}c}\overset{\overline{g}c=c\overline{g}}{=}\overline{c\overline{g}}\overset{\left(2\right)}{=}g\overline{c} & \because c\in C\left(G\right),\overline{g}\in G\therefore\overline{g}c=c\overline{g}\\
\overline{c}g= & g\overline{c} & \Rightarrow\overline{c}\in C\left(G\right)\\
\Downarrow\\
\overline{c}\in & C\left(G\right) & \forall c\in C\left(G\right)
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}C\left(G\right)= & \left\{ c\middle|\begin{aligned} &  & c\in G\\
cg= & gc & \forall g\in G
\end{aligned}
\right\} \subseteq G & C\left(G\right)\subseteq G\\
c_{{\scriptscriptstyle 1}}c_{{\scriptscriptstyle 2}}\in & C\left(G\right) & \forall c_{{\scriptscriptstyle 1}},c_{{\scriptscriptstyle 2}}\in C\left(G\right)\\
\overline{c}\in & C\left(G\right) & \forall c\in C\left(G\right)\\
\Downarrow\\
C\left(G\right)\le & G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Section
coset
\end_layout

\begin_layout Definition
left coset
\begin_inset Formula 
\[
\begin{aligned}H\le & G & G\text{ is a group}\\
\Downarrow\\
gH\overset{\text{def.}}{=} & \left\{ gh\middle|h\in H\right\}  & \forall g\in G\\
\Updownarrow\\
gH\text{ is } & \text{a left coset of }G
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned} &  & G\text{ is a group}\\
gH= & \left\{ gh\middle|h\in H\le G\right\}  & \forall g\in G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
right coset
\begin_inset Formula 
\[
\begin{aligned}H\le & G & G\text{ is a group}\\
\Downarrow\\
Hg\overset{\text{def.}}{=} & \left\{ hg\middle|h\in H\right\}  & \forall g\in G\\
\Updownarrow\\
Hg\text{ is } & \text{a right coset of }G
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned} &  & G\text{ is a group}\\
Hg= & \left\{ hg\middle|h\in H\le G\right\}  & \forall g\in G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 2}}\le\mathbb{Z}_{{\scriptscriptstyle 4}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 4}}= & \left\{ 0,1,2,3\right\}  & =\mathbb{Z}_{{\scriptscriptstyle \left[0,4\right)}}=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)\\
= & \left\{ e,g,g^{2},g^{3}\right\}  & =\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot_{G}\right)\\
= & \left\{ g,g^{2},g^{3},e\right\} \\
= & \left\{ g^{1},g^{2},g^{3},g^{4}\right\} =\left\langle g\right\rangle  & g^{4}=e\\
= & \left\{ a^{1},a^{2},a^{3},a^{4}\right\} \\
= & \left\{ a^{1},a^{2},a^{3},e\right\}  & a^{4}=e\\
= & \left\{ e,a,a^{2},a^{3}\right\} \supset\left\{ e,a^{2}\right\} =\left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{G}\right) & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
\mathbb{Z}_{{\scriptscriptstyle 2}}\le\mathbb{Z}_{{\scriptscriptstyle 4}}\Rightarrow\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ e,a^{2}\right\} \\
e\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ ee,ea^{2}\right\} =\left\{ e,a^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ ae,aa^{2}\right\} =\left\{ a,a^{3}\right\}  & =a\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ a^{2}e,a^{2}a^{2}\right\} =\left\{ a^{2},a^{4}\right\} =\left\{ a^{2},e\right\} =\left\{ e,a^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ a^{3}e,a^{3}a^{2}\right\} =\left\{ a^{3},a^{5}\right\} =\left\{ a^{3},a\right\} =\left\{ a,a^{3}\right\}  & =a\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a^{4}\mathbb{Z}_{{\scriptscriptstyle 2}}= & e\mathbb{Z}_{{\scriptscriptstyle 2}}=\left\{ e,a^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
\mathbb{Z}_{{\scriptscriptstyle 4}}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ g\mathbb{Z}_{{\scriptscriptstyle 2}}\middle|g\in\mathbb{Z}_{{\scriptscriptstyle 4}}\right\} \\
= & \left\{ e\mathbb{Z}_{{\scriptscriptstyle 2}},a\mathbb{Z}_{{\scriptscriptstyle 2}},a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}},a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \mathbb{Z}_{{\scriptscriptstyle 2}},a\mathbb{Z}_{{\scriptscriptstyle 2}}\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\le D_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\recolor{\begin{array}{ccc}
\begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
\rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
\rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}}\\
\rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}}\\
\pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 0}}
\end{array} & \overset{e=\rho_{{\scriptscriptstyle 0}}}{\rightarrow} & \begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}} & e & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
e & e & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
\rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 1}} & \rho_{{\scriptscriptstyle 2}} & e & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}}\\
\rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 2}} & e & \rho_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & e & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}} & e & \rho_{{\scriptscriptstyle 2}}\\
\pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \rho_{{\scriptscriptstyle 2}} & \rho_{{\scriptscriptstyle 1}} & e
\end{array}\\
 &  & e\ne\rho=\rho^{1}=\rho_{{\scriptscriptstyle 1}}\downarrow\rho^{2}=\left(\rho_{{\scriptscriptstyle 1}}\right)^{2}=\rho_{{\scriptscriptstyle 2}}\\
\begin{array}{ccccccc}
+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle 3}}}} & 0 & 1 & 2 & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
0 & 0 & 1 & 2 & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
1 & 1 & 2 & 0 & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}}\\
2 & 2 & 0 & 1 & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & e & \rho^{2} & \rho\\
\pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \rho & e & \rho^{2}\\
\pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \rho^{2} & \rho & e
\end{array} & \overset{\mathbb{Z}_{{\scriptscriptstyle 3}}\leq{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}}{\leftarrow} & \begin{array}{ccccccc}
\cdot_{{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}} & e & \rho & \rho^{2} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
e & e & \rho & \rho^{2} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}}\\
\rho & \rho & \rho^{2} & e & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}}\\
\rho^{2} & \rho^{2} & e & \rho & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 1}}\\
\pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & e & \rho^{2} & \rho\\
\pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \pi_{{\scriptscriptstyle 2}} & \rho & e & \rho^{2}\\
\pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 2}} & \pi_{{\scriptscriptstyle 1}} & \pi_{{\scriptscriptstyle 0}} & \rho^{2} & \rho & e
\end{array}
\end{array}}
\]

\end_inset


\begin_inset Formula 
\[
\recolor{\begin{aligned}D_{{\scriptscriptstyle 3}}= & \left\{ \begin{aligned}\rho_{{\scriptscriptstyle k,n}}\\
\pi_{{\scriptscriptstyle k,n}}
\end{aligned}
\middle|\begin{aligned}\rho_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & -\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & +\cos\dfrac{2\pi}{n}k
\end{bmatrix}\\
\pi_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & +\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & -\cos\dfrac{2\pi}{n}k
\end{bmatrix}
\end{aligned}
\quad\forall k\in\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}\right\}  & n=3\\
= & \left\{ \rho_{{\scriptscriptstyle 0}},\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\left(D_{{\scriptscriptstyle 3}},\cdot\right)=\left(D_{{\scriptscriptstyle 3}},\cdot_{G}\right)\\
= & \left\{ e,\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & e=\rho_{{\scriptscriptstyle 0}}\\
= & \left\{ e,\rho,\rho^{2},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\} \supset\left\{ e,\rho,\rho^{2}\right\} =\mathbb{Z}_{{\scriptscriptstyle 3}} & \begin{aligned}e\ne\rho=\rho^{1}= & \rho_{{\scriptscriptstyle 1}}\\
\rho^{2}=\left(\rho_{{\scriptscriptstyle 1}}\right)^{2}= & \rho_{{\scriptscriptstyle 2}}
\end{aligned}
\\
\mathbb{Z}_{{\scriptscriptstyle 3}}\le D_{{\scriptscriptstyle 3}}\Rightarrow\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ 0,1,2\right\}  & =\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}=\left(\mathbb{Z}_{{\scriptscriptstyle 3}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)\\
= & \left\{ e,g,g^{2}\right\}  & =\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot_{G}\right)\\
= & \left\{ g,g^{2},e\right\} \\
= & \left\{ g^{1},g^{2},g^{3}\right\} =\left\langle g\right\rangle  & g^{3}=e\\
= & \left\{ a^{1},a^{2},a^{3}\right\} \\
= & \left\{ a^{1},a^{2},e\right\}  & a^{3}=e\\
= & \left\{ e,a,a^{2}\right\} \\
= & \left\{ e,\rho,\rho^{2}\right\} =\left(\mathbb{Z}_{{\scriptscriptstyle 3}},\cdot_{{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}}\right)\subset D_{{\scriptscriptstyle 3}} & \rho^{3}=e\\
C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ e,\rho,\rho^{2}\right\} \subset D_{{\scriptscriptstyle 3}}\\
e\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ ee,e\rho,e\rho^{2}\right\} =\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\rho\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \rho e,\rho\rho,\rho\rho^{2}\right\} =\left\{ \rho,\rho^{2},\rho^{3}\right\} =\left\{ \rho,\rho^{2},e\right\} =\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \rho^{2}e,\rho^{2}\rho,\rho^{2}\rho^{2}\right\} =\left\{ \rho^{2},\rho^{3},\rho^{4}\right\} =\left\{ \rho^{2},e,\rho\right\} =\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\rho^{3}\mathbb{Z}_{{\scriptscriptstyle 3}}= & e\mathbb{Z}_{{\scriptscriptstyle 3}}=\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle 0}}e,\pi_{{\scriptscriptstyle 0}}\rho,\pi_{{\scriptscriptstyle 0}}\rho^{2}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}}\rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 0}}\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}}\rho_{{\scriptscriptstyle 2}}\right\}  & \begin{aligned}\pi_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}= & \pi_{{\scriptscriptstyle i-j}}\\
\pi_{{\scriptscriptstyle i}}= & \pi_{{\scriptscriptstyle 3+i}}
\end{aligned}
\\
= & \left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle -1}},\pi_{{\scriptscriptstyle -2}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 1}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle 1}}e,\pi_{{\scriptscriptstyle 1}}\rho,\pi_{{\scriptscriptstyle 1}}\rho^{2}\right\} =\left\{ \pi_{{\scriptscriptstyle 1}}\rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}}\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 1}}\rho_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle -1}}\right\} =\left\{ \pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 2}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle 2}}e,\pi_{{\scriptscriptstyle 2}}\rho,\pi_{{\scriptscriptstyle 2}}\rho^{2}\right\} =\left\{ \pi_{{\scriptscriptstyle 2}}\rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 2}}\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\rho_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \pi_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
D_{{\scriptscriptstyle 3}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ g\mathbb{Z}_{{\scriptscriptstyle 3}}\middle|g\in D_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ e\mathbb{Z}_{{\scriptscriptstyle 3}},\rho\mathbb{Z}_{{\scriptscriptstyle 3}},\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ \mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ e\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\}  & i\in\left\{ 0,1,2\right\} =\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}
\end{aligned}
}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $4\mathbb{Z}\le\mathbb{Z}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}= & \left\{ \cdots,-3,-2,-1,0,+1,+2,+3,\cdots\right\} =\left\{ k\middle|k\in\mathbb{Z}\right\} \\
4\mathbb{Z}= & \left\{ \cdots,-12,-8,-4,0,+4,+8,+12,\cdots\right\} =\left\{ 4k\middle|k\in\mathbb{Z}\right\} \\
4\mathbb{Z}\subset & \mathbb{Z}\\
4k_{{\scriptscriptstyle 1}}+4k_{{\scriptscriptstyle 2}}= & 4\left(k_{{\scriptscriptstyle 1}}+k_{{\scriptscriptstyle 2}}\right)\in4\mathbb{Z}\\
-4k=\overline{4k}= & 4\left(-k\right)\in4\mathbb{Z}\\
\Downarrow\\
4\mathbb{Z}\le & \mathbb{Z}\\
\\5+4\mathbb{Z}= & \left\{ \cdots,5+\left(-12\right),5+\left(-8\right),5+\left(-4\right),5+0,5+4,5+8,5+12,\cdots\right\} \\
= & \left\{ \cdots,-7,-3,1,5,9,13,17,\cdots\right\}  & =1+4\mathbb{Z}\\
4+4\mathbb{Z}= & \left\{ \cdots,4+\left(-12\right),4+\left(-8\right),4+\left(-4\right),4+0,4+4,4+8,4+12,\cdots\right\} \\
= & \left\{ \cdots,-8,-4,0,4,8,12,16,\cdots\right\}  & =0+4\mathbb{Z}\\
3+4\mathbb{Z}= & \left\{ \cdots,3+\left(-12\right),3+\left(-8\right),3+\left(-4\right),3+0,3+4,3+8,3+12,\cdots\right\} \\
= & \left\{ \cdots,-9,-5,-1,3,7,11,15,\cdots\right\}  & =-3+4\mathbb{Z}\\
2+4\mathbb{Z}= & \left\{ \cdots,2+\left(-12\right),2+\left(-8\right),2+\left(-4\right),2+0,2+4,2+8,2+12,\cdots\right\} \\
= & \left\{ \cdots,-10,-6,-2,2,6,10,14,\cdots\right\}  & =-2+4\mathbb{Z}\\
1+4\mathbb{Z}= & \left\{ \cdots,1+\left(-12\right),1+\left(-8\right),1+\left(-4\right),1+0,1+4,1+8,1+12,\cdots\right\} \\
= & \left\{ \cdots,-11,-7,-3,1,5,9,13,\cdots\right\}  & =-1+4\mathbb{Z}\\
0+4\mathbb{Z}= & \left\{ \cdots,0+\left(-12\right),0+\left(-8\right),0+\left(-4\right),0+0,0+4,0+8,0+12,\cdots\right\} \\
= & \left\{ \cdots,-12,-8,-4,0,4,8,12,\cdots\right\}  & =4\mathbb{Z}\\
-1+4\mathbb{Z}= & \left\{ \cdots,-1+\left(-12\right),-1+\left(-8\right),-1+\left(-4\right),-1+0,-1+4,-1+8,-1+12,\cdots\right\} \\
= & \left\{ \cdots,-13,-9,-5,-1,3,7,11,\cdots\right\}  & =3+4\mathbb{Z}\\
-2+4\mathbb{Z}= & \left\{ \cdots,-2+\left(-12\right),-2+\left(-8\right),-2+\left(-4\right),-2+0,-2+4,-2+8,-2+12,\cdots\right\} \\
= & \left\{ \cdots,-14,-10,-6,-2,2,6,10,\cdots\right\}  & =2+4\mathbb{Z}\\
-3+4\mathbb{Z}= & \left\{ \cdots,-3+\left(-12\right),-3+\left(-8\right),-3+\left(-4\right),-3+0,-3+4,-3+8,-3+12,\cdots\right\} \\
= & \left\{ \cdots,-15,-11,-7,-3,1,5,9,\cdots\right\}  & =1+4\mathbb{Z}\\
-4+4\mathbb{Z}= & \left\{ \cdots,-4+\left(-12\right),-4+\left(-8\right),-4+\left(-4\right),-4+0,-4+4,-4+8,-4+12,\cdots\right\} \\
= & \left\{ \cdots,-16,-12,-8,-4,0,4,8,\cdots\right\}  & =0+4\mathbb{Z}\\
-5+4\mathbb{Z}= & \left\{ \cdots,-5+\left(-12\right),-5+\left(-8\right),-5+\left(-4\right),-5+0,-5+4,-5+8,-5+12,\cdots\right\}  & =-1+4\mathbb{Z}\\
= & \left\{ \cdots,-17,-13,-9,-5,-1,3,7,\cdots\right\} \\
\vdots\quad
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{array}{ccccccccc}
\left\{ \cdots,-8,-4,+0,+4,+8,\cdots\right\}  & + & 4\mathbb{Z} & = & \left\{ 4+4\mathbb{Z}\right\}  & = & \left\{ 0+4\mathbb{Z}\right\}  & = & \left\{ 4\mathbb{Z}\right\} \\
\cup &  &  &  & \cup\\
\left\{ \cdots,-9,-5,-1,+3,+7,\cdots\right\}  & + & 4\mathbb{Z} & = & \left\{ 3+4\mathbb{Z}\right\} \\
\cup &  &  &  & \cup\\
\left\{ \cdots,-10,-6,-2,+2,+6,\cdots\right\}  & + & 4\mathbb{Z} & = & \left\{ 2+4\mathbb{Z}\right\} \\
\cup &  &  &  & \cup\\
\left\{ \cdots,-11,-7,-3,+1,+5,\cdots\right\}  & + & 4\mathbb{Z} & = & \left\{ 1+4\mathbb{Z}\right\} \\
\parallel &  &  &  & \parallel\\
\mathbb{Z} & + & 4\mathbb{Z} & = & \begin{array}{ccc}
\{ & 4\mathbb{Z},\\
 & 1+4\mathbb{Z},\\
 & 2+4\mathbb{Z}.\\
 & 3+4\mathbb{Z} & \}
\end{array}
\end{array}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}+4\mathbb{Z}= & \left\{ k+4\mathbb{Z}\middle|k\in\mathbb{Z}\right\} =\left\{ 4\mathbb{Z},1+4\mathbb{Z},2+4\mathbb{Z}.3+4\mathbb{Z}\right\} \\
= & \left\{ \cdots,-3+4\mathbb{Z},-2+4\mathbb{Z},-1+4\mathbb{Z},0+4\mathbb{Z},1+4\mathbb{Z},2+4\mathbb{Z},3+4\mathbb{Z},\cdots\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $L_{{\scriptscriptstyle g}}$
\end_inset

:
 left shift
\begin_inset Formula 
\[
H\overset{L_{{\scriptscriptstyle g}}}{\rightarrow}gH
\]

\end_inset


\end_layout

\begin_layout Theorem
according to the rearrangement theorem
\begin_inset Formula 
\[
\begin{aligned}H\le & G\\
H= & G
\end{aligned}
\Rightarrow\begin{aligned}gH= & H & \forall g\in G\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
H\le G\Rightarrow\begin{aligned}hH= & H & \forall h\in H\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
H\le G\Rightarrow\begin{aligned}gH= & H & \forall g\in H\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
H\le G\Rightarrow\left\{ \forall g\in H\left[gH=H\right]\right\} 
\]

\end_inset


\begin_inset Formula 
\[
\begin{array}{ccc}
g\in H\Rightarrow gH=H & \Leftrightarrow & gH\ne H\Rightarrow g\notin H\\
 & \text{let }H\le G\\
gH=H\Rightarrow g\in H & \Leftrightarrow & g\notin H\Rightarrow gH\ne H\\
g\cdot e=ge\in H\\
 & H\le G\\
 & \Downarrow\\
 & g\in H\Leftrightarrow gH=H\\
 & \Updownarrow\\
 & g\notin H\Leftrightarrow gH\ne H
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
\begin{array}{ccc}
H\le G & \wedge & g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in G\\
 & \Downarrow\\
g_{{\scriptscriptstyle 1}}H=g_{{\scriptscriptstyle 2}}H & \Leftrightarrow & \overline{g}_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 1}}\in H\\
 & \Updownarrow\\
g_{{\scriptscriptstyle 1}}H\ne g_{{\scriptscriptstyle 2}}H & \Leftrightarrow & \overline{g}_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 1}}\notin H
\end{array}
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
\begin{aligned}g_{{\scriptscriptstyle 1}}H= & g_{{\scriptscriptstyle 2}}H & \exists e=e_{{\scriptscriptstyle H}}=e_{{\scriptscriptstyle G}}\in H\subseteq G\\
g_{{\scriptscriptstyle 1}}=g_{{\scriptscriptstyle 1}}\cdot e=g_{{\scriptscriptstyle 1}}e= & g_{{\scriptscriptstyle 2}}h & \exists h\in H\\
g_{{\scriptscriptstyle 1}}= & g_{{\scriptscriptstyle 2}}h\\
\overline{g}_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 1}}= & \overline{g}_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 2}}h=eh=h\\
\overline{g}_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 1}}= & h\in H & \overline{g}_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 1}}\in H
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\overline{g_{{\scriptscriptstyle 2}}}g_{{\scriptscriptstyle 1}}\in H\Rightarrow g_{{\scriptscriptstyle 1}}H=g_{{\scriptscriptstyle 2}}H
\]

\end_inset


\end_layout

\begin_layout Theorem
coset mutually exclusive theorem
\begin_inset Formula 
\[
\begin{array}{ccc}
H\le G & \wedge & g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in G\\
 & \Downarrow\\
g_{{\scriptscriptstyle 1}}H\ne g_{{\scriptscriptstyle 2}}H & \Rightarrow & g_{{\scriptscriptstyle 1}}H\cap g_{{\scriptscriptstyle 2}}H=\emptyset
\end{array}
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
\begin{aligned}g_{{\scriptscriptstyle 1}}H\cap g_{{\scriptscriptstyle 2}}H\ne & \emptyset\\
\Downarrow\\
\exists g_{{\scriptscriptstyle 1}}h_{{\scriptscriptstyle 1}}=g_{{\scriptscriptstyle 2}}h_{{\scriptscriptstyle 2}}\in & g_{{\scriptscriptstyle 1}}H\cap g_{{\scriptscriptstyle 2}}H\\
\Downarrow\\
g_{{\scriptscriptstyle 1}}h_{{\scriptscriptstyle 1}}= & g_{{\scriptscriptstyle 2}}h_{{\scriptscriptstyle 2}}\\
g_{{\scriptscriptstyle 1}}=g_{{\scriptscriptstyle 1}}e=g_{{\scriptscriptstyle 1}}h_{{\scriptscriptstyle 1}}\overline{h}_{{\scriptscriptstyle 1}}= & g_{{\scriptscriptstyle 2}}h_{{\scriptscriptstyle 2}}\overline{h}_{{\scriptscriptstyle 1}}\\
g_{{\scriptscriptstyle 1}}= & g_{{\scriptscriptstyle 2}}h_{{\scriptscriptstyle 2}}\overline{h}_{{\scriptscriptstyle 1}}\\
\Downarrow\\
g_{{\scriptscriptstyle 1}}H=\left\{ g_{{\scriptscriptstyle 1}}h\middle|h\in H\right\} = & \left\{ g_{{\scriptscriptstyle 2}}h_{{\scriptscriptstyle 2}}\overline{h}_{{\scriptscriptstyle 1}}h\middle|h\in H\right\} \\
= & \left\{ g_{{\scriptscriptstyle 2}}\left(h_{{\scriptscriptstyle 2}}\overline{h}_{{\scriptscriptstyle 1}}h\right)\middle|h\in H\right\}  & \widetilde{h}=h_{{\scriptscriptstyle 2}}\overline{h}_{{\scriptscriptstyle 1}}h\in H\\
\overset{\text{rearrangement theorem}}{=} & \left\{ g_{{\scriptscriptstyle 2}}\widetilde{h}\middle|\widetilde{h}\in H\right\} =g_{{\scriptscriptstyle 2}}H & \Rightarrow\Leftarrow g_{{\scriptscriptstyle 1}}H\ne g_{{\scriptscriptstyle 2}}H
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
coset partitioning group
\begin_inset Formula 
\[
\begin{aligned}H\le & G\\
g_{{\scriptscriptstyle j}}\in & G\\
\Downarrow\\
G=\bigcup_{g\in G}gH= & \bigcup_{j}g_{{\scriptscriptstyle j}}H=\begin{cases}
\bigcup\limits_{j=1}^{n}g_{{\scriptscriptstyle j}}H & \left|G\right|=n\in\mathbb{N}\\
\bigcup\limits_{j\in J}g_{{\scriptscriptstyle j}}H & \left|G\right|=\left|J\right|,J\in\left\{ \mathbb{N},\mathbb{Z},\left[0,1\right],\mathbb{R},\cdots\right\} 
\end{cases}\\
= & \begin{cases}
g_{{\scriptscriptstyle 1}}H\cup g_{{\scriptscriptstyle 2}}H\cup\cdots\cup g_{{\scriptscriptstyle n}}H & \left|G\right|=n\in\mathbb{N}\\
\bigcup\limits_{j\in J}g_{{\scriptscriptstyle j}}H & \left|G\right|=\left|J\right|,J\in\left\{ \mathbb{N},\mathbb{Z},\left[0,1\right],\mathbb{R},\cdots\right\} 
\end{cases} & \exists m\in\mathbb{N}_{{\scriptscriptstyle \le n}}\left[g_{{\scriptscriptstyle m}}=e=e_{{\scriptscriptstyle H}}=e_{{\scriptscriptstyle G}}\right]\\
= & \begin{cases}
g_{{\scriptscriptstyle 1}}H\cup g_{{\scriptscriptstyle 2}}H\cup\cdots & \left|G\right|=n\in\mathbb{N}\\
\bigcup\limits_{j\in J}g_{{\scriptscriptstyle j}}H & J\in\left\{ \mathbb{N},\mathbb{Z},\left[0,1\right],\mathbb{R},\cdots\right\} 
\end{cases} & \wedge g_{{\scriptscriptstyle i}}H\ne g_{{\scriptscriptstyle j}}H\Rightarrow g_{{\scriptscriptstyle i}}H\cap g_{{\scriptscriptstyle j}}H=\emptyset
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard

\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 2}}\le\mathbb{Z}_{{\scriptscriptstyle 4}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 4}}= & \left\{ 0,1,2,3\right\}  & =\mathbb{Z}_{{\scriptscriptstyle \left[0,4\right)}}=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)\\
= & \left\{ e,g,g^{2},g^{3}\right\}  & =\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot_{G}\right)\\
= & \left\{ g,g^{2},g^{3},e\right\} \\
= & \left\{ g^{1},g^{2},g^{3},g^{4}\right\} =\left\langle g\right\rangle  & g^{4}=e\\
= & \left\{ a^{1},a^{2},a^{3},a^{4}\right\} \\
= & \left\{ a^{1},a^{2},a^{3},e\right\}  & a^{4}=e\\
= & \left\{ e,a,a^{2},a^{3}\right\} \supset\left\{ e,a^{2}\right\} =\left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{G}\right) & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
\mathbb{Z}_{{\scriptscriptstyle 2}}\le\mathbb{Z}_{{\scriptscriptstyle 4}}\Rightarrow\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ e,a^{2}\right\} \\
e\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ ee,ea^{2}\right\} =\left\{ e,a^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ ae,aa^{2}\right\} =\left\{ a,a^{3}\right\}  & =a\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ a^{2}e,a^{2}a^{2}\right\} =\left\{ a^{2},a^{4}\right\} =\left\{ a^{2},e\right\} =\left\{ e,a^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ a^{3}e,a^{3}a^{2}\right\} =\left\{ a^{3},a^{5}\right\} =\left\{ a^{3},a\right\} =\left\{ a,a^{3}\right\}  & =a\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a^{4}\mathbb{Z}_{{\scriptscriptstyle 2}}= & e\mathbb{Z}_{{\scriptscriptstyle 2}}=\left\{ e,a^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
\mathbb{Z}_{{\scriptscriptstyle 4}}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ g\mathbb{Z}_{{\scriptscriptstyle 2}}\middle|\forall g\in\mathbb{Z}_{{\scriptscriptstyle 4}}\right\} \\
= & \left\{ e\mathbb{Z}_{{\scriptscriptstyle 2}},a\mathbb{Z}_{{\scriptscriptstyle 2}},a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}},a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \mathbb{Z}_{{\scriptscriptstyle 2}},a\mathbb{Z}_{{\scriptscriptstyle 2}}\right\} \\
\mathbb{Z}_{{\scriptscriptstyle 4}}=\bigcup_{g\in\mathbb{Z}_{{\scriptscriptstyle 4}}}g\mathbb{Z}_{{\scriptscriptstyle 2}}= & e\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}} & \begin{aligned}e\mathbb{Z}_{{\scriptscriptstyle 2}}=a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \mathbb{Z}_{{\scriptscriptstyle 2}}\\
a\mathbb{Z}_{{\scriptscriptstyle 2}}=a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}= & a\mathbb{Z}_{{\scriptscriptstyle 2}}
\end{aligned}
\\
= & \mathbb{Z}_{{\scriptscriptstyle 2}}\cup a\mathbb{Z}_{{\scriptscriptstyle 2}}\\
= & e\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a\mathbb{Z}_{{\scriptscriptstyle 2}}=e\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}=a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a\mathbb{Z}_{{\scriptscriptstyle 2}}=a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\le D_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\recolor{\begin{aligned}D_{{\scriptscriptstyle 3}}= & \left\{ \begin{aligned}\rho_{{\scriptscriptstyle k,n}}\\
\pi_{{\scriptscriptstyle k,n}}
\end{aligned}
\middle|\begin{aligned}\rho_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & -\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & +\cos\dfrac{2\pi}{n}k
\end{bmatrix}\\
\pi_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & +\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & -\cos\dfrac{2\pi}{n}k
\end{bmatrix}
\end{aligned}
\quad\forall k\in\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}\right\}  & n=3\\
= & \left\{ \rho_{{\scriptscriptstyle 0}},\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\left(D_{{\scriptscriptstyle 3}},\cdot\right)=\left(D_{{\scriptscriptstyle 3}},\cdot_{G}\right)\\
= & \left\{ e,\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & e=\rho_{{\scriptscriptstyle 0}}\\
= & \left\{ e,\rho,\rho^{2},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\} \supset\left\{ e,\rho,\rho^{2}\right\} =\mathbb{Z}_{{\scriptscriptstyle 3}} & \begin{aligned}e\ne\rho=\rho^{1}= & \rho_{{\scriptscriptstyle 1}}\\
\rho^{2}=\left(\rho_{{\scriptscriptstyle 1}}\right)^{2}= & \rho_{{\scriptscriptstyle 2}}
\end{aligned}
\\
\mathbb{Z}_{{\scriptscriptstyle 3}}\le D_{{\scriptscriptstyle 3}}\Rightarrow\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ 0,1,2\right\}  & =\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}=\left(\mathbb{Z}_{{\scriptscriptstyle 3}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)\\
= & \left\{ e,g,g^{2}\right\}  & =\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot_{G}\right)\\
= & \left\{ g,g^{2},e\right\} \\
= & \left\{ g^{1},g^{2},g^{3}\right\} =\left\langle g\right\rangle  & g^{3}=e\\
= & \left\{ a^{1},a^{2},a^{3}\right\} \\
= & \left\{ a^{1},a^{2},e\right\}  & a^{3}=e\\
= & \left\{ e,a,a^{2}\right\} \\
= & \left\{ e,\rho,\rho^{2}\right\} =\left(\mathbb{Z}_{{\scriptscriptstyle 3}},\cdot_{{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}}\right)\subset D_{{\scriptscriptstyle 3}} & \rho^{3}=e\\
C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ e,\rho,\rho^{2}\right\} \subset D_{{\scriptscriptstyle 3}}\\
e\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ ee,e\rho,e\rho^{2}\right\} =\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\rho\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \rho e,\rho\rho,\rho\rho^{2}\right\} =\left\{ \rho,\rho^{2},\rho^{3}\right\} =\left\{ \rho,\rho^{2},e\right\} =\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \rho^{2}e,\rho^{2}\rho,\rho^{2}\rho^{2}\right\} =\left\{ \rho^{2},\rho^{3},\rho^{4}\right\} =\left\{ \rho^{2},e,\rho\right\} =\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\rho^{3}\mathbb{Z}_{{\scriptscriptstyle 3}}= & e\mathbb{Z}_{{\scriptscriptstyle 3}}=\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle 0}}e,\pi_{{\scriptscriptstyle 0}}\rho,\pi_{{\scriptscriptstyle 0}}\rho^{2}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}}\rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 0}}\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}}\rho_{{\scriptscriptstyle 2}}\right\}  & \begin{aligned}\pi_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}= & \pi_{{\scriptscriptstyle i-j}}\\
\pi_{{\scriptscriptstyle i}}= & \pi_{{\scriptscriptstyle 3+i}}
\end{aligned}
\\
= & \left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle -1}},\pi_{{\scriptscriptstyle -2}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 1}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle 1}}e,\pi_{{\scriptscriptstyle 1}}\rho,\pi_{{\scriptscriptstyle 1}}\rho^{2}\right\} =\left\{ \pi_{{\scriptscriptstyle 1}}\rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}}\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 1}}\rho_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle -1}}\right\} =\left\{ \pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 2}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle 2}}e,\pi_{{\scriptscriptstyle 2}}\rho,\pi_{{\scriptscriptstyle 2}}\rho^{2}\right\} =\left\{ \pi_{{\scriptscriptstyle 2}}\rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 2}}\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\rho_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \pi_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
D_{{\scriptscriptstyle 3}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ g\mathbb{Z}_{{\scriptscriptstyle 3}}\middle|g\in D_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ e\mathbb{Z}_{{\scriptscriptstyle 3}},\rho\mathbb{Z}_{{\scriptscriptstyle 3}},\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ \mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ e\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\}  & i\in\left\{ 0,1,2\right\} =\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}\\
D_{{\scriptscriptstyle 3}}=\bigcup_{g\in D_{{\scriptscriptstyle 3}}}g\mathbb{Z}_{{\scriptscriptstyle 3}}= & e\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\rho\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}} & \begin{aligned}e\mathbb{Z}_{{\scriptscriptstyle 3}}=\rho\mathbb{Z}_{{\scriptscriptstyle 3}}=\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}=\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}}=\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}}=\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}
\end{aligned}
\\
= & \mathbb{Z}_{{\scriptscriptstyle 3}}\cup\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}} & i\in\left\{ 0,1,2\right\} =\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}\\
= & \cdots & 3\cdot3=9\text{ possibilities of combinations}
\end{aligned}
}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $4\mathbb{Z}\le\mathbb{Z}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}+4\mathbb{Z}= & \left\{ k+4\mathbb{Z}\middle|k\in\mathbb{Z}\right\} =\left\{ 4\mathbb{Z},1+4\mathbb{Z},2+4\mathbb{Z}.3+4\mathbb{Z}\right\} \\
= & \left\{ \cdots,-3+4\mathbb{Z},-2+4\mathbb{Z},-1+4\mathbb{Z},0+4\mathbb{Z},1+4\mathbb{Z},2+4\mathbb{Z},3+4\mathbb{Z},\cdots\right\} 
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\mathbb{Z}=\bigcup_{k\in\mathbb{Z}}\left(k+4\mathbb{Z}\right)=\left(4\mathbb{Z}\right)\cup\left(1+4\mathbb{Z}\right)\cup\left(2+4\mathbb{Z}\right)\cup\left(3+4\mathbb{Z}\right)
\]

\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
\begin{aligned}H\le & G\\
g\ne & e=e_{{\scriptscriptstyle H}}=e_{{\scriptscriptstyle G}}\\
gH= & \left\{ gh\middle|h\in H\right\}  & \forall g\in G\\
\Downarrow\\
\left|gH\right|= & \left|H\right|
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
coset theorem = Langrange coset theorem = Lagrange theorem
\end_layout

\begin_layout Theorem
bridge between group theory and number theory 
\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
\begin{aligned}H\le & G\\
\left|G\right|< & \infty\\
\Downarrow &  & \begin{aligned}\left|gH\right|=\left|H\right|\\
G=\bigcup_{g\in G}gH\\
g_{{\scriptscriptstyle i}}H\ne g_{{\scriptscriptstyle j}}H\Rightarrow g_{{\scriptscriptstyle i}}H\cap g_{{\scriptscriptstyle j}}H=\emptyset
\end{aligned}
\\
\left|G\right|/ & \left|H\right|\in\mathbb{N} & \Rightarrow\left|H\right|\mid\left|G\right|\Leftrightarrow\left|G\right|=n\left|H\right|\quad n\in\mathbb{N}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group}\\
\left|G\right|= & p\in\mathbb{P}
\end{aligned}
\Rightarrow\left\{ H\middle|H\le G\right\} =\left\{ \left\{ e\right\} ,G\right\} 
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group}\\
\left|G\right|= & p\in\mathbb{P}\\
g\ne & e=e_{{\scriptscriptstyle G}}\\
\left\langle g\right\rangle = & \left\{ g^{n}\middle|n\in\mathbb{N}\right\} \\
\Downarrow\\
\left\langle g\right\rangle \le & G\\
\left\langle g\right\rangle \in & \left\{ H\middle|H\le G\right\} =\left\{ \left\{ e\right\} ,G\right\} \\
\left\langle g\right\rangle \ne & \left\{ e\right\} \\
\Downarrow\\
\left\langle g\right\rangle = & G\\
\Downarrow & \mathrm{ord}\left\langle g\right\rangle =\left|\left\langle g\right\rangle \right|\\
\mathrm{ord}\left\langle g\right\rangle = & \mathrm{ord}G\\
= & \left|\left\langle g\right\rangle \right|=\left|G\right|=p\in\mathbb{P}\\
\mathrm{ord}\left\langle g\right\rangle = & p\in\mathbb{P}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Subsection
left coset space
\end_layout

\begin_layout Definition
indexed set
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}\left\{ a_{{\scriptscriptstyle i}}\right\} _{{\scriptscriptstyle i\in I}}= & \left\{ a_{{\scriptscriptstyle i}}\middle|i\in I\right\} =\left\{ \cdots,a_{{\scriptscriptstyle i}},\cdots\right\}  & i\in I\\
= & \bigcup\limits_{i\in I}\left\{ a_{{\scriptscriptstyle i}}\right\}  & i\in I\\
= & \begin{cases}
\bigcup\limits_{i=1}^{n}\left\{ a_{{\scriptscriptstyle i}}\right\}  & \left|\left\{ a_{{\scriptscriptstyle i}}\right\} _{{\scriptscriptstyle i\in I}}\right|=n\in\mathbb{N}\\
\bigcup\limits_{i\in I}\left\{ a_{{\scriptscriptstyle i}}\right\}  & I\in\left\{ \mathbb{N},\mathbb{Z},\left[0,1\right],\mathbb{R},\cdots\right\} 
\end{cases}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left\{ a_{{\scriptscriptstyle i}}\right\} _{{\scriptscriptstyle i\in\mathbb{N}}}= & \left\{ a_{{\scriptscriptstyle i}}\middle|i\in\mathbb{N}\right\} =\left\{ a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},\cdots,a_{{\scriptscriptstyle i}},\cdots\right\} =\left\{ a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},a_{{\scriptscriptstyle 3}},\cdots\right\}  & i\in\mathbb{N}\\
=\left\{ a_{{\scriptscriptstyle n}}\right\} _{{\scriptscriptstyle n\in\mathbb{N}}}= & \left\{ a_{{\scriptscriptstyle n}}\middle|n\in\mathbb{N}\right\} =\left\{ a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},\cdots,a_{{\scriptscriptstyle n}},\cdots\right\} =\left\{ a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},a_{{\scriptscriptstyle 3}},\cdots\right\}  & n\in\mathbb{N}\\
= & \bigcup\limits_{i\in\mathbb{N}}\left\{ a_{{\scriptscriptstyle i}}\right\} =\bigcup\limits_{n\in\mathbb{N}}\left\{ a_{{\scriptscriptstyle n}}\right\} =\begin{cases}
\bigcup\limits_{i=1}^{n}\left\{ a_{{\scriptscriptstyle i}}\right\}  & \left|\left\{ a_{{\scriptscriptstyle i}}\right\} _{{\scriptscriptstyle i\in\mathbb{N}}}\right|=n\in\mathbb{N}\\
\bigcup\limits_{n\in\mathbb{N}}\left\{ a_{{\scriptscriptstyle n}}\right\}  & \left|\left\{ a_{{\scriptscriptstyle i}}\right\} _{{\scriptscriptstyle i\in\mathbb{N}}}\right|=\left|\mathbb{N}\right|
\end{cases}\\
\left\{ a_{{\scriptscriptstyle i}}\right\} _{{\scriptscriptstyle i=1}}^{{\scriptscriptstyle n}}= & \bigcup\limits_{i=1}^{n}\left\{ a_{{\scriptscriptstyle i}}\right\} =\left\{ a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},\cdots,a_{{\scriptscriptstyle n}}\right\} =\left\{ a_{{\scriptscriptstyle 1}},\cdots,a_{{\scriptscriptstyle n}}\right\}  & \forall n\in\mathbb{N}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left\{ a_{{\scriptscriptstyle i}}\right\} _{{\scriptscriptstyle i\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}}= & \left\{ a_{{\scriptscriptstyle i}}\middle|i\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\right\} =\left\{ a_{{\scriptscriptstyle 0}},a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},\cdots,a_{{\scriptscriptstyle i}},\cdots\right\} =\left\{ a_{{\scriptscriptstyle 0}},a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},a_{{\scriptscriptstyle 3}},\cdots\right\}  & i\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
=\left\{ a_{{\scriptscriptstyle k}}\right\} _{{\scriptscriptstyle k\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}}= & \left\{ a_{{\scriptscriptstyle k}}\middle|k\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\right\} =\left\{ a_{{\scriptscriptstyle 0}},a_{{\scriptscriptstyle 1}},\cdots,a_{{\scriptscriptstyle k}},\cdots\right\} =\left\{ a_{{\scriptscriptstyle 0}},a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},\cdots\right\}  & k\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
=\left\{ a_{{\scriptscriptstyle \mu}}\right\} _{{\scriptscriptstyle \mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}}= & \left\{ a_{{\scriptscriptstyle \mu}}\middle|\mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\right\} =\left\{ a_{{\scriptscriptstyle 0}},a_{{\scriptscriptstyle 1}},\cdots,a_{{\scriptscriptstyle \mu}},\cdots\right\} =\left\{ a_{{\scriptscriptstyle 0}},a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},\cdots\right\}  & \mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
= & \bigcup\limits_{i\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}\left\{ a_{{\scriptscriptstyle i}}\right\} =\bigcup\limits_{\mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}\left\{ a_{{\scriptscriptstyle \mu}}\right\} =\begin{cases}
\bigcup\limits_{\mu=0}^{n-1}\left\{ a_{{\scriptscriptstyle \mu}}\right\}  & \left|\left\{ a_{{\scriptscriptstyle \mu}}\right\} _{{\scriptscriptstyle \mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}}\right|=n\in\mathbb{N}\\
\bigcup\limits_{\mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}\left\{ a_{{\scriptscriptstyle \mu}}\right\}  & \left|\left\{ a_{{\scriptscriptstyle \mu}}\right\} _{{\scriptscriptstyle \mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}}\right|=\left|\mathbb{Z}_{{\scriptscriptstyle \ge0}}\right|=\left|\mathbb{N}\right|
\end{cases}\\
\\\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Definition
indexed sequence
\begin_inset Formula 
\[
\begin{aligned}\left\langle a_{{\scriptscriptstyle i}}\right\rangle _{{\scriptscriptstyle i\in I}}= & \left\langle a_{{\scriptscriptstyle i}}\middle|i\in I\right\rangle =\left\langle \cdots,a_{{\scriptscriptstyle i}},\cdots\right\rangle  & i\in I\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left\langle a_{{\scriptscriptstyle i}}\right\rangle _{{\scriptscriptstyle i\in\mathbb{N}}}= & \left\langle a_{{\scriptscriptstyle i}}\middle|i\in\mathbb{N}\right\rangle =\left\langle a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},\cdots,a_{{\scriptscriptstyle i}},\cdots\right\rangle =\left\langle a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},a_{{\scriptscriptstyle 3}},\cdots\right\rangle  & i\in\mathbb{N}\\
=\left\langle a_{{\scriptscriptstyle n}}\right\rangle _{{\scriptscriptstyle n\in\mathbb{N}}}= & \left\langle a_{{\scriptscriptstyle n}}\middle|n\in\mathbb{N}\right\rangle =\left\langle a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},\cdots,a_{{\scriptscriptstyle n}},\cdots\right\rangle =\left\langle a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},a_{{\scriptscriptstyle 3}},\cdots\right\rangle  & n\in\mathbb{N}\\
= & \left\langle \underset{i\in\mathbb{N}}{,}a_{{\scriptscriptstyle i}}\right\rangle =\left\langle \underset{n\in\mathbb{N}}{,}a_{{\scriptscriptstyle n}}\right\rangle =\begin{cases}
\left\langle \underset{i=1}{\overset{n}{,}}a_{{\scriptscriptstyle i}}\right\rangle  & \left|\left\langle a_{{\scriptscriptstyle i}}\right\rangle _{{\scriptscriptstyle i\in\mathbb{N}}}\right|=n\in\mathbb{N}\\
\left\langle \underset{i\in\mathbb{N}}{,}a_{{\scriptscriptstyle i}}\right\rangle  & \left|\left\langle a_{{\scriptscriptstyle i}}\right\rangle _{{\scriptscriptstyle i\in\mathbb{N}}}\right|=\left|\mathbb{N}\right|
\end{cases}\\
\left\langle a_{{\scriptscriptstyle i}}\right\rangle _{{\scriptscriptstyle i=1}}^{{\scriptscriptstyle n}}= & \left\langle \underset{i=1}{\overset{n}{,}}a_{{\scriptscriptstyle i}}\right\rangle =\left\langle a_{{\scriptscriptstyle 1}},a_{{\scriptscriptstyle 2}},\cdots,a_{{\scriptscriptstyle n}}\right\rangle =\left\langle a_{{\scriptscriptstyle 1}},\cdots,a_{{\scriptscriptstyle n}}\right\rangle  & \forall n\in\mathbb{N}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
indexed tuple
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}\left(x_{{\scriptscriptstyle i}}\right)_{{\scriptscriptstyle i\in I}}= & \left(x_{{\scriptscriptstyle i}}\middle|i\in I\right)=\left(\cdots,x_{{\scriptscriptstyle i}},\cdots\right) & i\in I\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\boldsymbol{x}=x_{{\scriptscriptstyle i}}=\left(x_{{\scriptscriptstyle i}}\right)_{{\scriptscriptstyle i\in\mathbb{N}}}= & \left(x_{{\scriptscriptstyle i}}\middle|i\in\mathbb{N}\right)=\left(x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},\cdots,x_{{\scriptscriptstyle i}},\cdots\right)=\left(x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},x_{{\scriptscriptstyle 3}},\cdots\right) & i\in\mathbb{N}\\
=\left(x_{{\scriptscriptstyle n}}\right)_{{\scriptscriptstyle n\in\mathbb{N}}}= & \left(x_{{\scriptscriptstyle n}}\middle|n\in\mathbb{N}\right)=\left(x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},\cdots,x_{{\scriptscriptstyle n}},\cdots\right)=\left(x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},x_{{\scriptscriptstyle 3}},\cdots\right) & n\in\mathbb{N}\\
= & \left(\underset{i\in\mathbb{N}}{,}x_{{\scriptscriptstyle i}}\right)=\left(\underset{n\in\mathbb{N}}{,}x_{{\scriptscriptstyle n}}\right)\\
=\left\langle x_{{\scriptscriptstyle i}}\right\rangle _{{\scriptscriptstyle i\in\mathbb{N}}}= & \begin{pmatrix}x_{{\scriptscriptstyle 1}} & x_{{\scriptscriptstyle 2}} & \cdots & x_{{\scriptscriptstyle i}} & \cdots\end{pmatrix}^{\intercal}=\begin{bmatrix}x_{{\scriptscriptstyle 1}} & x_{{\scriptscriptstyle 2}} & \cdots & x_{{\scriptscriptstyle i}} & \cdots\end{bmatrix}^{\intercal} & i\in\mathbb{N}\\
= & \begin{pmatrix}x_{{\scriptscriptstyle 1}}\\
x_{{\scriptscriptstyle 2}}\\
\vdots\\
x_{{\scriptscriptstyle i}}\\
\vdots
\end{pmatrix}=\begin{bmatrix}x_{{\scriptscriptstyle 1}}\\
x_{{\scriptscriptstyle 2}}\\
\vdots\\
x_{{\scriptscriptstyle i}}\\
\vdots
\end{bmatrix} & i\in\mathbb{N}\\
\left\langle x_{{\scriptscriptstyle i}}\right\rangle _{{\scriptscriptstyle i=1}}^{{\scriptscriptstyle n}}= & \left\langle \underset{i=1}{\overset{n}{,}}x_{{\scriptscriptstyle i}}\right\rangle =\left\langle x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},\cdots,x_{{\scriptscriptstyle n}}\right\rangle =\left\langle x_{{\scriptscriptstyle 1}},\cdots,x_{{\scriptscriptstyle n}}\right\rangle  & \forall n\in\mathbb{N}\\
 & \begin{pmatrix}x_{{\scriptscriptstyle 1}}\\
x_{{\scriptscriptstyle 2}}\\
\vdots\\
x_{{\scriptscriptstyle n}}
\end{pmatrix}=\begin{bmatrix}x_{{\scriptscriptstyle 1}}\\
x_{{\scriptscriptstyle 2}}\\
\vdots\\
x_{{\scriptscriptstyle n}}
\end{bmatrix}=\begin{pmatrix}x_{{\scriptscriptstyle 1}}\\
\vdots\\
x_{{\scriptscriptstyle n}}
\end{pmatrix}=\begin{bmatrix}x_{{\scriptscriptstyle 1}}\\
\vdots\\
x_{{\scriptscriptstyle n}}
\end{bmatrix} & \forall n\in\mathbb{N}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\boldsymbol{x}=x_{{\scriptscriptstyle \mu}}=\left(x_{{\scriptscriptstyle i}}\right)_{{\scriptscriptstyle i\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}}= & \left(x_{{\scriptscriptstyle i}}\middle|i\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\right)=\left(x_{{\scriptscriptstyle 0}},x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},\cdots,x_{{\scriptscriptstyle i}},\cdots\right)=\left(x_{{\scriptscriptstyle 0}},x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},x_{{\scriptscriptstyle 3}},\cdots\right) & i\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
=\left(x_{{\scriptscriptstyle k}}\right)_{{\scriptscriptstyle k\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}}= & \left(x_{{\scriptscriptstyle k}}\middle|k\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\right)=\left(x_{{\scriptscriptstyle 0}},x_{{\scriptscriptstyle 1}},\cdots,x_{{\scriptscriptstyle k}},\cdots\right)=\left(x_{{\scriptscriptstyle 0}},x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},\cdots\right) & k\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
=\left(x_{{\scriptscriptstyle \mu}}\right)_{{\scriptscriptstyle \mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}}= & \left(x_{{\scriptscriptstyle \mu}}\middle|\mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\right)=\left(x_{{\scriptscriptstyle 0}},x_{{\scriptscriptstyle 1}},\cdots,x_{{\scriptscriptstyle \mu}},\cdots\right)=\left(x_{{\scriptscriptstyle 0}},x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},\cdots\right) & \mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
= & \left(\underset{i\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}{,}x_{{\scriptscriptstyle i}}\right)=\left(\underset{\mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}{,}x_{{\scriptscriptstyle n}}\right)\\
=\left\langle x_{{\scriptscriptstyle \mu}}\right\rangle _{{\scriptscriptstyle \mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}}}= & \begin{pmatrix}x_{{\scriptscriptstyle 0}} & x_{{\scriptscriptstyle 1}} & \cdots & x_{{\scriptscriptstyle \mu}} & \cdots\end{pmatrix}^{\intercal}=\begin{bmatrix}x_{{\scriptscriptstyle 0}} & x_{{\scriptscriptstyle 1}} & \cdots & x_{{\scriptscriptstyle \mu}} & \cdots\end{bmatrix}^{\intercal} & \mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
= & \begin{pmatrix}x_{{\scriptscriptstyle 0}}\\
x_{{\scriptscriptstyle 1}}\\
\vdots\\
x_{{\scriptscriptstyle \mu}}\\
\vdots
\end{pmatrix}=\begin{bmatrix}x_{{\scriptscriptstyle 0}}\\
x_{{\scriptscriptstyle 1}}\\
\vdots\\
x_{{\scriptscriptstyle \mu}}\\
\vdots
\end{bmatrix} & \mu\in\mathbb{Z}_{{\scriptscriptstyle \ge0}}\\
\left\langle x_{{\scriptscriptstyle \mu}}\right\rangle _{{\scriptscriptstyle \mu=0}}^{{\scriptscriptstyle n-1}}= & \left\langle \underset{\mu=0}{\overset{n-1}{,}}x_{{\scriptscriptstyle \mu}}\right\rangle =\left\langle x_{{\scriptscriptstyle 0}},x_{{\scriptscriptstyle 1}},\cdots,x_{{\scriptscriptstyle n-1}}\right\rangle =\left\langle x_{{\scriptscriptstyle 0}},\cdots,x_{{\scriptscriptstyle n-1}}\right\rangle  & \forall n\in\mathbb{N}\\
= & \begin{pmatrix}x_{{\scriptscriptstyle 0}}\\
x_{{\scriptscriptstyle 1}}\\
\vdots\\
x_{{\scriptscriptstyle n-1}}
\end{pmatrix}=\begin{bmatrix}x_{{\scriptscriptstyle 0}}\\
x_{{\scriptscriptstyle 1}}\\
\vdots\\
x_{{\scriptscriptstyle n-1}}
\end{bmatrix}=\begin{pmatrix}x_{{\scriptscriptstyle 0}}\\
\vdots\\
x_{{\scriptscriptstyle n-1}}
\end{pmatrix}=\begin{bmatrix}x_{{\scriptscriptstyle 0}}\\
\vdots\\
x_{{\scriptscriptstyle n-1}}
\end{bmatrix} & \forall n\in\mathbb{N}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
left coset space
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned} &  & G\text{ is a group}\\
G/H= & \left\{ gH\middle|g\in G\right\}  & \forall H\le G\\
= & \bigcup_{g\in G}\left\{ gH\right\}  & \forall H\le G\\
= & \left\{ gH\right\} _{{\scriptscriptstyle g\in G}} & \forall H\le G
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}G= & \bigcup_{g\in G}gH & \forall H\le G\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 2}}\le\mathbb{Z}_{{\scriptscriptstyle 4}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}_{{\scriptscriptstyle 4}}= & \left\{ 0,1,2,3\right\}  & =\mathbb{Z}_{{\scriptscriptstyle \left[0,4\right)}}=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)\\
= & \left\{ e,g,g^{2},g^{3}\right\}  & =\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 4}},\cdot_{G}\right)\\
= & \left\{ g,g^{2},g^{3},e\right\} \\
= & \left\{ g^{1},g^{2},g^{3},g^{4}\right\} =\left\langle g\right\rangle  & g^{4}=e\\
= & \left\{ a^{1},a^{2},a^{3},a^{4}\right\} \\
= & \left\{ a^{1},a^{2},a^{3},e\right\}  & a^{4}=e\\
= & \left\{ e,a,a^{2},a^{3}\right\} \supset\left\{ e,a^{2}\right\} =\left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 2}},\cdot_{G}\right) & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
\mathbb{Z}_{{\scriptscriptstyle 2}}\le\mathbb{Z}_{{\scriptscriptstyle 4}}\Rightarrow\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ e,a^{2}\right\} \\
e\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ ee,ea^{2}\right\} =\left\{ e,a^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ ae,aa^{2}\right\} =\left\{ a,a^{3}\right\}  & =a\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ a^{2}e,a^{2}a^{2}\right\} =\left\{ a^{2},a^{4}\right\} =\left\{ a^{2},e\right\} =\left\{ e,a^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ a^{3}e,a^{3}a^{2}\right\} =\left\{ a^{3},a^{5}\right\} =\left\{ a^{3},a\right\} =\left\{ a,a^{3}\right\}  & =a\mathbb{Z}_{{\scriptscriptstyle 2}}\\
a^{4}\mathbb{Z}_{{\scriptscriptstyle 2}}= & e\mathbb{Z}_{{\scriptscriptstyle 2}}=\left\{ e,a^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 2}}\\
\mathbb{Z}_{{\scriptscriptstyle 4}}/\mathbb{Z}_{{\scriptscriptstyle 2}}=\mathbb{Z}_{{\scriptscriptstyle 4}}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \left\{ g\mathbb{Z}_{{\scriptscriptstyle 2}}\middle|g\in\mathbb{Z}_{{\scriptscriptstyle 4}}\right\} \\
= & \left\{ e\mathbb{Z}_{{\scriptscriptstyle 2}},a\mathbb{Z}_{{\scriptscriptstyle 2}},a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}},a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \mathbb{Z}_{{\scriptscriptstyle 2}},a\mathbb{Z}_{{\scriptscriptstyle 2}}\right\} \\
\mathbb{Z}_{{\scriptscriptstyle 4}}=\bigcup_{g\in\mathbb{Z}_{{\scriptscriptstyle 4}}}g\mathbb{Z}_{{\scriptscriptstyle 2}}= & e\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}} & \begin{aligned}e\mathbb{Z}_{{\scriptscriptstyle 2}}=a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}= & \mathbb{Z}_{{\scriptscriptstyle 2}}\\
a\mathbb{Z}_{{\scriptscriptstyle 2}}=a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}= & a\mathbb{Z}_{{\scriptscriptstyle 2}}
\end{aligned}
\\
= & \mathbb{Z}_{{\scriptscriptstyle 2}}\cup a\mathbb{Z}_{{\scriptscriptstyle 2}}\\
= & e\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a\mathbb{Z}_{{\scriptscriptstyle 2}}=e\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}=a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a\mathbb{Z}_{{\scriptscriptstyle 2}}=a^{2}\mathbb{Z}_{{\scriptscriptstyle 2}}\cup a^{3}\mathbb{Z}_{{\scriptscriptstyle 2}}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
2=4/2=\left|\left\{ e,a,a^{2},a^{3}\right\} \right|/\left|\left\{ e,a^{2}\right\} \right|=\left|\mathbb{Z}_{{\scriptscriptstyle 4}}\right|/\left|\mathbb{Z}_{{\scriptscriptstyle 2}}\right|=\left|\mathbb{Z}_{{\scriptscriptstyle 4}}/\mathbb{Z}_{{\scriptscriptstyle 2}}\right|=\left|\left\{ \mathbb{Z}_{{\scriptscriptstyle 2}},a\mathbb{Z}_{{\scriptscriptstyle 2}}\right\} \right|=2
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\le D_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\recolor{\begin{aligned}D_{{\scriptscriptstyle 3}}= & \left\{ \begin{aligned}\rho_{{\scriptscriptstyle k,n}}\\
\pi_{{\scriptscriptstyle k,n}}
\end{aligned}
\middle|\begin{aligned}\rho_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & -\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & +\cos\dfrac{2\pi}{n}k
\end{bmatrix}\\
\pi_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & +\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & -\cos\dfrac{2\pi}{n}k
\end{bmatrix}
\end{aligned}
\quad\forall k\in\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}\right\}  & n=3\\
= & \left\{ \rho_{{\scriptscriptstyle 0}},\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\left(D_{{\scriptscriptstyle 3}},\cdot\right)=\left(D_{{\scriptscriptstyle 3}},\cdot_{G}\right)\\
= & \left\{ e,\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & e=\rho_{{\scriptscriptstyle 0}}\\
= & \left\{ e,\rho,\rho^{2},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\} \supset\left\{ e,\rho,\rho^{2}\right\} =\mathbb{Z}_{{\scriptscriptstyle 3}} & \begin{aligned}e\ne\rho=\rho^{1}= & \rho_{{\scriptscriptstyle 1}}\\
\rho^{2}=\left(\rho_{{\scriptscriptstyle 1}}\right)^{2}= & \rho_{{\scriptscriptstyle 2}}
\end{aligned}
\\
\mathbb{Z}_{{\scriptscriptstyle 3}}\le D_{{\scriptscriptstyle 3}}\Rightarrow\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ 0,1,2\right\}  & =\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}=\left(\mathbb{Z}_{{\scriptscriptstyle 3}},+_{{\scriptscriptstyle \mathbb{Z}_{{\scriptscriptstyle n}}}}\right)\\
= & \left\{ e,g,g^{2}\right\}  & =\left(\mathbb{Z}_{{\scriptscriptstyle 3}},\cdot\right)=\left(\mathbb{Z}_{{\scriptscriptstyle 3}},\cdot_{G}\right)\\
= & \left\{ g,g^{2},e\right\} \\
= & \left\{ g^{1},g^{2},g^{3}\right\} =\left\langle g\right\rangle  & g^{3}=e\\
= & \left\{ a^{1},a^{2},a^{3}\right\} \\
= & \left\{ a^{1},a^{2},e\right\}  & a^{3}=e\\
= & \left\{ e,a,a^{2}\right\} \\
= & \left\{ e,\rho,\rho^{2}\right\} =\left(\mathbb{Z}_{{\scriptscriptstyle 3}},\cdot_{{\scriptscriptstyle D_{{\scriptscriptstyle 3}}}}\right)\subset D_{{\scriptscriptstyle 3}} & \rho^{3}=e\\
C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ e,\rho,\rho^{2}\right\} \subset D_{{\scriptscriptstyle 3}}\\
e\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ ee,e\rho,e\rho^{2}\right\} =\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\rho\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \rho e,\rho\rho,\rho\rho^{2}\right\} =\left\{ \rho,\rho^{2},\rho^{3}\right\} =\left\{ \rho,\rho^{2},e\right\} =\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \rho^{2}e,\rho^{2}\rho,\rho^{2}\rho^{2}\right\} =\left\{ \rho^{2},\rho^{3},\rho^{4}\right\} =\left\{ \rho^{2},e,\rho\right\} =\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\rho^{3}\mathbb{Z}_{{\scriptscriptstyle 3}}= & e\mathbb{Z}_{{\scriptscriptstyle 3}}=\left\{ e,\rho,\rho^{2}\right\}  & =\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle 0}}e,\pi_{{\scriptscriptstyle 0}}\rho,\pi_{{\scriptscriptstyle 0}}\rho^{2}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}}\rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 0}}\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}}\rho_{{\scriptscriptstyle 2}}\right\}  & \begin{aligned}\pi_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}= & \pi_{{\scriptscriptstyle i-j}}\\
\pi_{{\scriptscriptstyle i}}= & \pi_{{\scriptscriptstyle 3+i}}
\end{aligned}
\\
= & \left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle -1}},\pi_{{\scriptscriptstyle -2}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 1}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle 1}}e,\pi_{{\scriptscriptstyle 1}}\rho,\pi_{{\scriptscriptstyle 1}}\rho^{2}\right\} =\left\{ \pi_{{\scriptscriptstyle 1}}\rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}}\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 1}}\rho_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle -1}}\right\} =\left\{ \pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 2}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle 2}}e,\pi_{{\scriptscriptstyle 2}}\rho,\pi_{{\scriptscriptstyle 2}}\rho^{2}\right\} =\left\{ \pi_{{\scriptscriptstyle 2}}\rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 2}}\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\rho_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \pi_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}}\right\} =\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
D_{{\scriptscriptstyle 3}}/\mathbb{Z}_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ g\mathbb{Z}_{{\scriptscriptstyle 3}}\middle|g\in D_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ e\mathbb{Z}_{{\scriptscriptstyle 3}},\rho\mathbb{Z}_{{\scriptscriptstyle 3}},\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ \mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ e\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\}  & i\in\left\{ 0,1,2\right\} =\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}\\
D_{{\scriptscriptstyle 3}}=\bigcup_{g\in D_{{\scriptscriptstyle 3}}}g\mathbb{Z}_{{\scriptscriptstyle 3}}= & e\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\rho\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}}\cup\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}} & \begin{aligned}e\mathbb{Z}_{{\scriptscriptstyle 3}}=\rho\mathbb{Z}_{{\scriptscriptstyle 3}}=\rho^{2}\mathbb{Z}_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\\
\pi_{{\scriptscriptstyle 0}}\mathbb{Z}_{{\scriptscriptstyle 3}}=\pi_{{\scriptscriptstyle 1}}\mathbb{Z}_{{\scriptscriptstyle 3}}=\pi_{{\scriptscriptstyle 2}}\mathbb{Z}_{{\scriptscriptstyle 3}}=\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}
\end{aligned}
\\
= & \mathbb{Z}_{{\scriptscriptstyle 3}}\cup\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}} & i\in\left\{ 0,1,2\right\} =\mathbb{Z}_{{\scriptscriptstyle \left[0,3\right)}}\\
= & \cdots & 3\cdot3=9\text{ possibilities of combinations}
\end{aligned}
}
\]

\end_inset


\begin_inset Formula 
\[
2=6/3=\left|\left\{ e,\rho,\rho^{2},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\} \right|/\left|\left\{ e,\rho,\rho^{2}\right\} \right|=\left|D_{{\scriptscriptstyle 3}}\right|/\left|\mathbb{Z}_{{\scriptscriptstyle 3}}\right|=\left|D_{{\scriptscriptstyle 3}}/\mathbb{Z}_{{\scriptscriptstyle 3}}\right|=\left|\left\{ e\mathbb{Z}_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \right|=2
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $4\mathbb{Z}\le\mathbb{Z}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\mathbb{Z}/4\mathbb{Z}=\mathbb{Z}+4\mathbb{Z}= & \left\{ k+4\mathbb{Z}\middle|k\in\mathbb{Z}\right\} =\left\{ 4\mathbb{Z},1+4\mathbb{Z},2+4\mathbb{Z}.3+4\mathbb{Z}\right\} \\
= & \left\{ \cdots,-3+4\mathbb{Z},-2+4\mathbb{Z},-1+4\mathbb{Z},0+4\mathbb{Z},1+4\mathbb{Z},2+4\mathbb{Z},3+4\mathbb{Z},\cdots\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbb{Z}=\bigcup_{k\in\mathbb{Z}}\left(k+4\mathbb{Z}\right)=\left(4\mathbb{Z}\right)\cup\left(1+4\mathbb{Z}\right)\cup\left(2+4\mathbb{Z}\right)\cup\left(3+4\mathbb{Z}\right)
\]

\end_inset


\end_layout

\begin_layout Section
normal subgroup
\end_layout

\begin_layout Definition
normal subgroup
\begin_inset Formula 
\[
\begin{aligned}H\le & G\\
gH= & Hg & \forall g\in G\\
\Downarrow\\
H\trianglelefteq & G\\
\Updownarrow\\
H\text{ is } & \text{a normal subgroup of }G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
trivial subgroups are normal subgroups
\begin_inset Formula 
\[
\begin{aligned}\left\{ e\right\} \le & G\\
G\le & G & \left\{ e\right\} ,G\le G\\
g\left\{ e\right\} = & \left\{ e\right\} g & \forall g\in G\\
gG= & Gg & \forall g\in G\\
\Downarrow\\
\left\{ e\right\} \trianglelefteq & G\\
G\trianglelefteq & G & \left\{ e\right\} ,G\trianglelefteq G\\
\Updownarrow\\
\text{trivial groups}\left\{ e\right\} ,G\text{ are } & \text{ normal subgroups of }G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
central subgroup is a normal subgroup
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}C\left(G\right)\le & G\\
g\cdot C\left(G\right)=gC\left(G\right)= & C\left(G\right)g=C\left(G\right)\cdot g & \forall g\in G\\
\Downarrow\\
C\left(G\right)\trianglelefteq & G\\
\Updownarrow\\
\text{central subgroup }C\left(G\right)\text{ is } & \text{a normal subgroup of }G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\trianglelefteq S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}$
\end_inset


\begin_inset Formula 
\[
\begin{aligned}\rho_{{\scriptscriptstyle k+3}}= & \rho_{{\scriptscriptstyle k}}\\
\pi_{{\scriptscriptstyle k+3}}= & \pi_{{\scriptscriptstyle k}}\\
\rho_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}= & \rho_{{\scriptscriptstyle i+j}}\\
\rho_{{\scriptscriptstyle i}}\pi_{{\scriptscriptstyle j}}= & \pi_{{\scriptscriptstyle i+j}}\\
\pi_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}= & \pi_{{\scriptscriptstyle i-j}}\\
\pi_{{\scriptscriptstyle i}}\pi_{{\scriptscriptstyle j}}= & \rho_{{\scriptscriptstyle i-j}}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\recolor{\begin{aligned}C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ 0,1,2\right\} \\
= & \left\{ 0_{{\scriptscriptstyle \rho}},1_{{\scriptscriptstyle \rho}},2_{{\scriptscriptstyle \rho}}\right\} =\left\{ \left[123\right],\left[231\right],\left[312\right]\right\} =\left\{ \left(\right),\left(123\right),\left(132\right)\right\}  & \le S_{{\scriptscriptstyle 3}}\\
= & \left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}1}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}2}},\cdots,\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{n}\left(n-1\right)}}\right\} \overset{n=3}{=}\left\{ \mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}0}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}1}},\mathrm{e}^{{\scriptscriptstyle \mathrm{i}\frac{2\pi}{3}2}}\right\} \\
= & \left\{ e,g,g^{2},\cdots,g^{n-1}\right\} =\left\{ g^{0},g^{1},g^{2}\right\} =\left\{ e,g,g^{2}\right\} ,g^{n}=e\\
= & \left\{ e,\rho,\rho^{2}\right\} =\left\{ \rho_{{\scriptscriptstyle 0}},\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}}\right\} =\left\{ \rho_{{\scriptscriptstyle j}}\middle|j\in\left\{ 0,1,2\right\} \right\}  & \le D_{{\scriptscriptstyle 3}}=\left\{ \rho_{{\scriptscriptstyle k}},\pi_{{\scriptscriptstyle k}}\right\} =\left\{ \rho_{{\scriptscriptstyle k,3}},\pi_{{\scriptscriptstyle k,3}}\right\} \\
\rho_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \rho_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}\middle|j\in\left\{ 0,1,2\right\} \right\} \\
= & \left\{ \rho_{{\scriptscriptstyle i+j}}\middle|j\in\left\{ 0,1,2\right\} \right\} \overset{i+j=j+i}{=}\left\{ \rho_{{\scriptscriptstyle j+i}}\middle|j\in\left\{ 0,1,2\right\} \right\} \\
= & \left\{ \rho_{{\scriptscriptstyle j}}\rho_{{\scriptscriptstyle i}}\middle|j\in\left\{ 0,1,2\right\} \right\} =\mathbb{Z}_{{\scriptscriptstyle 3}}\rho_{{\scriptscriptstyle i}} & \Rightarrow\rho_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\rho_{{\scriptscriptstyle i}}\\
\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \pi_{{\scriptscriptstyle i}}\rho_{{\scriptscriptstyle j}}\middle|j\in\left\{ 0,1,2\right\} \right\} \\
= & \left\{ \pi_{{\scriptscriptstyle i-j}}\middle|j\in\left\{ 0,1,2\right\} \right\} \overset{\pi_{{\scriptscriptstyle k+3}}=\pi_{{\scriptscriptstyle k}}}{=}\left\{ \pi_{{\scriptscriptstyle 3+i-j}}\middle|j\in\left\{ 0,1,2\right\} \right\} \\
= & \left\{ \pi_{{\scriptscriptstyle i+\left(3-j\right)}}\middle|3-j\in\left\{ 3,2,1\right\} \right\} =\left\{ \pi_{{\scriptscriptstyle \left(3-j\right)+i}}\middle|3-j\in\left\{ 3,2,1\right\} \right\}  & i+\left(3-j\right)=\left(3-j\right)+i\\
= & \left\{ \rho_{{\scriptscriptstyle 3-j}}\pi_{{\scriptscriptstyle i}}\middle|3-j\in\left\{ 3,2,1\right\} \right\} =\left\{ \rho_{{\scriptscriptstyle 3-j}}\middle|3-j\in\left\{ 3,2,1\right\} \right\} \pi_{{\scriptscriptstyle i}} & \rho_{{\scriptscriptstyle i}}\pi_{{\scriptscriptstyle j}}=\pi_{{\scriptscriptstyle i+j}}\\
= & \left\{ \rho_{{\scriptscriptstyle j}}\middle|j\in\left\{ 0,1,2\right\} \right\} \pi_{{\scriptscriptstyle i}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\pi_{{\scriptscriptstyle i}} & \Rightarrow\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\pi_{{\scriptscriptstyle i}}\\
\Downarrow\\
\rho_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \mathbb{Z}_{{\scriptscriptstyle 3}}\rho_{{\scriptscriptstyle i}}\\
\pi_{{\scriptscriptstyle i}}\mathbb{Z}_{{\scriptscriptstyle 3}}= & \mathbb{Z}_{{\scriptscriptstyle 3}}\pi_{{\scriptscriptstyle i}} & \Rightarrow g\mathbb{Z}_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}g\quad\forall g\in D_{{\scriptscriptstyle 3}}\\
\Downarrow\\
\mathbb{Z}_{{\scriptscriptstyle 3}} & \le D_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}\\
g\mathbb{Z}_{{\scriptscriptstyle 3}}= & \mathbb{Z}_{{\scriptscriptstyle 3}}g\quad\forall g\in D_{{\scriptscriptstyle 3}} & \Rightarrow\mathbb{Z}_{{\scriptscriptstyle 3}}\trianglelefteq D_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}\\
 &  & \Updownarrow\\
 &  & \mathbb{Z}_{{\scriptscriptstyle 3}}\trianglelefteq S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}
\end{aligned}
}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle 2}}\ntrianglelefteq S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}$
\end_inset


\end_layout

\begin_layout Definition
set mulitplication and set inverse
\begin_inset Formula 
\[
\begin{aligned}S^{{\scriptscriptstyle -1}}=\overline{S}= & \left\{ \overline{s}\middle|s\in S\right\} =\left\{ s^{{\scriptscriptstyle -1}}\middle|s\in S\right\} \\
gS= & \left\{ gs\middle|s\in S\right\}  & \left\{ sg\middle|s\in S\right\} =Sg\\
S_{{\scriptscriptstyle 1}}S_{{\scriptscriptstyle 2}}= & \left\{ s_{{\scriptscriptstyle 1}}s_{{\scriptscriptstyle 2}}\middle|\begin{aligned}s_{{\scriptscriptstyle 1}}\in S_{{\scriptscriptstyle 1}}\\
s_{{\scriptscriptstyle 2}}\in S_{{\scriptscriptstyle 2}}
\end{aligned}
\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
subgroup or group multiplication closure and inverse closure
\begin_inset Formula 
\[
\begin{aligned}H\le & G\\
\Downarrow\\
HH= & H & HH=\left\{ h_{{\scriptscriptstyle 1}}h_{{\scriptscriptstyle 2}}\middle|\begin{aligned}h_{{\scriptscriptstyle 1}}\in H\\
h_{{\scriptscriptstyle 2}}\in H
\end{aligned}
\right\} \\
\wedge\\
H^{{\scriptscriptstyle -1}}=\overline{H}= & H & H^{{\scriptscriptstyle -1}}=\overline{H}=\left\{ \overline{h}\middle|h\in H\right\} =\left\{ h^{{\scriptscriptstyle -1}}\middle|h\in H\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
H\le G\Rightarrow\begin{cases}
HH=H & \text{group multiplication closure}\\
\overline{H}=H & \text{group inverse closure}
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
\begin{aligned}N\trianglelefteq & G\\
g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in & G\\
\Downarrow\\
\left(g_{{\scriptscriptstyle 1}}N\right)\left(g_{{\scriptscriptstyle 2}}N\right)= & \left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)N
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
\begin{aligned}\left(g_{{\scriptscriptstyle 1}}N\right)\left(g_{{\scriptscriptstyle 2}}N\right)= & \left(g_{{\scriptscriptstyle 1}}Ng_{{\scriptscriptstyle 2}}\right)\left(N\right) & \left(g_{{\scriptscriptstyle 1}}n_{{\scriptscriptstyle 1}}\right)\left(g_{{\scriptscriptstyle 2}}n_{{\scriptscriptstyle 2}}\right)=\left(g_{{\scriptscriptstyle 1}}n_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)n_{{\scriptscriptstyle 2}}\\
= & \left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}N\right)\left(N\right) & \overset{N\trianglelefteq G\Rightarrow gn=ng}{=}\left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}n_{{\scriptscriptstyle 1}}\right)n_{{\scriptscriptstyle 2}}\\
= & \left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)\left(NN\right) & =\left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)\left(n_{{\scriptscriptstyle 1}}n_{{\scriptscriptstyle 2}}\right)\\
= & \left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)\left(N\right) & N\le G\Rightarrow NN=N\Leftarrow HH=H\Leftarrow H\le G\\
\left(g_{{\scriptscriptstyle 1}}N\right)\left(g_{{\scriptscriptstyle 2}}N\right)= & \left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)N
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
\begin{aligned}N\trianglelefteq & G\\
g\in & G\\
\Downarrow\\
\left(gN\right)^{{\scriptscriptstyle -1}}=\overline{\left(gN\right)}= & \overline{g}N=g^{{\scriptscriptstyle -1}}N
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
\begin{aligned}\overline{\left(gN\right)}=\overline{\left\{ gn\right\} }= & \left\{ \overline{gn}\right\}  & \overline{S}=\left\{ \overline{s}\middle|s\in S\right\} \\
= & \left\{ \overline{n}\overline{g}\right\}  & \overline{gn}gn=\overline{n}\overline{g}gn=e\\
= & \overline{N}\overline{g}\\
= & N\overline{g} & \overline{N}=N\Leftarrow N\le G\\
= & \overline{g}N & gN=Ng\Leftarrow N\trianglelefteq G\\
\overline{\left(gN\right)}= & \overline{g}N
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Theorem
\begin_inset Formula 
\[
N\trianglelefteq G\Rightarrow\left(N\right)\left(gN\right)=NgN=gN
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
\begin{aligned}\left(N\right)\left(gN\right)= & \left(N\right)\left(Ng\right) & gN=Ng\Leftarrow N\trianglelefteq G\\
= & \left(NN\right)g & \left(n_{{\scriptscriptstyle 1}}\right)\left(n_{{\scriptscriptstyle 2}}g\right)=\left(n_{{\scriptscriptstyle 1}}n_{{\scriptscriptstyle 2}}\right)g\\
= & Ng & NN=N\Leftarrow N\le G\\
= & gN & gN=Ng\Leftarrow N\trianglelefteq G
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Subsection
quotient group
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group}\\
\Updownarrow\\
G=\left(G,\cdot\right)=\left(G,\cdot_{{\scriptscriptstyle G}}\right)= & \left\{ g\middle|\begin{aligned}g_{{\scriptscriptstyle 1}}\cdot g_{{\scriptscriptstyle 2}}= & g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\in G & \forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}}\in G & \quad\left(c\right)\cdot_{{\scriptscriptstyle G}}\text{closure}\\
g_{{\scriptscriptstyle 1}}\left(g_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 3}}\right)= & \left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)g_{{\scriptscriptstyle 3}}=g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 3}} & \forall g_{{\scriptscriptstyle 1}},g_{{\scriptscriptstyle 2}},g_{{\scriptscriptstyle 3}}\in G & \quad\left(a\right)\cdot_{{\scriptscriptstyle G}}\text{associativity}\\
e\cdot g=eg= & g=ge=g\cdot e & \exists e=e_{{\scriptscriptstyle G}}\in G,\forall g\in G & \quad\left(id\right)\text{identity element}\\
\overline{g}\cdot g=\bar{g}g= & e=g\bar{g}=g\cdot\overline{g} & \forall g\in G,\exists\overline{g}\in G & \quad\left(in\right)\text{inverse element}
\end{aligned}
\right\} 
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Definition
quotient group
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned}G/N= & GN=\left(G/N,\cdot_{{\scriptscriptstyle \text{group}}}\right)=\left(GN,\cdot_{{\scriptscriptstyle \text{set}}}\right)\\
= & \left\{ gN\middle|\begin{aligned}\left(g_{{\scriptscriptstyle 1}}N\right)\left(g_{{\scriptscriptstyle 2}}N\right)= & \left(g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}\right)N\in G/N & \forall g_{{\scriptscriptstyle 1}}N,g_{{\scriptscriptstyle 2}}N\in G/N & \quad\left(c\right)\cdot_{{\scriptscriptstyle \text{group}}}\text{closure}\\
g_{{\scriptscriptstyle 1}}N\left(g_{{\scriptscriptstyle 2}}Ng_{{\scriptscriptstyle 3}}N\right)= & \left(g_{{\scriptscriptstyle 1}}Ng_{{\scriptscriptstyle 2}}N\right)g_{{\scriptscriptstyle 3}}N=g_{{\scriptscriptstyle 1}}g_{{\scriptscriptstyle 2}}g_{{\scriptscriptstyle 3}}N & \forall g_{{\scriptscriptstyle 1}}N,g_{{\scriptscriptstyle 2}}N,g_{3}N\in G/N & \quad\left(a\right)\cdot_{{\scriptscriptstyle \text{group}}}\text{associativity}\\
\left(N\right)\left(gN\right)=NgN= & gN=\left(gN\right)N=\left(gN\right)\left(N\right) & \exists N=e_{{\scriptscriptstyle G/N}}\in G/N,\forall gN\in G/N & \quad\left(id\right)\text{identity element}\\
\overline{\left(gN\right)}\left(gN\right)=\overline{gN}gN= & N\overline{g}gN=NeN=NN=N & \forall gN\in G/N,\exists\overline{gN}\in G/N & \quad\left(in\right)\text{inverse element}
\end{aligned}
\right\} \\
\Downarrow\\
G/N= & \left\{ gN\middle|g\in G\right\} \text{ is a group}\qquad\forall N\trianglelefteq G\\
\Updownarrow & \text{def.}\\
G/N\text{ is } & \text{a quotient group}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left|G/N\right|=\left|G\right|/\left|N\right|\overset{\text{if }\left|N\right|>1\text{ or }G/N\ne\left\{ e\right\} }{<}\left|G\right|
\]

\end_inset


\end_layout

\begin_layout Standard
quotient group vs.
 left coset space
\end_layout

\begin_layout Itemize
quotient group
\begin_inset Formula 
\[
\begin{aligned} &  & G\text{ is a group}\\
G/N= & \left\{ gN\middle|g\in G\right\}  & \forall N\trianglelefteq G\\
= & \bigcup_{g\in G}\left\{ gN\right\}  & \forall N\trianglelefteq G\\
= & \left\{ gN\right\} _{{\scriptscriptstyle g\in G}} & \forall N\trianglelefteq G
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}G= & \bigcup_{g\in G}gN & \forall N\trianglelefteq G\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\left|G/N\right|=\left|G\right|/\left|N\right|
\]

\end_inset


\end_layout

\begin_layout Itemize
left coset space
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{aligned} &  & G\text{ is a group}\\
G/H= & \left\{ gH\middle|g\in G\right\}  & \forall H\le G\\
= & \bigcup_{g\in G}\left\{ gH\right\}  & \forall H\le G\\
= & \left\{ gH\right\} _{{\scriptscriptstyle g\in G}} & \forall H\le G
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}G= & \bigcup_{g\in G}gH & \forall H\le G\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\left|G/H\right|=\left|G\right|/\left|H\right|
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}\vartriangleright C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\vartriangleright E_{{\scriptscriptstyle 3}}$
\end_inset


