\begin_inset Formula 
\[
\recolor{\begin{aligned}D_{{\scriptscriptstyle 3}}= & \left\{ \rho_{{\scriptscriptstyle 0}},\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\}  & =\left\{ \rho_{{\scriptscriptstyle 0,3}},\rho_{{\scriptscriptstyle 1,3}},\rho_{{\scriptscriptstyle 2,3}},\pi_{{\scriptscriptstyle 0,3}},\pi_{{\scriptscriptstyle 1,3}},\pi_{{\scriptscriptstyle 2,3}}\right\} \\
= & \left\{ \begin{aligned}\rho_{{\scriptscriptstyle k}}\\
\pi_{{\scriptscriptstyle k}}
\end{aligned}
\middle|\begin{aligned}\rho_{{\scriptscriptstyle k}}=\begin{bmatrix}+\cos\dfrac{2\pi}{3}k & -\sin\dfrac{2\pi}{3}k\\
+\sin\dfrac{2\pi}{3}k & +\cos\dfrac{2\pi}{3}k
\end{bmatrix}\\
\pi_{{\scriptscriptstyle k}}=\begin{bmatrix}+\cos\dfrac{2\pi}{3}k & +\sin\dfrac{2\pi}{3}k\\
+\sin\dfrac{2\pi}{3}k & -\cos\dfrac{2\pi}{3}k
\end{bmatrix}
\end{aligned}
\right\}  & k\in\left\{ 0,1,2\right\} \\
D_{{\scriptscriptstyle n}}= & \left\{ \begin{aligned}\rho_{{\scriptscriptstyle k}}\\
\pi_{{\scriptscriptstyle k}}
\end{aligned}
\middle|\begin{aligned}\rho_{{\scriptscriptstyle k}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & -\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & +\cos\dfrac{2\pi}{n}k
\end{bmatrix}\\
\pi_{{\scriptscriptstyle k}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & +\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & -\cos\dfrac{2\pi}{n}k
\end{bmatrix}
\end{aligned}
\right\}  & \begin{aligned}\forall n\in\mathbb{N}\\
k\in\left\{ 0,\cdots,n-1\right\} 
\end{aligned}
\\
= & \left\{ \begin{aligned}\rho_{{\scriptscriptstyle k,n}}\\
\pi_{{\scriptscriptstyle k,n}}
\end{aligned}
\middle|\begin{aligned}\rho_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & -\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & +\cos\dfrac{2\pi}{n}k
\end{bmatrix}\\
\pi_{{\scriptscriptstyle k,n}}=\begin{bmatrix}+\cos\dfrac{2\pi}{n}k & +\sin\dfrac{2\pi}{n}k\\
+\sin\dfrac{2\pi}{n}k & -\cos\dfrac{2\pi}{n}k
\end{bmatrix}
\end{aligned}
\right\}  & \begin{aligned}\forall n\in\mathbb{N}\\
k\in\left\{ 0,\cdots,n-1\right\} 
\end{aligned}
\\
D_{{\scriptscriptstyle 2}}= & \left\{ \begin{aligned}\rho_{{\scriptscriptstyle k}}\\
\pi_{{\scriptscriptstyle k}}
\end{aligned}
\middle|\begin{aligned}\rho_{{\scriptscriptstyle k}}=\begin{bmatrix}+\cos\dfrac{2\pi}{2}k & -\sin\dfrac{2\pi}{2}k\\
+\sin\dfrac{2\pi}{2}k & +\cos\dfrac{2\pi}{2}k
\end{bmatrix}\\
\pi_{{\scriptscriptstyle k}}=\begin{bmatrix}+\cos\dfrac{2\pi}{2}k & +\sin\dfrac{2\pi}{2}k\\
+\sin\dfrac{2\pi}{2}k & -\cos\dfrac{2\pi}{2}k
\end{bmatrix}
\end{aligned}
\right\}  & k\in\left\{ 0,2-1\right\} =\left\{ 0,1\right\} \\
= & \left\{ \begin{aligned}\rho_{{\scriptscriptstyle k}}\\
\pi_{{\scriptscriptstyle k}}
\end{aligned}
\middle|\begin{aligned}\rho_{{\scriptscriptstyle k}}=\begin{bmatrix}+\cos\pi k & -\sin\pi k\\
+\sin\pi k & +\cos\pi k
\end{bmatrix}\\
\pi_{{\scriptscriptstyle k}}=\begin{bmatrix}+\cos\pi k & +\sin\pi k\\
+\sin\pi k & -\cos\pi k
\end{bmatrix}
\end{aligned}
\right\}  & k\in\left\{ 0,1\right\} \\
= & \{\begin{bmatrix}\cos0 & -\sin0\\
\sin0 & \cos0
\end{bmatrix},\begin{bmatrix}\cos\pi & -\sin\pi\\
\sin\pi & \cos\pi
\end{bmatrix}, & =\{\begin{bmatrix}1 & 0\\
0 & 1
\end{bmatrix},\begin{bmatrix}-1 & 0\\
0 & -1
\end{bmatrix},\\
 & \quad\begin{bmatrix}\cos0 & \sin0\\
\sin0 & -\cos0
\end{bmatrix},\begin{bmatrix}\cos\pi & \sin\pi\\
\sin\pi & -\cos\pi
\end{bmatrix}\} & \begin{bmatrix}1 & 0\\
0 & -1
\end{bmatrix},\begin{bmatrix}-1 & 0\\
0 & 1
\end{bmatrix}\}\\
= & \{\rho_{{\scriptscriptstyle 0}},\rho_{{\scriptscriptstyle 1}}, & =\left\{ \rho_{{\scriptscriptstyle 0}},\rho_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}}\right\} \\
 & \quad\pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}}\} & =\left\{ \rho_{{\scriptscriptstyle 0,2}},\rho_{{\scriptscriptstyle 1,2}},\pi_{{\scriptscriptstyle 0,2}},\pi_{{\scriptscriptstyle 1,2}}\right\} \\
D_{{\scriptscriptstyle 1}}= & \{\begin{bmatrix}\cos0 & -\sin0\\
\sin0 & \cos0
\end{bmatrix}, & =\left\{ \rho_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 0}}\right\} \\
 & \quad\begin{bmatrix}\cos0 & \sin0\\
\sin0 & -\cos0
\end{bmatrix}\} & =\left\{ \begin{bmatrix}1 & 0\\
0 & 1
\end{bmatrix},\begin{bmatrix}1 & 0\\
0 & -1
\end{bmatrix}\right\} =\left\{ \rho_{{\scriptscriptstyle 0,1}},\pi_{{\scriptscriptstyle 0,1}}\right\} 
\end{aligned}
}
\]

