\end_layout

\begin_deeper
\begin_layout Itemize
monster group
\end_layout

\end_deeper
\begin_layout Section
conjugate class
\end_layout

\begin_layout Definition
conjugate class
\begin_inset Formula 
\[
\begin{aligned} &  & G\text{ is a group}\\
\left[g\right]= & \left\{ hg\overline{h}\middle|h\in G\right\}  & \forall g\in G
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left[g\right]\ni eg\overline{e}= & ege=\left(eg\right)e=\left(g\right)e=ge=g\\
\Downarrow\\
g= & eg\overline{e}\in\left[g\right]\\
\Downarrow\\
g\in & \left[g\right]
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\widetilde{g}\in & \left[g\right]=\left\{ hg\overline{h}\middle|h\in G\right\} \quad\begin{aligned}G\text{ is a group}\\
\forall g\in G
\end{aligned}
\\
\Downarrow\\
\widetilde{g}= & hg\overline{h}\\
\overline{h}\widetilde{g}h= & \overline{h}hg\overline{h}h=ege=g\\
g= & \overline{h}\widetilde{g}h=\overline{h}\widetilde{g}\overline{\overline{h}}\in\left\{ \overline{h}\widetilde{g}\overline{\overline{h}}\middle|h\in G\right\} =\left\{ h\widetilde{g}\overline{h}\middle|h\in G\right\} =\left[\widetilde{g}\right]\quad\begin{aligned}G\text{ is a group}\\
\forall\widetilde{g}\in G
\end{aligned}
\\
\Downarrow\\
g\in & \left[\widetilde{g}\right]
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\overline{h\widetilde{h}}h\widetilde{h}=\overline{\widetilde{h}}\overline{h}h\widetilde{h}=\overline{\widetilde{h}}\left(\overline{h}h\right)\widetilde{h}=\overline{\widetilde{h}}\left(e\right)\widetilde{h}=\overline{\widetilde{h}}\widetilde{h}=e
\]

\end_inset


\begin_inset Formula 
\[
\begin{cases}
g\in\left[\widetilde{g}\right] & \Rightarrow g=h\widetilde{g}\overline{h}=h\widetilde{h}\widetilde{\widetilde{g}}\overline{\widetilde{h}}\overline{h}\overset{\overline{h\widetilde{h}}=\overline{\widetilde{h}}\overline{h}}{=}h\widetilde{h}\widetilde{\widetilde{g}}\overline{h\widetilde{h}}\in\left\{ h\widetilde{h}\widetilde{\widetilde{g}}\overline{h\widetilde{h}}\middle|h\widetilde{h}\in G\right\} =\left[\widetilde{\widetilde{g}}\right]\\
\widetilde{g}\in\left[\widetilde{\widetilde{g}}\right] & \Rightarrow\widetilde{g}=\widetilde{h}\widetilde{\widetilde{g}}\overline{\widetilde{h}}\Uparrow
\end{cases}\Rightarrow g\in\left[\widetilde{\widetilde{g}}\right]
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left[e\right]= & \left\{ he\overline{h}\middle|h\in G\right\} =\left\{ h\overline{h}\middle|h\in G\right\} =\left\{ e\middle|h\in G\right\} =\left\{ e\right\} \quad\begin{aligned}G\text{ is a group}\\
e=e_{{\scriptscriptstyle G}}\in G
\end{aligned}
\\
\left[e\right]= & \left\{ e\right\} 
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}\left[c\right]= & \left\{ hc\overline{h}\middle|h\in G\right\} =\left\{ ch\overline{h}\middle|\begin{aligned}ch=hc\quad\forall h\in & G\end{aligned}
\right\} =\left\{ ce\middle|h\in G\right\} =\left\{ c\right\} \quad\forall c\in C\left(G\right)\\
\left[c\right]= & \left\{ c\right\} 
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\begin{aligned}g\in\left[\widetilde{g}\right]= & \left\{ h\widetilde{g}\overline{h}\middle|h\in G\right\}  & \begin{aligned}G\text{ is a group}\\
\widetilde{g}\in G
\end{aligned}
\\
\exists h\in G & \left[g=h\widetilde{g}\overline{h}\right]\\
g= & h\widetilde{g}\overline{h}\\
g^{n}= & \overset{n}{\overbrace{h\widetilde{g}\overline{h}h\widetilde{g}\overline{h}\cdots h\widetilde{g}\overline{h}}}=h\overset{n}{\overbrace{\widetilde{g}e\widetilde{g}e\cdots e\widetilde{g}}}\overline{h}=h\overset{n}{\overbrace{\widetilde{g}\widetilde{g}\cdots\widetilde{g}}}\overline{h}=h\widetilde{g}^{n}\overline{h}\in\left[\widetilde{g}^{n}\right] & \forall n\in\mathbb{N}\\
g^{n}\in & \left[\widetilde{g}^{n}\right] & \forall n\in\mathbb{N}\\
g^{-n}=\overline{g}^{n}=\overline{g^{n}}= & \overline{h\widetilde{g}^{n}\overline{h}}=\overline{\overline{h}}\overline{\widetilde{g}}^{n}\overline{h}=h\overline{\widetilde{g}}^{n}\overline{h}=h\widetilde{g}^{-n}\overline{h} & \forall n\in\mathbb{N}\\
g^{m}\in & \left[\widetilde{g}^{m}\right] & \forall m\in\mathbb{Z}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $D_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}$
\end_inset


\end_layout

