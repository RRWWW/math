\end_layout

\begin_layout Subsection
simple group
\end_layout

\begin_layout Definition
simple group
\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group}\\
G & \text{ does not have no nontrivial normal groups} & \forall N\trianglelefteq G\left[N\in\left\{ \left\{ e\right\} ,G\right\} \right]\\
\Downarrow\\
G & \text{ is a simple group}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Definition
finite simple group
\begin_inset Formula 
\[
\begin{aligned}G & \text{ is a group}\\
G & \text{ does not have no nontrivial normal groups} & \forall N\trianglelefteq G\left[N\in\left\{ \left\{ e\right\} ,G\right\} \right]\\
\left|G\right| & <\infty\\
\Downarrow\\
G & \text{ is a simple group}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
\text{finite simple group}:\text{finite group theory}\sim\text{prime number}:\text{number theory}
\]

\end_inset


\end_layout

\begin_layout Definition
classification of finite simple groups
\end_layout

\begin_layout Itemize
cyclic groups of prime order 
\begin_inset Formula $\mathbb{Z}_{{\scriptscriptstyle \mathbb{P}}}$
\end_inset


\end_layout

\begin_layout Itemize
alternating groups of degree at least 5 
\begin_inset Formula $A_{{\scriptscriptstyle n\ge5}}$
\end_inset


\end_layout

