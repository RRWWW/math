\quad=\{ & \left[123\right], & \left[231\right], & \left[312\right], & \left[213\right], & \left[132\right], & \left[321\right] & \} & \text{one-line notation}\\
\quad=\{ & 123, & 231, & 312, & 213, & 132, & 321 & \} & \text{one-line notation,}\\
 &  &  &  &  &  &  &  & \text{simplified}\\
\quad=\{ & \left(1\right)\left(2\right)\left(3\right), & \left(123\right), & \left(132\right), & \left(12\right)\left(3\right), & \left(1\right)\left(23\right), & \left(31\right)\left(2\right) & \} & \text{cycle notation}\\
\quad=\{ & \left(\right), & \left(123\right), & \left(132\right), & \left(12\right), & \left(23\right), & \left(31\right) & \} & \text{cycle notation,}\\
 &  &  &  &  &  &  &  & \text{simplified}\\
D_{{\scriptscriptstyle 3}}=\{ & \rho_{{\scriptscriptstyle 0}}, & \rho_{{\scriptscriptstyle 1}}, & \rho_{{\scriptscriptstyle 2}}, & \pi_{{\scriptscriptstyle 0}}, & \pi_{{\scriptscriptstyle 1}}, & \pi_{{\scriptscriptstyle 2}} & \}\\
\quad=\{ & e, & \rho, & \rho^{2}, & \pi_{{\scriptscriptstyle 0}}, & \pi_{{\scriptscriptstyle 1}}, & \pi_{{\scriptscriptstyle 2}} & \}
\end{array}}
\]

\end_inset


\begin_inset Formula 
\[
\left|S_{{\scriptscriptstyle 3}}\right|=3!=6=2\cdot3=\left|D_{{\scriptscriptstyle 3}}\right|
\]

\end_inset


\begin_inset Formula 
\[
\recolor{\begin{aligned}C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \rho_{{\scriptscriptstyle 0}}\right\} \cup\left\{ \rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}}\right\}  & =\left\{ \rho_{{\scriptscriptstyle 0}},\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}}\right\} \\
= & \left\{ \left[123\right]\right\} \cup\left\{ \left[231\right],\left[312\right]\right\}  & =\left\{ \left[123\right],\left[231\right],\left[312\right]\right\} \\
= & \left\{ \left(\right)\right\} \cup\left\{ \left(123\right),\left(132\right)\right\}  & =\left\{ \left(\right),\left(123\right),\left(132\right)\right\} 
\end{aligned}
}
\]

\end_inset


\begin_inset Formula 
\[
\left|\mathbb{Z}_{{\scriptscriptstyle 3}}\right|=\left|C_{{\scriptscriptstyle 3}}\right|=3
\]

\end_inset


\begin_inset Formula 
\[
\recolor{\begin{aligned}D_{{\scriptscriptstyle 3}}/C_{{\scriptscriptstyle 3}}=S_{{\scriptscriptstyle 3}}/\mathbb{Z}_{{\scriptscriptstyle 3}}= & \left\{ \sigma\mathbb{Z}_{{\scriptscriptstyle 3}}\middle|\sigma\in S_{{\scriptscriptstyle 3}}\right\}  & =S_{{\scriptscriptstyle 3}}\mathbb{Z}_{{\scriptscriptstyle 3}}\\
= & \left\{ \left[123\right]\mathbb{Z}_{{\scriptscriptstyle 3}},\left[213\right]\mathbb{Z}_{{\scriptscriptstyle 3}}\right\}  & =\left\{ \left(\right)\mathbb{Z}_{{\scriptscriptstyle 3}},\left(12\right)\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ \left[123\right]\mathbb{Z}_{{\scriptscriptstyle 3}},\left[132\right]\mathbb{Z}_{{\scriptscriptstyle 3}}\right\}  & =\left\{ \left(\right)\mathbb{Z}_{{\scriptscriptstyle 3}},\left(23\right)\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ \left[123\right]\mathbb{Z}_{{\scriptscriptstyle 3}},\left[321\right]\mathbb{Z}_{{\scriptscriptstyle 3}}\right\}  & =\left\{ \left(\right)\mathbb{Z}_{{\scriptscriptstyle 3}},\left(31\right)\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ eC_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle i}}C_{{\scriptscriptstyle 3}}\right\}  & =D_{{\scriptscriptstyle 3}}C_{{\scriptscriptstyle 3}}\\
= & \left\{ \rho_{{\scriptscriptstyle 0}}C_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 0}}C_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ \rho_{{\scriptscriptstyle 0}}C_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 1}}C_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ \rho_{{\scriptscriptstyle 0}}C_{{\scriptscriptstyle 3}},\pi_{{\scriptscriptstyle 2}}C_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ \left\{ \left[123\right],\left[231\right],\left[312\right]\right\} ,\left\{ \left[213\right],\left[132\right],\left[321\right]\right\} \right\} \\
= & \left\{ \left\{ \left(\right),\left(123\right),\left(132\right)\right\} ,\left\{ \left(12\right),\left(23\right),\left(31\right)\right\} \right\} \\
= & \left\{ \left\{ \rho_{{\scriptscriptstyle 0}},\rho_{{\scriptscriptstyle 1}},\rho_{{\scriptscriptstyle 2}}\right\} ,\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\} \right\} \\
= & \left\{ \left\{ e,\rho,\rho^{2}\right\} ,\left\{ \pi_{{\scriptscriptstyle 0}},\pi_{{\scriptscriptstyle 1}},\pi_{{\scriptscriptstyle 2}}\right\} \right\} 
\end{aligned}
}
\]

\end_inset


\begin_inset Formula 
\[
\left|D_{{\scriptscriptstyle 3}}/C_{{\scriptscriptstyle 3}}\right|=\left|S_{{\scriptscriptstyle 3}}/\mathbb{Z}_{{\scriptscriptstyle 3}}\right|=\left|D_{{\scriptscriptstyle 3}}\right|/\left|C_{{\scriptscriptstyle 3}}\right|=\left|S_{{\scriptscriptstyle 3}}\right|/\left|\mathbb{Z}_{{\scriptscriptstyle 3}}\right|=6/3=2
\]

\end_inset


\begin_inset Formula 
\[
C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\trianglelefteq S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}
\]

\end_inset


\begin_inset Formula 
\[
C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}\vartriangleleft S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}
\]

\end_inset


\begin_inset Formula 
\[
S_{{\scriptscriptstyle 3}}=D_{{\scriptscriptstyle 3}}\vartriangleright C_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}
\]

\end_inset


\begin_inset Formula 
\[
S_{{\scriptscriptstyle 3}}\vartriangleright C_{{\scriptscriptstyle 3}}
\]

\end_inset


\begin_inset Formula 
\[
\recolor{E_{{\scriptscriptstyle 3}}=\left\{ e\right\} =\left\{ \rho_{{\scriptscriptstyle 0}}\right\} =\left\{ \left[123\right]\right\} =\left\{ \left(\right)\right\} \subset\left\{ \left(\right),\left(123\right),\left(132\right)\right\} =C_{{\scriptscriptstyle 3}}}
\]

\end_inset


\begin_inset Formula 
\[
\left|E_{{\scriptscriptstyle 3}}\right|=1
\]

\end_inset


\begin_inset Formula 
\[
\recolor{\begin{aligned}C_{{\scriptscriptstyle 3}}/E_{{\scriptscriptstyle 3}}=\mathbb{Z}_{{\scriptscriptstyle 3}}/E_{{\scriptscriptstyle 3}}= & \left\{ \sigma E_{{\scriptscriptstyle 3}}\middle|\sigma\in\mathbb{Z}_{{\scriptscriptstyle 3}}\right\} \\
= & \left\{ \left[123\right]E_{{\scriptscriptstyle 3}},\left[231\right]E_{{\scriptscriptstyle 3}},\left[312\right]E_{{\scriptscriptstyle 3}}\right\}  & =\left\{ \left\{ \left[123\right]\right\} ,\left\{ \left[231\right]\right\} ,\left\{ \left[312\right]\right\} \right\} \\
= & \left\{ \left(\right)E_{{\scriptscriptstyle 3}},\left(123\right)E_{{\scriptscriptstyle 3}},\left(132\right)E_{{\scriptscriptstyle 3}}\right\}  & =\left\{ \left\{ \left(\right)\right\} ,\left\{ \left(123\right)\right\} ,\left\{ \left(132\right)\right\} \right\} \\
= & \left\{ \rho_{{\scriptscriptstyle 0}}E_{{\scriptscriptstyle 3}},\rho_{{\scriptscriptstyle 1}}E_{{\scriptscriptstyle 3}},\rho_{{\scriptscriptstyle 2}}E_{{\scriptscriptstyle 3}}\right\}  & =\left\{ \left\{ \rho_{{\scriptscriptstyle 0}}\right\} ,\left\{ \rho_{{\scriptscriptstyle 1}}\right\} ,\left\{ \rho_{{\scriptscriptstyle 2}}\right\} \right\} \\
= & \left\{ eE_{{\scriptscriptstyle 3}},\rho E_{{\scriptscriptstyle 3}},\rho^{2}E_{{\scriptscriptstyle 3}}\right\}  & =\left\{ \left\{ e\right\} ,\left\{ \rho\right\} ,\left\{ \rho^{2}\right\} \right\} 
\end{aligned}
}
\]

\end_inset


\begin_inset Formula 
\[
\left|C_{{\scriptscriptstyle 3}}/E_{{\scriptscriptstyle 3}}\right|=\left|\mathbb{Z}_{{\scriptscriptstyle 3}}/E_{{\scriptscriptstyle 3}}\right|=\left|C_{{\scriptscriptstyle 3}}\right|/\left|E_{{\scriptscriptstyle 3}}\right|=\left|\mathbb{Z}_{{\scriptscriptstyle 3}}\right|/\left|E_{{\scriptscriptstyle 3}}\right|=3/1=3
\]

