\[
\recolor{\begin{aligned}f\left(x\right)= & p_{{\scriptscriptstyle n}}x^{n}+p_{{\scriptscriptstyle n-1}}x^{n-1}+\cdots+p_{{\scriptscriptstyle 1}}x+p_{{\scriptscriptstyle 0}} & \Leftrightarrow f\left(x\right)\in\mathbb{K}\left[x\right]\\
= & p_{{\scriptscriptstyle j}}x^{j}=\sum\limits_{j=1}^{n}p_{{\scriptscriptstyle j}}x^{j} & \begin{aligned}p_{{\scriptscriptstyle n}}\ne0\Rightarrow\deg f\left(x\right)=n\in\mathbb{N}\\
j\in\mathbb{Z}_{{\scriptscriptstyle \left[0,n\right]}}\\
p_{{\scriptscriptstyle j}}\in\mathbb{K}=\mathbb{F}
\end{aligned}
\\
= & p_{{\scriptscriptstyle n}}\left(x-x_{{\scriptscriptstyle 1}}\right)\left(x-x_{{\scriptscriptstyle 2}}\right)\cdots\left(x-x_{{\scriptscriptstyle n}}\right) & \left\{ x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},\cdots,x_{{\scriptscriptstyle n}}\right\} \subseteq\mathbb{K}\left(x_{{\scriptscriptstyle 1}},x_{{\scriptscriptstyle 2}},\cdots,x_{{\scriptscriptstyle n}}\right)\\
= & p_{{\scriptscriptstyle n}}\left(x-x_{{\scriptscriptstyle 1}}\right)\cdots\left(x-x_{{\scriptscriptstyle n}}\right) & \left\{ x_{{\scriptscriptstyle 1}},\cdots,x_{{\scriptscriptstyle n}}\right\} \subseteq\mathbb{K}\left(x_{{\scriptscriptstyle 1}},\cdots,x_{{\scriptscriptstyle n}}\right)\\
\Updownarrow\\
f\left(x\right)\text{ is} & \text{ reducible over }\mathbb{K}\left(x_{{\scriptscriptstyle 1}},\cdots,x_{{\scriptscriptstyle n}}\right)
\end{aligned}
}
\]

