\begin_layout Standard
sample size estimation
\begin_inset Formula 
\[
\begin{cases}
H_{{\scriptscriptstyle 0}}:Y\sim\mathrm{n}\left(\mu_{{\scriptscriptstyle 0}},\sigma_{{\scriptscriptstyle 0}}\right) & \Leftrightarrow f_{{\scriptscriptstyle Y}}\left(y\right)=\frac{\mathrm{e}^{\frac{-1}{2}\left(\frac{y-\mu_{{\scriptscriptstyle 0}}}{\sigma_{{\scriptscriptstyle 0}}}\right)^{2}}}{\sigma_{{\scriptscriptstyle 0}}\sqrt{2\pi}}\\
H_{{\scriptscriptstyle 1}}:Y\sim\mathrm{n}\left(\mu_{{\scriptscriptstyle 1}},\sigma_{{\scriptscriptstyle 1}}\right) & \Leftrightarrow f_{{\scriptscriptstyle Y}}\left(y\right)=\frac{\mathrm{e}^{\frac{-1}{2}\left(\frac{y-\mu_{{\scriptscriptstyle 1}}}{\sigma_{{\scriptscriptstyle 1}}}\right)^{2}}}{\sigma_{{\scriptscriptstyle 1}}\sqrt{2\pi}}
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
\mathrm{P}\left(\left|\dfrac{Y-\mu_{{\scriptscriptstyle 0}}}{\sigma_{{\scriptscriptstyle 0}}}\right|\le z_{{\scriptscriptstyle \alpha/2}}\middle|H_{{\scriptscriptstyle 0}}\right)=\int_{-z_{{\scriptscriptstyle \alpha/2}}}^{z_{{\scriptscriptstyle \alpha/2}}}\frac{\mathrm{e}^{\frac{-z^{2}}{2}}}{\sqrt{2\pi}}\mathrm{d}z=\int_{-z_{{\scriptscriptstyle \alpha/2}}}^{z_{{\scriptscriptstyle \alpha/2}}}\phi\left(z\right)\mathrm{d}z=\varPhi\left(z_{{\scriptscriptstyle \alpha/2}}\right)-\varPhi\left(z_{{\scriptscriptstyle -\alpha/2}}\right)=1-\alpha
\]

\end_inset


\end_layout

